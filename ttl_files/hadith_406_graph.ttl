<http://etratnet.ir/graph/ontology/406> rdf:type go:ontology ;
        go:hascontent "6.  «پيامبر خدا صلى‌الله‌عليه‌و‌آله فرمود: هر كس بگويد كه خدا به بدى و زشتكارى فرمان مى‌دهد بر خدا دروغ بسته است، هر كس بگويد كه خوبى و بدى به غير خواست خداست خدا را از پادشاهى‌اش خارج ساخته است، هر كس بگويد كه انجام دادن گناه با غير از نيروى خداست بر خدا دروغ بسته است و هر كس بر خدا دروغ بندد، خداوند او را به آتش [دوزخ]درآورد»."@fa,
        "6.  عَنْ أَبِي عَبْدِ اللّه‌ِ عليه‌السلام، قَالَ: «قَالَ رَسُولُ اللّه‌ِ صلى‌الله‌عليه‌و‌آله: مَنْ زَعَمَ أَنَّ اللّه‌َ يَأْمُرُ بِالسُّوءِ وَالْفَحْشَاءِ، فَقَدْ كَذَبَ عَلَى اللّه‌ِ؛ وَمَنْ زَعَمَ أَنَّ الْخَيْرَ وَالشَّرَّ بِغَيْرِ مَشِيئَةِ اللّه‌ِ، فَقَدْ أَخْرَجَ اللّه‌َ مِنْ سُلْطَانِهِ؛ وَمَنْ زَعَمَ أَنَّ الْمَعَاصِيَ بِغَيْرِ قُوَّةِ اللّه‌ِ، فَقَدْ كَذَبَ عَلَى اللّه‌ِ؛ وَمَنْ كَذَبَ عَلَى اللّه‌ِ، أَدْخَلَهُ اللّه‌ُ النَّارَ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/406-1> rdf:type owl:ontology .

ex:messengerofallah a go:person ;
    rdfs:label "messenger of allah (peace be upon him and his family)"@en .

ex:allah a go:deity ;
    rdfs:label "allah"@en .

ex:speechevent1 a go:speechevent ;
    go:hasagent ex:messengerofallah ;
    go:hastheme ex:proposition1 ;
    rdfs:label "saying by messenger of allah"@en .

ex:proposition1 a go:proposition ;
    rdfs:label "whoever claims that allah commands evil and immorality has lied about allah"@en ;
    go:hassubject ex:claimant ;
    go:hasobject ex:lieaboutallah .

ex:claimant a go:indefiniteperson ;
    rdfs:label "whoever claims"@en .

ex:lieaboutallah a go:lie ;
    go:about ex:allah ;
    rdfs:label "lying about allah"@en .

ex:commandsevilandimmorality a go:actionpredicate ;
    rdfs:label "allah commands evil and immorality"@en ;
    go:agent ex:allah ;
    go:theme ex:evilandimmorality .

ex:evilandimmorality a go:concept ;
    rdfs:label "evil and immorality"@en .

ex:proposition1 go:hasclaim ex:commandsevilandimmorality .
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/406-2> rdf:type owl:ontology .

# entities
ex:messengerofallah rdf:type ex:prophet ;
    ex:honorific "peace be upon him and his family" .

ex:allah rdf:type ex:deity .

ex:good rdf:type go:substance ;
    rdfs:label "good" .

ex:evil rdf:type go:substance ;
    rdfs:label "evil" .

# events
ex:speech1 rdf:type ex:speechevent ;
    go:hasagent ex:messengerofallah ;
    go:hastheme ex:claim1 .

ex:claim1 rdf:type ex:claim ;
    ex:content "good and evil do not happen without the will of allah." ;
    go:hasagent ex:claimant ;
    go:hastheme ex:goodandevilwithoutwill .

ex:goodandevilwithoutwill rdf:type go:event ;
    go:hastheme ex:good, ex:evil ;
    ex:causedby ex:willofallah .

ex:willofallah rdf:type go:substance ;
    rdfs:label "will of allah" ;
    ex:belongsto ex:allah .

ex:removal rdf:type ex:removalfromauthorityevent ;
    ex:cause ex:claim1 ;
    ex:effectedby ex:allah ;
    ex:patient ex:claimant .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/406-3> rdf:type owl:ontology .

ex:messengerofallah a go:agent, go:person ;
    rdfs:label "messenger of allah" .

ex:allah a go:agent, go:deity ;
    rdfs:label "allah" .

ex:sins a go:event ;
    rdfs:label "sins" .

ex:claimaboutsinsandpower a go:event ;
    a go:claim ;
    go:hasagent ex:unknownclaimant ;
    go:hastheme ex:sinswithoutallahpower ;
    rdfs:label "claim that sins occur without the power of allah" .

ex:unknownclaimant a go:agent, go:person ;
    rdfs:label "whoever" .

ex:lyingaboutallah a go:event ;
    a go:lying ;
    go:hasagent ex:unknownclaimant ;
    go:hastheme ex:allah ;
    rdfs:label "lying about allah" .

ex:sinswithoutallahpower a go:proposition ;
    rdfs:label "sins without the power of allah" .

ex:sayingevent a go:speechact ;
    go:hasagent ex:messengerofallah ;
    go:hastheme [
        a go:proposition ;
        go:involves ex:claimaboutsinsandpower, ex:lyingaboutallah
    ] ;
    rdfs:label "messenger of allah said about claim and lying" .
go:event rdfs:label "رخداد"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/406-4> rdf:type owl:ontology .

ex:prophetmuhammad a go:person ;
    rdfs:label "prophet muhammad (peace be upon him and his family)" .

ex:god a go:deity ;
    rdfs:label "god" .

ex:hell a go:place ;
    rdfs:label "hell" .

ex:liar a go:person ;
    go:hasrole "whoever lies about god" .

ex:prophetspeech a go:event , ex:speechevent ;
    go:hasagent ex:prophetmuhammad ;
    go:hastheme ex:statement .

ex:statement a go:substance ;
    go:hascontent "whoever lies about god, god will admit him to hell" .

ex:lyingevent a go:event , ex:lying ;
    go:hasagent ex:liar ;
    go:hastheme ex:god .

ex:admittohellevent a go:event , ex:admission ;
    go:hasagent ex:god ;
    go:hastheme ex:liar ;
    go:haslocation ex:hell .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.