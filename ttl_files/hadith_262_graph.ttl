<http://etratnet.ir/ontology/262-22> rdf:type owl:ontology .

ex:abu_al_hasan a foaf:person ;
    foaf:name "abu al-hasan" ;
    ex:honorific "peace be upon him" .

ex:utterance_1 a go:event, ex:speakingevent ;
    go:hasagent ex:abu_al_hasan ;
    go:hastheme "will you not show shame?" .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/262-23> rdf:type owl:ontology .

ex:heretics a go:group ;
    rdfs:label "heretics"@en .

ex:prophetmuhammad a go:person ;
    rdfs:label "prophet muhammad"@en .

ex:god a go:deity ;
    rdfs:label "god"@en .

ex:accusationevent a go:event ;
    rdfs:label "attempt to accuse"@en ;
    go:hasagent ex:heretics ;
    go:hastheme ex:prophetmuhammad ;
    ex:hascontent ex:chargecontent ;
    ex:issuccessful "false"^^xsd:boolean .

ex:chargecontent a go:information ;
    rdfs:label "charge that a message comes from god then another comes from a different source"@en ;
    ex:contradictionbetween [ ex:source1 ex:god ; ex:source2 ex:anothersource ] .

ex:anothersource a go:substance ;
    rdfs:label "another source"@en .
go:event rdfs:label "رخداد"@fa.
go:information rdfs:label "اطلاعات"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/262-24> rdf:type owl:ontology .

ex:abuqura a go:person .
ex:prophetmuhammad a go:person .

ex:speechevent1 a go:speechevent ;
    go:hasagent ex:abuqura ;
    go:hascontent ex:experienceevent1 .

ex:experienceevent1 a go:experienceevent ;
    go:hasagent ex:prophetmuhammad ;
    go:hastheme ex:anotherbout .

ex:anotherbout a go:event .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/262-25> rdf:type owl:ontology .

# entities
ex:statement1 a go:utterance ;
  go:hastheme ex:quote1 .

ex:quote1 a go:proposition ;
  go:hascontent "the heart did not lie about what it saw" .

ex:fuad_muhammad a go:person ;
  rdfs:label "fuad muhammad" .

ex:muhammad a go:person ;
  rdfs:label "muhammad" .

ex:muhammad_eyes a go:bodypart ;
  go:partof ex:muhammad ;
  rdfs:label "muhammad's eyes" .

# event of statement
ex:utterance1 a go:event ;
  go:hasagent ex:narrator ;
  go:hastheme ex:quote1 .

ex:narrator a go:person ;
  go:hasrole "speaker" .

# event of truth assertion (negated lie) related to fuad muhammad
ex:truthassertion1 a go:event ;
  go:hasagent ex:fuad_muhammad ;
  go:hastheme ex:perception1 ;
  go:negation true .

ex:perception1 a go:perception ;
  go:hasagent ex:muhammad_eyes ;
  go:hascontent "what muhammad's eyes saw" .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/262-26> rdf:type owl:ontology .

ex:fuad_muhammad a go:person ;
    rdfs:label "fuad muhammad" .

ex:reportingevent1 a go:event ;
    go:hasagent ex:fuad_muhammad ;
    go:hastheme ex:seeingevent1 .

ex:seeingevent1 a go:event ;
    go:hasagent ex:fuad_muhammad ;
    ex:perceivedby "eyesight"^^xsd:string .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/262-27> rdf:type owl:ontology .

ex:fuad_muhammad a foaf:person ;
    foaf:name "fuad muhammad" .

ex:lord a go:deity ;
    foaf:name "lord" .

ex:great_signs a go:phenomenon ;
    go:hasquality ex:great ;
    go:belongsto ex:lord .

ex:some_great_signs a go:phenomenon ;
    go:partof ex:great_signs ;
    go:quantity ex:some .

ex:see_event_1 a go:seeevent ;
    go:hasagent ex:fuad_muhammad ;
    go:hastheme ex:some_great_signs .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/262-28> rdf:type owl:ontology .

ex:verses_of_god a go:substance ;
    rdfs:label "verses of god"@en .

ex:god_himself a go:substance ;
    rdfs:label "god himself"@en .

ex:verses_of_god ex:isdistinctfrom ex:god_himself .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/262-29> rdf:type owl:ontology .

ex:eventinstance1 a go:speechevent ;
    go:hasagent ex:god ;
    go:hastheme ex:speechcontent1 .

ex:god a go:deity .

ex:speechcontent1 a go:content ;
    go:hastext "and they do not encompass him in knowledge." .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/262-30> rdf:type owl:ontology .

ex:seeingevent a go:observationevent ;
  go:hasagent ex:eyes ;
  go:hastheme ex:signsofgod .

ex:eyes a go:substance .
ex:signsofgod a go:object .
ex:knowledge a go:substance .

ex:knowledge go:surrounds ex:eyes .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/262-31> rdf:type owl:ontology .

ex:knowledgeoccurrence a go:event ;
    go:hastheme ex:knowledge ;
    go:islinkedto ex:signssight .

ex:knowledge a go:entity .

ex:signssight a go:event ;
    go:hasagent ex:eyes ;
    go:hastheme ex:signs .

ex:eyes a go:entity .

ex:signs a go:entity .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/262-32> rdf:type owl:ontology .

ex:narrations a go:expression .
ex:quran a go:substance .

ex:contradiction a go:event ;
    go:hastheme ex:narrations, ex:quran .

ex:denial a go:event ;
    go:hastheme ex:narrations .

ex:conditionalrelation a go:causalrelation ;
    go:hascause ex:contradiction ;
    go:haseffect ex:denial .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/262-33> rdf:type owl:ontology .

ex:muslims a go:agent ;
    rdfs:label "muslims" .

ex:god a go:substance ;
    rdfs:label "god" .

ex:godknowledge a go:property ;
    go:ispropertyof ex:god ;
    go:hascharacteristic ex:unboundedknowledge .

ex:unboundedknowledge a go:characteristic ;
    go:description "knowledge that cannot be fully encompassed" .

ex:agreementevent a go:event ;
    go:hasagent ex:muslims ;
    go:hastheme ex:proposition .

ex:proposition a go:statement ;
    go:content "god's knowledge is unbounded" ;
    go:refersto ex:godknowledge .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/262-34> rdf:type owl:ontology .

ex:god a go:deity ;
    go:label "god" .

ex:eyes a go:organism ;
    go:label "eyes" .

ex:perceptionbyeyes a go:event ;
    go:hasagent ex:eyes ;
    go:hastheme ex:god .

ex:god ex:isnonperceivableby ex:eyes .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/262-35> rdf:type owl:ontology .

ex:god a go:substance .

# statement that god is not like any thing (no similarity)
ex:god go:isnotlike go:anything .
go:substance rdfs:label "شی"@fa.