@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/432-8> rdf:type owl:ontology .

ex:godalmighty a go:agent ;
    rdfs:label "god almighty" .

ex:people a go:agent ;
    rdfs:label "people" .

ex:sayingevent a go:communicationevent ;
    go:hasagent ex:godalmighty ;
    go:hastheme ex:utterance_question .

ex:utterance_question a go:utterance ;
    rdfs:label "do you hate people until they become believers?" ;
    go:hasagent ex:godalmighty ;
    go:hastheme ex:people ;
    go:hascondition ex:becomebelievers .

ex:becomebelievers a go:statechange ;
    go:hasagent ex:people ;
    rdfs:label "become believers" .
go:agent rdfs:label "عامل"@fa.


