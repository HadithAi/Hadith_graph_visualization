    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/268> rdf:type go:ontology ;
        go:hascontent "8.  «پيامبر خدا صلى‌الله‌عليه‌و‌آله فرمود: شبى كه به آسمان برده شدم، جبرئيل مرا به جايى رساند كه او خود هرگز قدم به آن جا نگذاشته بود. پس، پرده برايش كنار زده شد و خداوند از نور عظمت خود تا دوست داشت به او نماياند»."@fa,
        "8.  عَنْ أَبِي الْحَسَنِ الرِّضَا عليه‌السلام، قَالَ: «قَالَ رَسُولُ اللّه‌ِ صلى‌الله‌عليه‌و‌آله: لَمَّا أُسْرِيَ بِي إِلَى السَّمَاءِ، بَلَغَ بِي جَبْرَئِيلُ مَكَاناً لَمْ يَطَأْهُ قَطُّ جَبْرَئِيلُ، فَكُشِفَ لَهُ، فَأَرَاهُ اللّه‌ُ مِنْ نُورِ عَظَمَتِهِ مَا أَحَبَّ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/268-1> rdf:type owl:ontology .

ex:prophet_muhammad a go:person ;
    rdfs:label "prophet muhammad (peace be upon him and his family)" .

ex:gabriel a go:person ;
    rdfs:label "gabriel" .

ex:ascensionevent a go:event ;
    go:hasagent ex:prophet_muhammad ;
    go:haslocation ex:heavens .

ex:gabrielreachesplace a go:event ;
    go:hasagent ex:gabriel ;
    go:hastheme ex:uniqueplace .

ex:uniqueplace a go:place ;
    go:description "a place in the heavens that gabriel had never set foot on before" .

ex:utterance1 a go:event ;
    go:hasagent ex:prophet_muhammad ;
    go:expresses ex:ascensionevent, ex:gabrielreachesplace .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/268-2> rdf:type owl:ontology .

ex:god a go:agent .
ex:gabriel a go:agent .
ex:thatplace a go:place .

ex:revelationevent1 a ex:revelationevent ;
    go:hasagent ex:god ;
    go:hastheme ex:thatplace ;
    go:hasrecipient ex:gabriel .
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/268-3> rdf:type owl:ontology .

ex:god a go:substance ;
    go:denotes <http://dbpedia.org/resource/god> .

ex:gabriel a go:substance ;
    go:denotes <http://dbpedia.org/resource/gabriel> .

ex:lightofgreatness a go:substance ;
    go:denotes "light symbolizing god's greatness" .

ex:showinglightevent a go:event, ex:eventinstance ;
    go:hasagent ex:god ;
    go:hasrecipient ex:gabriel ;
    go:hastheme ex:lightofgreatness ;
    go:hasmanner ex:godslove .

ex:godslove a go:quality ;
    go:inheresin ex:god .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:quality rdfs:label "ویژگی"@fa.