@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/338-3> rdf:type owl:Ontology .

ex:Human a go:Human .
ex:Speaker a go:Person .
ex:Verse a go:Text .
ex:Kufa a go:Place ;
    go:name "Kufa" .

ex:ReturningEvent a go:Event ;
    go:HasAgent ex:Human ;
    go:HasTheme ex:Speaker ;
    go:EventType go:Returning .

ex:AskingEvent a go:Event ;
    go:HasAgent ex:Human ;
    go:HasRecipient ex:Speaker ;
    go:HasTheme ex:NameInKufa ;
    go:EventType go:Asking .

ex:NameInKufa a go:Substance ;
    go:Description "Name of the speaker in Kufa" ;
    go:LocatedIn ex:Kufa .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
