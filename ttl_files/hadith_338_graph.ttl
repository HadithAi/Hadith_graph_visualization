<http://etratnet.ir/ontology/338-5> rdf:type owl:Ontology .

ex:eventAsk a go:Event ;
  go:HasAgent ex:Person_You ;
  go:HasTheme ex:Person_Speaker ;
  go:HasLocation ex:Place_Basra .

ex:eventSay a go:Event ;
  go:HasAgent ex:Person_Speaker ;
  go:HasTheme ex:Name_SoAndSo ;
  go:isSubEventOf ex:eventInteraction .

ex:Person_You a go:Person ;
  go:label "You" .

ex:Person_Speaker a go:Person ;
  go:label "Speaker" .

ex:Place_Basra a go:Place ;
  go:label "Basra" .

ex:Name_SoAndSo a go:Name ;
  go:label "so-and-so" .

ex:eventInteraction a go:Event .
