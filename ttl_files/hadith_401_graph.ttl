<http://etratnet.ir/ontology/401-37> rdf:type owl:Ontology .

ex:You a ex:Imam ;
    a go:agent .

ex:DayOfSalvation a go:Event ;
    rdfs:label "Day of Salvation" .

ex:TheMostMerciful a go:Substance ;
    go:hasTitle "Ar-Rahman" ;
    a go:divineBeing .

ex:HopeToObey a go:Event ;
    go:HasAgent ex:SpeakerGroup ;
    go:HasTheme ex:You ;
    go:HasTime ex:DayOfSalvation ;
    go:HasCause ex:TheMostMerciful ;
    go:desires "forgiveness" .

ex:SpeakerGroup a go:group ;
    go:hasRole "speaker and others" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
