@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/401-38> rdf:type owl:Ontology .

ex:clarificationEvent1 a go:EventInstance ;
    go:HasAgent ex:Imam ;
    go:HasRecipient ex:Us ;
    go:HasTheme ex:AmbiguousInformation .

ex:Imam a go:Substance ;
    rdfs:label "the Imam" .

ex:Us a go:Substance ;
    rdfs:label "us" .

ex:AmbiguousInformation a go:Substance ;
    rdfs:label "What was ambiguous" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
