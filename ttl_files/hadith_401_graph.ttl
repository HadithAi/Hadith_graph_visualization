<http://etratnet.ir/ontology/401-39> rdf:type owl:Ontology .

ex:RewardEvent1 a go:Event ;
  go:HasAgent ex:Lord1 ;
  go:HasTheme ex:Imam1 ;
  go:HasManner ex:Goodness1 .

ex:Lord1 a go:agent ;
  rdfs:label "Lord" .

ex:Imam1 a go:person ;
  rdfs:label "Imam" .

ex:Goodness1 a go:Quality ;
  rdfs:label "Goodness" .
go:Event rdfs:label "رخداد"@fa.
go:Quality rdfs:label "ویژگی"@fa.