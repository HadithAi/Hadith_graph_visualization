@prefix ex: <http://etratnet.ir/graph/proposition/367#> .
    @prefix bt: <http://borhan-onto.ir/ontology/time#> .
    @prefix go: <http://etratnet.ir/ontology#> .
    @prefix owl: <http://www.w3.org/2002/07/owl#> .
    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix xml: <http://www.w3.org/XML/1998/namespace> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
    @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


    @base <http://etratnet.ir/ontology/> .
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/367-1> rdf:type owl:Ontology .

ex:Allah a go:Deity ;
    go:hasAttribute ex:Greatness, ex:Honor, ex:Venerability, ex:Inviolability ;
    go:hasProperty [ a go:Immunity ;
                    go:negationOf ex:BeingWronged ] .

ex:Greatness a go:Attribute ;
    rdfs:label "Great"@en .

ex:Honor a go:Attribute ;
    rdfs:label "Honorable"@en .

ex:Venerability a go:Attribute ;
    rdfs:label "Venerable"@en .

ex:Inviolability a go:Attribute ;
    rdfs:label "Inviolable"@en .

ex:BeingWronged a go:Event .
go:Event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/367-2> rdf:type owl:Ontology .

ex:MixingEvent a go:Event ;
    go:HasAgent go:God ;
    go:HasTheme ex:PeopleOfTheHouse .

ex:InjusticeAgainstPeopleOfTheHouse a ex:Injustice ;
    ex:target ex:PeopleOfTheHouse .

ex:GodsInjustice a ex:Injustice ;
    ex:equivalentTo ex:InjusticeAgainstPeopleOfTheHouse ;
    ex:agent go:God .
go:Event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/367-3> rdf:type owl:Ontology .

ex:Authority_AhlAlBayt a go:Authority ;
    rdfs:label "ولاية أهل البيت"@ar ;
    go:isEquivalentTo ex:Authority_Allah .

ex:Authority_Allah a go:Authority ;
    rdfs:label "ولاية الله"@ar ;
    go:HasAgent ex:Allah .

ex:Allah a go:Deity ;
    rdfs:label "الله سبحانه"@ar .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/367-4> rdf:type owl:Ontology .

ex:event1 a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:utterance1 .

ex:God a go:Substance ;
    rdfs:label "God" .

ex:Messenger a go:Substance ;
    rdfs:label "His Messenger" .

ex:Believers a go:Substance ;
    rdfs:label "Those Who Have Believed" .

ex:utterance1 a go:Utterance ;
    go:utteredText "Indeed, your guardian is God, His Messenger, and those who have believed." ;
    go:states (
        [ a go:GuardianRole ; go:hasGuardian ex:God ]
        [ a go:GuardianRole ; go:hasGuardian ex:Messenger ]
        [ a go:GuardianRole ; go:hasGuardian ex:Believers ]
    ) .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/367-5> rdf:type owl:Ontology .

ex:thisVerse a go:Text ;
    go:hasContent "This verse" .

ex:beliefEvent a go:Event ;
    go:HasTheme ex:thisVerse ;
    go:HasAgent ex:believersGroup .

ex:believersGroup a go:Group ;
    go:identity ex:imams .

ex:imams a go:Group ;
    go:isMemberOf ex:AhlAlBayt .

ex:AhlAlBayt a go:Community ;
    rdfs:label "Ahl al-Bayt" .
go:Event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/367-6> rdf:type owl:Ontology .

ex:InjusticeEvent1 a go:Injustice ;
    go:HasTheme ex:AhlAlBayt ;
    go:HasTheme ex:Themselves .

ex:AhlAlBayt a go:Group ;
    go:hasMember ex:PeopleOfTheHouse .

ex:Themselves a go:Self .

ex:GodAlmighty a go:Deity .

# Explicitly stating no injustice towards God
ex:InjusticeEvent1 go:HasNoEffectOn ex:GodAlmighty .