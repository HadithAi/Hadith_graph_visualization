<http://etratnet.ir/graph/ontology/413> rdf:type go:ontology ;
        go:hascontent "13. «نه جبر است و نه تفويض، بلكه حالتى است ميان اين دو». عرض كردم: يعنى چه حالتى است ميان اين دو؟ فرمود: «مثلاً مردى را در حال ارتكاب گناهى ببينى و او را از آن نهى كنى. اما او نپذيرد و تو به حال خود رهايش كنى و او آن گناه را انجام دهد. [در اين حال] چنين نيست كه چون او از تو نپذيرفته است و تو به حال خود رهايش ساخته‌اى، تو او را به گناه فرمان داده‌اى»."@fa,
        "13. عَنْ أَبِي عَبْدِ اللّه‌ِ عليه‌السلام، قَالَ: «لاَ جَبْرَ وَلاَ تَفْوِيضَ، وَلكِنْ أَمْرٌ بَيْنَ أَمْرَيْنِ». قَالَ: قُلْتُ: وَمَا أَمْرٌ بَيْنَ أَمْرَيْنِ؟ قَالَ: «مَثَلُ ذلِكَ: رَجُلٌ رَأَيْتَهُ عَلى مَعْصِيَةٍ، فَنَهَيْتَهُ، فَلَمْ يَنْتَهِ، فَتَرَكْتَهُ، فَفَعَلَ تِلْكَ الْمَعْصِيَةَ؛ فَلَيْسَ حَيْثُ لَمْ يَقْبَلْ مِنْكَ فَتَرَكْتَهُ كُنْتَ أَنْتَ الَّذِي أَمَرْتَهُ بِالْمَعْصِيَةِ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/413-1> rdf:type owl:ontology .

ex:divinecommand a go:event ;
    rdfs:label "divine command" .

ex:compulsion a go:concept ;
    rdfs:label "compulsion" .

ex:delegation a go:concept ;
    rdfs:label "delegation" .

ex:divinecommand ex:hasnoproperty ex:compulsion , ex:delegation .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/413-2> rdf:type owl:ontology .

ex:divine_command a go:command ;
    ex:isbetween ex:command1 , ex:command2 .

ex:command1 a go:command .
ex:command2 a go:command .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/413-3> rdf:type owl:ontology .

ex:speaker a go:agent .
ex:man a go:agent .
ex:sin a go:event .

ex:seeingevent a go:event ;
    go:hasagent ex:speaker ;
    go:hastheme ex:man .

ex:prohibitingevent a go:event ;
    go:hasagent ex:speaker ;
    go:hastheme ex:sin ;
    go:hasrecipient ex:man .
go:event rdfs:label "رخداد"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/413-4> rdf:type owl:ontology .

ex:man a go:go_0003674 ;  # a human entity
    rdfs:label "the man" .

ex:sin a go:go_0008150 ;  # representing a wrongdoing or sin
    rdfs:label "the sin" .

ex:seeingevent a go:event ;
    go:hasagent ex:observer ;   # the 'i' in 'i saw'
    go:hastheme ex:man ;
    rdfs:label "seeing the man" .

ex:committingsinevent a go:event ;
    go:hasagent ex:man ;
    go:hastheme ex:sin ;
    rdfs:label "committing the sin" .

ex:forbiddingevent a go:event ;
    go:hasagent ex:authority ;
    go:hastheme ex:man ;
    rdfs:label "forbidding the man" .

ex:notrefrainingevent a go:event ;
    go:hasagent ex:man ;
    go:hastheme ex:sin ;
    rdfs:label "not refraining from the sin" ;
    go:isafter ex:forbiddingevent .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/413-5> rdf:type owl:ontology .

# entities
ex:man1 rdf:type go:person .
ex:speaker rdf:type go:person .

# events
ex:disobeyevent1 rdf:type go:event ;
    go:hasagent ex:man1 ;
    go:hastheme ex:prohibitionevent1 ;
    go:occursafter ex:prohibitionevent1 .

ex:prohibitionevent1 rdf:type go:event ;
    go:haspatient ex:man1 .

ex:leaveevent1 rdf:type go:event ;
    go:hasagent ex:speaker ;
    go:hastheme ex:man1 .

# temporal relation
ex:disobeyevent1 go:occursafter ex:prohibitionevent1 .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/413-6> rdf:type owl:ontology .

ex:man a go:substance ;
    rdfs:label "the man" .

ex:i a go:agent ;
    rdfs:label "i" .

ex:sin a go:substance ;
    rdfs:label "the sin" .

ex:commitsin a go:event ;
    go:hasagent ex:man ;
    go:hastheme ex:sin ;
    rdfs:label "commit sin" .

ex:leaveaction a go:event ;
    go:hasagent ex:i ;
    go:hastheme ex:man ;
    go:hascause ex:commitsin ;
    rdfs:label "leave the man committing sin" .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/413-7> rdf:type owl:ontology .

ex:man a go:person .
ex:you a go:person .
ex:prohibitionevent a go:event ;
    go:hasagent ex:you ;
    go:hastheme ex:man ;
    go:hastype "prohibition" .

ex:notacceptevent a go:event ;
    go:hasagent ex:man ;
    go:hastheme ex:prohibitionevent ;
    go:hastype "rejection" .

ex:leavingevent a go:event ;
    go:hasagent ex:you ;
    go:hastheme ex:man ;
    go:hastype "leaving" .

ex:commanddisobedienceevent a go:event ;
    go:hasagent ex:you ;
    go:hastheme ex:disobedienceaction ;
    go:hastype "command" .

ex:disobedienceaction a go:event ;
    go:hastype "disobedience" .

ex:leavingevent go:doesnotimply ex:commanddisobedienceevent .
go:event rdfs:label "رخداد"@fa.