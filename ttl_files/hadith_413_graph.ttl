@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/413-7> rdf:type owl:Ontology .

ex:Man a go:Person .
ex:You a go:Person .
ex:ProhibitionEvent a go:Event ;
    go:HasAgent ex:You ;
    go:HasTheme ex:Man ;
    go:hasType "prohibition" .

ex:NotAcceptEvent a go:Event ;
    go:HasAgent ex:Man ;
    go:HasTheme ex:ProhibitionEvent ;
    go:hasType "rejection" .

ex:LeavingEvent a go:Event ;
    go:HasAgent ex:You ;
    go:HasTheme ex:Man ;
    go:hasType "leaving" .

ex:CommandDisobedienceEvent a go:Event ;
    go:HasAgent ex:You ;
    go:HasTheme ex:DisobedienceAction ;
    go:hasType "command" .

ex:DisobedienceAction a go:Event ;
    go:hasType "disobedience" .

ex:LeavingEvent go:doesNotImply ex:CommandDisobedienceEvent .
go:Event rdfs:label "رخداد"@fa.