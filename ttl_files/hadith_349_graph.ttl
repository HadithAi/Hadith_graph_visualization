@prefix ex: <http://etratnet.ir/graph/proposition/349#> .
    @prefix bt: <http://borhan-onto.ir/ontology/time#> .
    @prefix go: <http://etratnet.ir/ontology#> .
    @prefix owl: <http://www.w3.org/2002/07/owl#> .
    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix xml: <http://www.w3.org/XML/1998/namespace> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
    @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


    @base <http://etratnet.ir/ontology/> .
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/349-1> rdf:type owl:Ontology .

ex:God a go:Deity .
ex:Adam a go:Human .

_:creationEvent a go:CreationEvent ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Adam ;
    go:HasManner [ a go:Image ; go:HasAttribute ex:NewCreated ] .

ex:NewCreated a go:Attribute ; go:label "new created" .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/349-2> rdf:type owl:Ontology .

ex:SelectionEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:ThisPicture ;
    ex:comparedTo ex:AllDifferentPictures .

ex:ChoiceEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:ThisPicture ;
    ex:comparedTo ex:AllDifferentPictures .

ex:God a go:Agent .
ex:ThisPicture a go:VisualRepresentation .
ex:AllDifferentPictures a go:VisualRepresentation .
go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/349-3> rdf:type owl:Ontology .

ex:addEvent a go:Event ;
  go:HasAgent ex:God ;
  go:HasTheme ex:PictureOfAdam ;
  go:HasRecipient ex:GodSelf .

ex:God a go:Substance .
ex:PictureOfAdam a go:Substance .
ex:GodSelf a go:Substance .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/349-4> rdf:type owl:Ontology .

ex:event1 a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Kaaba ;
    go:HasRecipient ex:God .

ex:God a go:Substance .
ex:Kaaba a go:Substance .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/349-5> rdf:type owl:Ontology .

ex:eventInstance1 a go:AdditionEvent ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Spirit ;
    go:HasRecipient ex:God .

ex:God a go:Substance ;
    rdfs:label "God" .

ex:Spirit a go:Substance ;
    rdfs:label "Spirit" .
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/349-6> rdf:type owl:Ontology .

ex:eventInstance1 a go:SpeechEvent ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Utterance1 .

ex:God a go:Deity .

ex:Utterance1 a go:Text ;
    go:hasLiteralValue "My house" .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/349-7> rdf:type owl:Ontology .

ex:eventInstance1 a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Adam ;
    go:HasInstrument ex:GodSpirit .

ex:God a go:Deity .
ex:Adam a go:Person .
ex:GodSpirit a go:Substance .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.