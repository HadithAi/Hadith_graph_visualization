@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/362-22> rdf:type owl:Ontology .

ex:ChangeInGod a go:Event ;
    go:HasTheme ex:God .

ex:God a go:Substance ;
    go:role "Creator" .

ex:Creator a go:Substance .

ex:Created a go:Substance .

# The condition states if Change occurs to God, then Creator is not known from Created
# Represented here logically as a comment or can model state:

ex:IndistinguishabilityState a go:State ;
    go:Involves ex:Creator, ex:Created ;
    go:Condition ex:ChangeInGod ;
    go:Property go:NotDistinguishable .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/362-23> rdf:type owl:Ontology .

ex:God a go:Deity ;
    go:HasQuality ex:Exalted .

ex:PerishingEvent a go:Event ;
    go:HasTheme ex:God ;
    go:HasTime ex:CertainDay ;
    go:HasClaim ex:ClaimThatGodPerishes .

ex:CertainDay a go:TimeEntity .

ex:ClaimThatGodPerishes a go:Claim ;
    go:About ex:PerishingEvent .

# Statement: God is exalted above the claim that He perishes, meaning God transcends/dismisses that claim.
ex:God go:Transcends ex:ClaimThatGodPerishes .

ex:Exalted a go:Quality ;
    rdfs:label "Exalted" .
go:Event rdfs:label "رخداد"@fa.
go:Time rdfs:label "زمان"@fa.
go:Quality rdfs:label "ویژگی"@fa.


