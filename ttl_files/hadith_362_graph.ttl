@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/362-6> rdf:type owl:Ontology .

# Entities
ex:God a go:Substance ;
    rdfs:label "God"@en .

ex:FriendsOfGod a go:Substance ;
    rdfs:label "Friends of God"@en .

ex:Anger a go:Emotion ;
    rdfs:label "Anger"@en .

# Anger instances
ex:anger_God a ex:AngerState ;
    a go:Emotion ;
    go:HasAgent ex:God ;
    rdfs:label "Anger of God"@en .

ex:anger_FriendsOfGod a ex:AngerState ;
    a go:Emotion ;
    go:HasAgent ex:FriendsOfGod ;
    rdfs:label "Anger of Friends of God"@en .

# Identity statement
ex:anger_FriendsOfGod owl:sameAs ex:anger_God .
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/362-7> rdf:type owl:Ontology .

ex:God a go:Substance ;
   go:HasName "Allah" .

ex:FriendsOfGod a go:Substance ;
   go:hasRelationTo ex:God .

ex:Event1 a go:Event ;
   go:HasAgent ex:God ;
   go:HasTheme ex:FriendsOfGod ;
   go:HasEffect [
       a go:state ;
       go:means "to be advocates (callers) to God and guides on God"
   ] .

ex:FriendsOfGod go:HasRole ex:AdvocatesToGod, ex:GuidesOnGod .

ex:AdvocatesToGod a go:role ;
   go:target ex:God .

ex:GuidesOnGod a go:role ;
   go:target ex:God .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/362-8> rdf:type owl:Ontology .

