@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/362-1> rdf:type owl:Ontology .

ex:eventInstance1 a go:SayingEvent ;
    go:HasAgent ex:God ;
    go:HasTheme ex:CommandToWorship .

ex:God a go:Deity ;
    rdfs:label "God" .

ex:CommandToWorship a go:Command ;
    go:hasContent "Worship Me and do not associate anything with Me" .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/362-2> rdf:type owl:Ontology .

ex:SpeechEvent1 a go:Event , ex:SpeechEvent ;
    go:HasAgent ex:Allah ;
    go:HasTheme ex:DivineCommand1 .

ex:Allah a go:Deity .

ex:DivineCommand1 a ex:DivineCommand ;
    ex:HasContent ex:Command1 .

ex:Command1 a ex:Command ;
    ex:IncludesImperative ex:BeServantsOfAllah , ex:BeBrothers .

ex:BeServantsOfAllah a ex:ImperativeStatement ;
    ex:CommandTarget ex:People ;
    ex:CommandContent "Be servants of Allah" .

ex:BeBrothers a ex:ImperativeStatement ;
    ex:CommandTarget ex:People ;
    ex:CommandContent "Be brothers" .

ex:People a go:Agent .
go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.


