@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/362-10> rdf:type owl:Ontology .

ex:God a go:Agent ;
    rdfs:label "God" .

ex:GuardianOfGod a go:Substance ;
    rdfs:label "Guardian of God" .

ex:Whoever a go:Agent ;
    rdfs:label "Whoever" .

ex:SayingEvent a go:Event ;
    rdfs:label "God's Saying" ;
    go:HasAgent ex:God ;
    go:HasTheme ex:SayingContent .

ex:SayingContent a go:Information ;
    rdfs:label "Whoever insults GuardianOfGod has challenged God to war and invited Him." .

ex:InsultingEvent a go:Event ;
    rdfs:label "Insulting Guardian" ;
    go:HasAgent ex:Whoever ;
    go:HasTheme ex:GuardianOfGod .

ex:ChallengeEvent a go:Event ;
    rdfs:label "Challenge to War" ;
    go:HasAgent ex:Whoever ;
    go:HasTheme ex:God .

ex:InvitationEvent a go:Event ;
    rdfs:label "Invitation to War" ;
    go:HasAgent ex:Whoever ;
    go:HasRecipient ex:God .

# Semantic connection between the insulting and subsequent challenge and invitation
ex:InsultingEvent go:causes ex:ChallengeEvent, ex:InvitationEvent .
go:Event rdfs:label "رخداد"@fa.
go:Information rdfs:label "اطلاعات"@fa.
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/362-11> rdf:type owl:Ontology .

ex:God a go:Deity .
ex:Messenger a go:Prophet .

ex:saying1 a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:statement1 .

ex:statement1 a go:Statement ;
    go:expressesClause ex:obedienceConditional .

# The conditional: If person obeys Messenger, then person obeys God

ex:obedienceConditional a go:ConditionalStatement ;
    go:ifClause ex:obedienceToMessenger ;
    go:thenClause ex:obedienceToGod .

ex:obedienceToMessenger a go:Event ;
    go:HasAgent ex:Person ;
    go:HasTheme ex:Messenger ;
    rdf:type go:Obedience .

ex:obedienceToGod a go:Event ;
    go:HasAgent ex:Person ;
    go:HasTheme ex:God ;
    rdf:type go:Obedience .
go:Event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/362-12> rdf:type owl:Ontology .

ex:SpeechEvent1 a go:SpeechEvent ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Statement1 .

ex:God a go:Agent .

ex:You a go:Agent .

ex:ThoseWhoPledgeAllegiance a go:Group ;
    go:relatedTo ex:You .

ex:PledgeAllegianceEvent1 a go:SocialEvent ;
    go:HasAgent ex:ThoseWhoPledgeAllegiance ;
    go:HasRecipient ex:God ;
    go:description "Pledge allegiance to You means pledge allegiance to God"^^xsd:string .

ex:Statement1 a go:Text ;
    go:content "Indeed, those who pledge allegiance to you are actually pledging allegiance to God; the hand of God is above their hands."^^xsd:string .

ex:HandOfGod a go:Substance ;
    go:positionedAbove ex:HandsOfThoseWhoPledge .

ex:HandsOfThoseWhoPledge a go:Substance .

ex:God go:hasPart ex:HandOfGod .

ex:ThoseWhoPledgeAllegiance go:hasPart ex:HandsOfThoseWhoPledge .
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.


