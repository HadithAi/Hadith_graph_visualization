@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/283-1> rdf:type owl:Ontology .

ex:God a go:Deity , go:entity ;
    go:hasQuality ex:Indescribable .

ex:Indescribable a go:Quality .
go:Quality rdfs:label "ویژگی"@fa.
@prefix : <http://etratnet.ir/graph/> .
@prefix ex: <http://etratnet.ir/graph/proposition/283#> .
@prefix bt: <http://borhan-onto.ir/ontology/time#> .
@prefix go: <http://etratnet.ir/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/283-2> rdf:type owl:Ontology .

ex:God a go:Substance ;
    rdfs:label "God" .

ex:Quran a go:WrittenWork ;
    rdfs:label "Quran" ;
    go:hasAuthor ex:God .

ex:DescriptionEvent a go:Event ;
    rdfs:label "Description of God" ;
    go:hasTheme ex:God ;
    go:hasAgent ex:Humans ;
    go:EventProperty ex:Indescribability .

ex:AppraisalEvent a go:Event ;
    rdfs:label "Appraisal of God" ;
    go:hasAgent ex:Humans ;
    go:hasTheme ex:God ;
    go:EventProperty ex:ImproperAppraisal ;
    go:reportedIn ex:Quran .

ex:Humans a go:Agent ;
    rdfs:label "Humans" .

ex:Indescribability a ex:Quality ;
    rdfs:label "Indescribability" .

ex:ImproperAppraisal a ex:Quality ;
    rdfs:label "Did not appraise God with true appraisal" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.
