<http://etratnet.ir/graph/ontology/283> rdf:type go:ontology ;
        go:hascontent "11. از امام صادق عليه‌السلام شنيدم كه مى‌فرمايد: «خداوند وصف نشود. چگونه وصف شود حال آن كه در كتابش فرموده است: «خدا را چنان كه بايسته شناخت اوست، نشناختند»؟ پس، به هر صفتى وصف [و شناسانده] شود همچنان بالاتر از آن است»."@fa,
        "11.  سَمِعْتُ أَبَا عَبْدِ اللّه‌ِ عليه‌السلام يَقُولُ: «إِنَّ اللّه‌َ لاَ يُوصَفُ، وَكَيْفَ يُوصَفُ وَقَدْ قَالَ فِي كِتَابِهِ: «وَما قَدَرُوا اللّه‌َ حَقَّ قَدْرِهِ»؟! فَلاَ يُوصَفُ بِقَدَرٍ إِلاَّ كَانَ أَعْظَمَ مِنْ ذلِكَ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/283-1> rdf:type owl:ontology .

ex:god a go:deity , go:entity ;
    go:hasquality ex:indescribable .

ex:indescribable a go:quality .
go:quality rdfs:label "ویژگی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/283-2> rdf:type owl:ontology .

ex:god a go:substance ;
    rdfs:label "god" .

ex:quran a go:writtenwork ;
    rdfs:label "quran" ;
    go:hasauthor ex:god .

ex:descriptionevent a go:event ;
    rdfs:label "description of god" ;
    go:hastheme ex:god ;
    go:hasagent ex:humans ;
    go:eventproperty ex:indescribability .

ex:appraisalevent a go:event ;
    rdfs:label "appraisal of god" ;
    go:hasagent ex:humans ;
    go:hastheme ex:god ;
    go:eventproperty ex:improperappraisal ;
    go:reportedin ex:quran .

ex:humans a go:agent ;
    rdfs:label "humans" .

ex:indescribability a ex:quality ;
    rdfs:label "indescribability" .

ex:improperappraisal a ex:quality ;
    rdfs:label "did not appraise god with true appraisal" .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/283-3> rdf:type owl:ontology .

ex:god a go:deity .

ex:measure a go:quantity .

ex:descriptionevent a go:state ;
    go:hastheme ex:god ;
    go:hascondition ex:greaterthanmeasure ;
    go:negated true .

ex:greaterthanmeasure a go:condition ;
    go:hascomparandum ex:measure ;
    go:hascomparativesubject ex:god ;
    go:hascomparator "greater"^^<http://www.w3.org/2001/xmlschema#string> .