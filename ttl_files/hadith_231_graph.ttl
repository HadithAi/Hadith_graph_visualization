    @prefix bt: <http://borhan-onto.ir/ontology/time#> .
    @prefix go: <http://etratnet.ir/ontology#> .
    @prefix owl: <http://www.w3.org/2002/07/owl#> .
    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix xml: <http://www.w3.org/xml/1998/namespace> .
    @prefix xsd: <http://www.w3.org/2001/xmlschema#> .
    @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
    @prefix time: <http://www.w3.org/2006/time#> .
    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/231> rdf:type go:ontology ;
        go:hascontent "3.  به امام صادق عليه‌السلام عرض كردم: من با گروهى مناظره كردم و به آنها گفتم: خداوند، جلّ جلاله، بزرگ‌تر و ارجمندتر و والاتر از آن است كه به واسطه آفريدگانش شناخته شود؛ بلكه اين بندگان‌اند كه به سبب خدا شناخته مى‌شوند. حضرت فرمود: «رحمت خدا بر تو باد»."@fa,
        "3.  قُلْتُ لِأَبِي عَبْدِ اللّه‌ِ عليه‌السلام: إِنِّي نَاظَرْتُ قَوْماً، فَقُلْتُ لَهُمْ: إِنَّ اللّه‌َ ـ جَلَّ جَلاَلُهُ ـ أَجَلُّ وَأَعَزُّ وَأَكْرَمُ مِنْ أَنْ يُعْرَفَ بِخَلْقِهِ، بَلِ الْعِبَادُ يُعْرَفُونَ بِاللّه‌ِ، فَقَالَ: «رَحِمَكَ اللّه‌ُ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/231-1> rdf:type owl:ontology .

ex:speaker a go:person ;
    rdfs:label "speaker (i)" .

ex:abdulallah a go:person ;
    rdfs:label "abdul allah" ;
    ex:hashonorific "peace be upon him" .

ex:groupofpeople a go:group ;
    rdfs:label "group of people" .

ex:sayevent a go:event ;
    a go:communication ;
    go:hasagent ex:speaker ;
    go:hasrecipient ex:abdulallah ;
    go:hastheme ex:utterance1 .

ex:utterance1 a go:message ;
    rdfs:label "statement about arguing with a group" .

ex:argueevent a go:event ;
    a go:dispute ;
    go:hasagent ex:speaker ;
    go:hastheme ex:groupofpeople .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/231-2> rdf:type owl:ontology .

ex:sayevent1 a go:event , ex:sayevent ;
    go:hasagent ex:speaker_i ;
    go:hasrecipient ex:hearers_thesepeople ;
    go:hastheme ex:statement1 .

ex:speaker_i a go:agent ;
    rdfs:label "i" .

ex:hearers_thesepeople a go:agent ;
    rdfs:label "these people" .

ex:allah a go:substance ;
    go:hasattribute ex:majestic , ex:honorable , ex:generous ;
    go:istranscendentover ex:beingknownbycreation .

ex:majestic a go:attribute ;
    go:haslabel "more majestic" .

ex:honorable a go:attribute ;
    go:haslabel "more honorable" .

ex:generous a go:attribute ;
    go:haslabel "more generous" .

ex:beingknownbycreation a go:abstractconcept ;
    go:haslabel "being known by his creation" .

ex:statement1 a go:proposition ;
    go:about ex:allah ;
    go:expresses [
        a go:attributecomparison ;
        go:compares ex:allah ;
        go:isgreaterthan ex:beingknownbycreation ;
        go:hasattributes (ex:majestic ex:honorable ex:generous)
    ] .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/231-3> rdf:type owl:ontology .

ex:god a go:substance ;
    rdfs:label "god"@en .

ex:people a go:substance ;
    rdfs:label "the people"@en .

ex:servants a go:substance ;
    rdfs:label "the servants"@en .

ex:knowingevent1 a ex:knowingevent, go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:people .

ex:knowingevent2 a ex:knowingevent, go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:servants .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/231-4> rdf:type owl:ontology .

ex:abu_abdillah_a_salam a go:person ;
    rdfs:label "abu abdillah (peace be upon him)" .

ex:speechevent_1 a go:speechevent ;
    go:hasagent ex:abu_abdillah_a_salam ;
    go:hastheme "may god have mercy on you." .