<http://etratnet.ir/ontology/324-55> rdf:type owl:ontology .

# define entities
ex:we rdf:type go:agent .
ex:creator rdf:type go:substance ; rdfs:label "creator"@en .
ex:creation rdf:type go:substance ; rdfs:label "this creation"@en .

# define the knowing event
ex:knowingevent rdf:type go:event , ex:knowingeventinstance ; 
  go:hasagent ex:we ;
  go:hastheme ex:proposition .

# proposition that creator is kind in his creation (modeled as an event of kindness)
ex:kindnessevent rdf:type go:event ;
  go:hasagent ex:creator ;
  go:hastheme ex:creation ;
  rdfs:label "creator is kind in his creation"@en .

# the proposition is represented as a statement about kindnessevent
ex:proposition rdf:type go:proposition ;
  go:describes ex:kindnessevent .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/324-56> rdf:type owl:ontology .

ex:creationevent1 a go:creationevent ;
    go:hasagent ex:god_almighty ;
    go:hastheme ex:everything ;
    go:hasmanner ex:withouttreatment ;
    go:hasmanner ex:withouttools .

ex:god_almighty a go:deity .

ex:everything a go:substance .

ex:withouttreatment a go:manner .

ex:withouttools a go:manner .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/324-57> rdf:type owl:ontology .

# class definitions
ex:maker a rdfs:class ; rdfs:subclassof go:maker .
ex:thing a rdfs:class ; rdfs:subclassof go:object .
ex:createevent a rdfs:class ; rdfs:subclassof go:event .

# general statement expressed as a blank node representing universal quantification
_:stmt a rdf:statement ;
    rdf:subject ex:maker ;
    rdf:predicate go:performs ;
    rdf:object _:event .

_:event a ex:createevent ;
    go:hasagent ex:maker ;
    go:hastheme ex:thing .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/324-58> rdf:type owl:ontology .

ex:god a go:substance ;
    rdfs:label "الله" ;
    go:hasattribute ex:creator, ex:gentle, ex:majestic .

ex:creator a go:attribute .
ex:gentle a go:attribute .
ex:majestic a go:attribute .

ex:creationevent a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:createdthing ;
    go:hasorigin ex:nothing .

ex:createdthing a go:substance ;
    go:createdby ex:god .

ex:nothing a go:substance ;
    go:exists "false"^^<http://www.w3.org/2001/xmlschema#boolean> .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.