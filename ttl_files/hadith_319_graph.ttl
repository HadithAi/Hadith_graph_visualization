<http://etratnet.ir/graph/ontology/319> rdf:type go:ontology ;
        go:hascontent "8.  مردى در محضر امام صادق عليه‌السلام «اللّه‌ اكبر (= خدا بزرگ‌تر است)» گفت؛ حضرت فرمود: «خدا بزرگ‌تر از چه چيز است؟» مرد گفت: از همه چيز. امام صادق عليه‌السلام فرمود: «محدودش ساختى». مرد گفت: پس، چه بگويم؟ فرمود: «بگو: خدا بزرگ‌تر از آن است كه به وصف درآيد»."@fa,
        "8. عَنْ أَبِي عَبْدِ اللّه‌ِ عليه‌السلام، قَالَ: قَالَ رَجُلٌ عِنْدَهُ: اللّه‌ُ أَكْبَرُ، فَقَالَ: «اللّه‌ُ أَكْبَرُ مِنْ أَيِّ شَيْءٍ؟» فَقَالَ: مِنْ كُلِّ شَيْءٍ، فَقَالَ أَبُو عَبْدِ اللّه‌ِ عليه‌السلام: «حَدَّدْتَهُ» فَقَالَ الرَّجُلُ: كَيْفَ أَقُولُ؟ قَالَ: «قُلْ: اللّه‌ُ أَكْبَرُ مِنْ أَنْ يُوصَفَ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/319-1> rdf:type owl:ontology .

ex:man a go:substance ;
    rdfs:label "الرجل" .

ex:abu_abdullah a go:substance ;
    rdfs:label "أبي عبد الله عليه السلام" .

ex:speechevent1 a go:speechevent ;
    go:hasagent ex:man ;
    go:hasrecipient ex:abu_abdullah ;
    go:hastheme "الله أكبر" .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/319-2> rdf:type owl:ontology .

ex:speechevent1 a go:speechevent ;
    go:hasagent ex:abu_abdullah ;
    go:hasrecipient ex:man ;
    go:hastheme ex:question1 .

ex:abu_abdullah a go:person ;
    rdfs:label "abu abdullah (peace be upon him)" .

ex:man a go:person ;
    rdfs:label "man" .

ex:question1 a go:informationcontent ;
    rdfs:label "is god greater than anything?" ;
    go:refersto ex:god , ex:anything .

ex:god a go:deity ;
    rdfs:label "god" .

ex:anything a go:thing ;
    rdfs:label "anything" .
go:information rdfs:label "اطلاعات"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/319-3> rdf:type owl:ontology .

ex:man a go:person ;
    rdfs:label "man"@en .

ex:god a go:deity ;
    rdfs:label "god"@en .

ex:everything a go:concept ;
    rdfs:label "everything"@en .

ex:speech1 a go:communicationevent ;
    go:hasagent ex:man ;
    go:hastheme ex:statement1 .

ex:statement1 a go:comparativestatement ;
    go:hassubject ex:god ;
    go:hasobject ex:everything ;
    go:greaterthan true .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/319-4> rdf:type owl:ontology .

ex:abu_abdullah a go:person ;
  rdfs:label "abu abdullah" .

ex:god a go:deity ;
  rdfs:label "god" .

ex:sayevent1 a go:sayevent , go:event ;
  go:hasagent ex:abu_abdullah ;
  go:hastheme ex:defineevent1 ;
  rdfs:label "speech by abu abdullah: 'i have defined god.'" .

ex:defineevent1 a go:definitionevent , go:event ;
  go:hasagent ex:abu_abdullah ;
  go:hastheme ex:god ;
  rdfs:label "defining god" .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/319-5> rdf:type owl:ontology .

ex:man1 a go:person ;
    rdfs:label "الرجل"@ar .

ex:questioncontent1 a go:informationobject ;
    go:hastext "كيف أقول ذلك؟"@ar .

ex:event1 a go:event ;
    go:hasagent ex:man1 ;
    go:hastheme ex:questioncontent1 ;
    rdfs:label "questioning event" .
go:event rdfs:label "رخداد"@fa.
go:information rdfs:label "اطلاعات"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/319-6> rdf:type owl:ontology .

ex:abu_abdullah a go:person ;
    go:hastitle "peace be upon him" .

ex:speechevent1 a go:speechevent ;
    go:hasagent ex:abu_abdullah ;
    go:hastheme ex:commandevent1 .

ex:commandevent1 a go:commandevent ;
    go:hasagent ex:abu_abdullah ;
    go:hastheme ex:utterance1 .

ex:utterance1 a go:utterance ;
    go:utteredtext "god is greater than can be described." .

ex:god a go:deity .

ex:description a go:property ;
    go:propertyof ex:god ;
    go:propertytype "descriptive capacity" ;
    go:propertyvalue "greater than can be described" .