@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/319-2> rdf:type owl:Ontology .

ex:SpeechEvent1 a go:SpeechEvent ;
    go:HasAgent ex:Abu_Abdullah ;
    go:HasRecipient ex:Man ;
    go:HasTheme ex:Question1 .

ex:Abu_Abdullah a go:Person ;
    rdfs:label "Abu Abdullah (peace be upon him)" .

ex:Man a go:Person ;
    rdfs:label "Man" .

ex:Question1 a go:InformationContent ;
    rdfs:label "Is God greater than anything?" ;
    go:refersTo ex:God , ex:Anything .

ex:God a go:Deity ;
    rdfs:label "God" .

ex:Anything a go:Thing ;
    rdfs:label "Anything" .
go:Information rdfs:label "اطلاعات"@fa.
