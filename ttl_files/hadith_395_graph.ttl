<http://etratnet.ir/graph/ontology/395> rdf:type go:ontology ;
        go:hascontent "1.  «خداوند پيش از آن كه آفريدگانش را بيافريند، خوشبختى و بدبختى را آفريد. بنابراين، خدا كسى را كه خوشبخت آفريده هرگز دشمن نمى‌دارد، و چنانچه كار بدى انجام دهد كار او را دشمن مى‌دارد و خودش را دشمن نمى‌دارد، و اگر بدبخت [آفريده شده] باشد هرگز او را دوست نمى‌دارد، و چنانچه كار نيكى انجام دهد كار او را دوست مى‌دارد امّا به خاطر سرانجامى كه به سوى آن مى‌رود، خودش را دشمن مى‌دارد. پس، هرگاه خدا چيزى را دوست داشته باشد هرگز منفورش نمى‌دارد، و هرگاه چيزى را منفور بدارد هرگز دوستش نمى‌دارد»."@fa,
        "1.  عَنْ أَبِي عَبْدِ اللّه‌ِ عليه‌السلام، قَالَ: «إِنَّ اللّه‌َ خَلَقَ السَّعَادَةَ وَالشَّقَاءَ قَبْلَ أَنْ يَخْلُقَ خَلْقَهُ، فَمَنْ خَلَقَهُ اللّه‌ُ سَعِيداً، لَمْ يُبْغِضْهُ أَبَداً، وَإِنْ عَمِلَ شَرّاً، أَبْغَضَ عَمَلَهُ وَلَمْ يُبْغِضْهُ، وَإِنْ كَانَ شَقِيّاً، لَمْ يُحِبَّهُ أَبَداً، وَإِنْ عَمِلَ صَالِحاً، أَحَبَّ عَمَلَهُ وَأَبْغَضَهُ؛ لِمَا يَصِيرُ إِلَيْهِ، فَإِذَا أَحَبَّ اللّه‌ُ شَيْئاً، لَمْ يُبْغِضْهُ أَبَداً، وَإِذَا أَبْغَضَ شَيْئاً، لَمْ يُحِبَّهُ أَبَداً»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/395-1> rdf:type owl:ontology .

ex:god a go:deity ;
    rdfs:label "god" .

ex:happiness a go:substance ;
    rdfs:label "happiness" .

ex:misery a go:substance ;
    rdfs:label "misery" .

ex:creation a go:substance ;
    rdfs:label "creation" .

ex:create_happiness_misery ex:before ex:create_creation .

ex:create_happiness_misery a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:happiness , ex:misery .

ex:create_creation a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:creation .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/395-2> rdf:type owl:ontology .

ex:hateevent a go:event ;
    go:hasagent ex:allah ;
    go:hastheme ex:happycreature .

ex:allah a go:substance .

ex:happycreature a go:substance ;
    go:hasstate ex:happystate .

ex:happystate a go:substance ;
    go:description "happy" .

# negation of the hate event
ex:hateevent a go:event ;
    go:negation true .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/395-3> rdf:type owl:ontology .

ex:god a go:deity .
ex:al_saeed a go:person .

ex:work_al_saeed a go:event ;
    go:hasagent ex:al_saeed .

ex:doing_evil a go:event ;
    go:hasagent ex:al_saeed .

ex:hate_action a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:work_al_saeed ;
    go:hascause ex:doing_evil .

# capturing that god "hates" the work of al-saeed when he does evil
ex:god ex:emotionalattitude [
    a ex:dislike ;
    ex:target ex:work_al_saeed ;
    ex:condition ex:doing_evil
] .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/395-4> rdf:type owl:ontology .

ex:god a go:deity .
ex:happycreature a go:substance ;
    go:hasquality go:happiness .

ex:commitevilevent a go:event ;
    go:hasagent ex:happycreature ;
    go:hastheme go:evil .

# conditional statement that god does not hate the happycreature if it commits evil
_:condition a go:conditionalevent ;
    go:condition ex:commitevilevent ;
    go:result [ a go:negation ;
                go:hasagent ex:god ;
                go:hastheme ex:happycreature ;
                go:emotion go:hate ] .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/395-5> rdf:type owl:ontology .

ex:god a go:agent ;
    rdfs:label "god" .

ex:creature a go:substance ;
    go:hasquality ex:miserable .

ex:miserable a go:quality ;
    go:qualifies ex:creature .

ex:loveevent a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:creature .

ex:notlovingmiserable a go:negation ;
    go:negates ex:loveevent ;
    go:condition ex:miserable .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:quality rdfs:label "ویژگی"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/395-6> rdf:type owl:ontology .

ex:eventinstance1 a go:event ;
    go:hasagent ex:miserablecreation1 ;
    go:hastheme ex:work1 ;
    ex:hasquality ex:righteousness .

ex:god a go:god ;
    go:hastheme ex:work1 ;   # god loves the work
    ex:loves ex:eventinstance1 .

ex:miserablecreation1 a go:person ;
    ex:hasattribute ex:miserable .

ex:work1 a go:substance ;
    ex:iscreatedby ex:miserablecreation1 .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/395-7> rdf:type owl:ontology .

ex:event_hate_1 a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:work_1 .

ex:god a go:substance .

ex:work_1 a go:substance ;
    go:haspossessor ex:creature_miserable_1 .

ex:creature_miserable_1 a go:substance ;
    go:hasproperty "miserable" .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/395-8> rdf:type owl:ontology .

ex:eventlove a go:event ;
    go:hasagent go:god ;
    go:hastheme ex:workofwretchedcreation .

ex:workofwretchedcreation a go:work ;
    go:hascreator ex:wretchedcreation .

ex:wretchedcreation a go:creagen ;
    go:hasproperty ex:wretchedness .

ex:eventreturn a go:event ;
    go:hasagent ex:workofwretchedcreation ;
    go:hastheme go:god .

ex:eventlove go:hascause ex:eventreturn .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/395-9> rdf:type owl:ontology .

ex:god a go:agent .
ex:something a go:substance .

ex:loveevent a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:something .

ex:hateevent a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:something ;
    owl:negativepropertyassertion [ 
        owl:sourceindividual ex:hateevent ;
        owl:assertionproperty go:exists ;
        owl:targetindividual false
    ] .

ex:conditionalstatement a go:condition ;
    go:hascondition [
        a go:event ;
        go:hasagent ex:god ;
        go:hastheme ex:something ;
        a ex:loveevent
    ] ;
    go:hasconsequence [
        a go:event ;
        go:hasagent ex:god ;
        go:hastheme ex:something ;
        a ex:hateevent ;
        owl:negativepropertyassertion [ 
            owl:sourceindividual ex:hateevent ;
            owl:assertionproperty go:exists ;
            owl:targetindividual false
        ]
    ] .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/395-10> rdf:type owl:ontology .

ex:god a go:substance .
ex:something a go:substance .

ex:hateevent a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:something .

ex:loveevent a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:something .

# conditional rule representation (informal comment): if hateevent occurs, then loveevent does not occur for the same theme.
# this is captured semantically, but logically would require a rule beyond rdf triples.
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.