go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/412-10> rdf:type owl:ontology .

ex:utterance1 a go:speechevent ;
    go:hasagent ex:god_almighty ;
    go:hastheme ex:proposition1 .

ex:god_almighty a go:deity ;
    rdfs:label "god almighty" .

ex:human_servant a go:person ;
    rdfs:label "human servant" ;
    go:hasattribute ex:evil_deeds .

ex:evil_deeds a go:attribute ;
    go:description "evil deeds of human servant" .

ex:proposition1 a go:proposition ;
    go:expresses "a human servant is more deserving of his evil deeds than god almighty" ;
    go:hassubject ex:human_servant ;
    go:hasobject ex:god_almighty ;
    go:relatesto ex:evil_deeds .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/412-11> rdf:type owl:ontology .

ex:sayevent1 a go:event ;
    go:hasagent ex:allah_almighty ;
    go:hastheme ex:quote1 .

ex:allah_almighty a go:agent ;
    rdfs:label "allah almighty" .

ex:quote1 a go:text ;
    go:containsstatement ex:statement1 , ex:statement2 .

ex:statement1 a go:state ;
    go:hassubject ex:allah_almighty ;
    go:isnotquestionedabout ex:actionsofallah .

ex:actionsofallah a go:action .

ex:statement2 a go:futureevent ;
    go:hassubject ex:humans ;
    go:isquestionedabout ex:actionsofhumans .

ex:humans a go:agent ;
    go:haslabel "they (humans)" .

ex:actionsofhumans a go:action .
go:event rdfs:label "رخداد"@fa.
go:agent rdfs:label "عامل"@fa.


