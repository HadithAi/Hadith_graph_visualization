ex:eventInstance1 a go:Event ;
    go:HasTheme ex:thing1 .

ex:thing1 a go:Substance ;
    go:hasQuality "unreasonable" ;
    go:hasQuality "unlimited" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.