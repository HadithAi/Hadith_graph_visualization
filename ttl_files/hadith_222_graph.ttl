@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/222-1> rdf:type owl:Ontology .

ex:Abu_Jaafar a go:Person ;
    go:hasHonorific "peace be upon him"^^xsd:string ;
    go:label "Mr. Abu Ja'far"^^xsd:string .

ex:Monotheism a go:Concept ;
    go:label "Monotheism"^^xsd:string ;
    ex:isNotRational true ;
    ex:isNotLimited true .

ex:Utterance1 a go:SpeechAct ;
    go:HasAgent ex:Abu_Jaafar ;
    go:HasTheme ex:Monotheism ;
    go:expresses ex:Proposition1 .

ex:Proposition1 a go:Proposition ;
    go:hasSubject ex:Monotheism ;
    ex:hasProperty ex:NonRational ;
    ex:hasProperty ex:NonLimited .

ex:NonRational a go:Property ;
    go:label "not rational"^^xsd:string .

ex:NonLimited a go:Property ;
    go:label "not limited"^^xsd:string .
