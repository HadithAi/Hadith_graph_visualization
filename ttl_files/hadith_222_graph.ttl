@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/222-2> rdf:type owl:Ontology .

ex:event1 a go:Event ;
    go:HasAgent ex:Speaker ;
    go:HasTheme ex:ContraryToMonotheismCondition .

ex:Speaker a go:agent ;
    go:denotedBy "المتحدث"@ar .

ex:ContraryToMonotheismCondition a go:state ;
    go:HasQuality ex:ContraryToMonotheismQuality .

ex:ContraryToMonotheismQuality a go:Quality ;
    go:denotedBy "خلاف التوحيد"@ar .

ex:Monotheism a go:concept ;
    go:denotedBy "التوحيد"@ar .
go:Event rdfs:label "رخداد"@fa.
go:Quality rdfs:label "ویژگی"@fa.
