<http://etratnet.ir/ontology/222-5> rdf:type owl:Ontology .

ex:Delusions a go:Substance ;
    rdfs:label "Delusions"@en .

ex:Monotheism a go:Substance ;
    rdfs:label "Monotheism"@en .

ex:NotComprehendEvent a go:Event ;
    go:HasAgent ex:Delusions ;
    go:HasTheme ex:Monotheism ;
    go:hasResult ex:Negation .

ex:Negation a go:state ;
    rdfs:label "Negation of comprehension"@en .

ex:ContraryToComprehension a go:state ;
    rdfs:label "Contrary to what is comprehensible"@en .

ex:NotComprehendEvent go:hasCause ex:ContraryToComprehension .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
