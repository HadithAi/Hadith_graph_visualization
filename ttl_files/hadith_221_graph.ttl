<http://etratnet.ir/graph/ontology/221> rdf:type go:ontology ;
        go:hascontent "7. «آفرينشِ خداوندِ تسخيرگر، سلطنت خداوندِ قاهر، شكوه آشكار خداوند، روشنايى خيره‌كننده خداوند، برهان راستين خداوند، و آنچه زبان بندگان بدان گويا [و معترف]شده، و آنچه خداوند توسط پيامبران فرستاده، و آنچه بر بندگان نازل فرموده، خود براى خردمندان دليلى كافى بر وجود خداوندگار ارجمند و شكوهمند است»."@fa,
        "7. عَنْ أَبِي جَعْفَرٍ عليه‌السلام، قَالَ: «كَفى لِأُولِي الْأَلْبَابِ بِخَلْقِ الرَّبِّ الْمُسَخِّرِ، وَمُلْكِ الرَّبِّ الْقَاهِرِ، وَجَلاَلِ الرَّبِّ الظَّاهِرِ، وَنُورِ الرَّبِّ الْبَاهِرِ، وَبُرْهَانِ الرَّبِّ الصَّادِقِ، وَمَا أَنْطَقَ بِهِ أَلْسُنَ الْعِبَادِ، وَمَا أَرْسَلَ بِهِ الرُّسُلَ، وَمَا أَنْزَلَ عَلَى الْعِبَادِ، دَلِيلاً عَلَى الرَّبِّ عَزَّ وَجَلَّ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/221-1> rdf:type owl:ontology .

ex:creationevent1 a go:creation, go:event ;
    go:hasagent ex:thelord ;
    go:hastheme ex:livestock ;
    go:hasrecipient ex:peopleofunderstanding .

ex:thelord a go:substance .
ex:livestock a go:substance ;
    ex:sufficient "true"^^<http://www.w3.org/2001/xmlschema#boolean> .
ex:peopleofunderstanding a go:substance .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/221-2> rdf:type owl:ontology .

ex:event1 a go:state ;
    go:hasagent ex:lordgodalmighty ;
    go:hastheme ex:wisepeople ;
    go:statetype ex:sufficiency .

ex:lordgodalmighty a go:deity ;
    rdfs:label "lord god almighty"@en .

ex:wisepeople a go:persongroup ;
    rdfs:label "those who have understanding"@en .

ex:sufficiency a go:statetype ;
    rdfs:label "sufficiency"@en .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/221-3> rdf:type owl:ontology .

ex:manifestlordmajesty a go:substance ;
    rdfs:label "manifest lord's majesty"@en .

ex:peopleofunderstanding a go:substance ;
    rdfs:label "people of understanding"@en .

ex:sufficiencyevent a go:event ;
    rdfs:label "sufficiency"@en ;
    go:hastheme ex:manifestlordmajesty ;
    go:hasrecipient ex:peopleofunderstanding .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/221-4> rdf:type owl:ontology .

ex:brightlightoflord a go:substance ;
    rdfs:label "bright light of the lord"@en .

ex:peopleofunderstanding a go:group ;
    rdfs:label "people of understanding"@en .

ex:sufficiencystate a go:state ;
    rdfs:label "sufficiency of bright light for people of understanding"@en ;
    go:hastheme ex:brightlightoflord ;
    go:hasrecipient ex:peopleofunderstanding ;
    go:hasproperty ex:sufficiency .

ex:sufficiency a go:property ;
    rdfs:label "sufficient"@en .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/221-5> rdf:type owl:ontology .

ex:proofoftruthfullord a go:substance ;
    ex:hasattribute ex:truthfullord .

ex:thoseofsoundunderstanding a go:group .

ex:sufficiencyrelation a go:state ;
    go:hastheme ex:proofoftruthfullord ;
    go:hasrecipient ex:thoseofsoundunderstanding .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/221-6> rdf:type owl:ontology .

ex:event_pronouncing a go:event ;
    go:hasagent ex:entity_tongues_of_servants ;
    go:hastheme ex:entity_statement_uttered .

ex:entity_tongues_of_servants a go:agent ;
    go:partof ex:entity_servants .

ex:entity_statement_uttered a go:substance ;
    go:isevidenceof ex:entity_lord_almighty .

ex:entity_servants a go:group .

ex:entity_lord_almighty a go:deity ;
    go:hasattribute ex:attribute_almighty .

ex:attribute_almighty a go:attribute .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/221-7> rdf:type owl:ontology .

ex:eventinstance1 a go:event ;
    go:hasagent ex:messengers ;
    ex:isevidenceof ex:lord_almighty .

ex:messengers a go:agent .
ex:lord_almighty a go:deity .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/221-8> rdf:type owl:ontology .

ex:guidance a go:substance ;
    go:label "guidance" .

ex:servants a go:agent ;
    go:label "servants" .

ex:lord_a a go:agent ;
    go:label "lord, the almighty" .

ex:sendingdownguidance a go:event ;
    go:hastheme ex:guidance ;
    go:hasrecipient ex:servants ;
    go:hastopic ex:lord_a ;
    go:eventstatus "notoccurred"^^<http://www.w3.org/2001/xmlschema#string> .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.