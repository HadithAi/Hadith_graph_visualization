  go:hastheme ex:statement1 .

ex:prophet a go:person ;
  rdfs:label "الرسول"@ar ;
  rdfs:label "the prophet"@en .

ex:statement1 a go:statement ;
  rdfs:label "the muslim is the one who makes other muslims safe from his tongue and hand."@en ;
  go:describes ex:muslimsaferole .

ex:muslimsaferole a go:role ;
  go:characterizedby [
    a go:propertyvalue ;
    go:hasproperty ex:safetyfromtonguehand ;
    go:hasvalue true
  ] .

ex:safetyfromtonguehand a go:property ;
