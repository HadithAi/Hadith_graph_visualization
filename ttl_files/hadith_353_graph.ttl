  rdfs:label "muslims"@en .

ex:tongue a go:anatomicalstructure ;
  rdfs:label "لسان"@ar ;
  rdfs:label "tongue"@en .

ex:hand a go:anatomicalstructure ;
  rdfs:label "يد"@ar ;
  rdfs:label "hand"@en .
go:event rdfs:label "رخداد"@fa.


