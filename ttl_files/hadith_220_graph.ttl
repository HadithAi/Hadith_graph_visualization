@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/220-22> rdf:type owl:ontology .

ex:themaker a go:substance ;
    rdfs:label "the maker" .

ex:fivesenses a go:substance ;
    rdfs:label "five senses" .

ex:perceptionevent a go:event ;
    go:hasagent ex:fivesenses ;
    go:hastheme ex:themaker ;
    go:hasaction "perceive" ;
    ex:isnegated true .

ex:feelingevent a go:event ;
    go:hasagent ex:fivesenses ;
    go:hastheme ex:themaker ;
    go:hasaction "feel" ;
    ex:isnegated true .

ex:understandingevent a go:event ;
    go:hasagent ex:fivesenses ;
    go:hastheme ex:themaker ;
    go:hasaction "understand" ;
    ex:isnegated true .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


