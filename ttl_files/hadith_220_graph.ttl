@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/220-1> rdf:type owl:Ontology .

ex:SpeechEvent1 a go:Speech ;
    go:HasAgent ex:AbuAbdullah_AlayhisSalam ;
    go:HasTheme ex:Quote1 .

ex:AbuAbdullah_AlayhisSalam a go:Person ;
    rdfs:label "Abu Abdullah (peace be upon him)" .

ex:Quote1 a go:Quotation ;
    go:hasText "They are two who may be either both old and strong, or both weak, or one strong and the other weak." ;
    go:refersTo ex:Entity1, ex:Entity2 .

ex:Entity1 a go:Substance ;
    go:hasQuality ex:Old, ex:Strong .

ex:Entity2 a go:Substance ;
    go:hasQuality ex:Old, ex:Strong .

ex:AlternativeScenario1 a go:Scenario ;
    go:hasTheme ex:Entity1, ex:Entity2 ;
    go:hasQuality ex:Weak ;
    go:includesEntities ex:Entity1 , ex:Entity2 .

ex:AlternativeScenario2 a go:Scenario ;
    go:hasTheme ex:Entity1, ex:Entity2 ;
    go:hasQuality ex:Strong ;
    go:hasQuality ex:Weak .

ex:ScenarioLink a go:Or ;
    go:includes ex:Entity1, ex:Entity2 .

ex:Old a go:Quality ;
    go:label "old" .

ex:Strong a go:Quality ;
    go:label "strong" .

ex:Weak a go:Quality ;
    go:label "weak" .
go:Substance rdfs:label "شی"@fa.
go:Quality rdfs:label "ویژگی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/220-2> rdf:type owl:Ontology .

ex:Entity1 a go:Individual .
ex:Entity2 a go:Individual .

ex:BothStrongCondition a ex:ConditionEvent ;
    ex:hasAgent ex:Entity1, ex:Entity2 ;
    ex:condition "both strong" .

# Under this condition:

# Neither Entity1 nor Entity2 rejects the other
ex:RejectEvent a go:Event .
# We represent the absence by stating that ex:Entity1 does not have a go:hasRejected relationship with ex:Entity2, similarly for the reverse (not explicitly stated in triples as absence is implicit)

# Neither Entity1 nor Entity2 monopolizes the planning
ex:MonopolizeEvent a go:Event ;
    ex:theme ex:Planning .
# Similarly, no single entity has ex:exclusivelyControls ex:Planning

# Hence the condition implies mutual acceptance and shared planning

go:Event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/220-3> rdf:type owl:Ontology .

ex:Entity1 a go:entity ;
    rdfs:label "Entity One" .
ex:Entity2 a go:entity ;
    rdfs:label "Entity Two" .

ex:ClaimEvent a go:Event ;
    rdfs:label "Claim that Entity1 is strong and Entity2 is weak" ;
    go:HasAgent ex:Agent ;
    go:HasTheme [
        ex:assertion "Entity1 is strong and Entity2 is weak" ;
        ex:subject1 ex:Entity1 ;
        ex:subject2 ex:Entity2 ;
        ex:property1 ex:Strong ;
        ex:property2 ex:Weak
    ] .

ex:FactEvent a go:Event ;
    rdfs:label "Fact that Entity1 and Entity2 are identical due to deficiency in Entity2" ;
    go:HasTheme [
        rdf:type ex:IdentityAssertion ;
        ex:entities (ex:Entity1 ex:Entity2)
    ] ;
    go:HasCause [
        ex:Deficiency a go:Quality ;
        go:QualityOf ex:Entity2 ;
        rdfs:label "Apparent deficiency"
    ] .

ex:Strong a go:Quality ;
    rdfs:label "Strong" .
ex:Weak a go:Quality ;
    rdfs:label "Weak" .

ex:Agent a go:entity ;
    rdfs:label "Claimant" .
go:Event rdfs:label "رخداد"@fa.
go:Quality rdfs:label "ویژگی"@fa.


