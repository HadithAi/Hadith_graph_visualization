<http://etratnet.ir/ontology/243-13> rdf:type owl:ontology .

# entities
ex:headofgalut a go:person ;
    go:hasrole "headofgalut"^^xsd:string .

ex:religion a go:substance ;
    rdfs:label "religion"^^xsd:string ;
    go:istrue "true"^^xsd:boolean .

# event of speech
ex:testimonyevent a go:event ;
    go:hasagent ex:headofgalut ;
    go:hastheme ex:testimonycontent .

# the content of the testimony
ex:testimonycontent a go:utterance ;
    go:expresses "i testify that your religion is the truth."^^xsd:string ;
    go:about ex:religion .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

