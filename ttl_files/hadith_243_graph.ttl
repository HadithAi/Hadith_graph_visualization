    go:denotes "رَأسُ الجالوتِ" .

ex:yourreligion a go:religion ;
    go:denotes "دينك" .

# event of bearing witness
	evt:testimony1 a go:testimony, go:event ;
    go:hasagent ex:leaderofgalut ;
    go:hastheme evt:oppositionisfalse .

# the proposition testified about
	evt:oppositionisfalse a go:proposition ;
    go:assertsfalsehoodof evt:oppositiontoyourreligion .
	evt:oppositiontoyourreligion a go:opposition ;
    go:opposes ex:yourreligion .
go:event rdfs:label "رخداد"@fa.