@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/292-1> rdf:type owl:Ontology .

ex:event1 a go:Event ;
    go:HasAgent ex:I ;
    go:HasRecipient ex:Abu_al_Hasan ;
    go:HasTheme ex:statement_Hisham .

ex:I a go:Person .
ex:Abu_al_Hasan a go:Person .
ex:Hisham_al_Jawaliqi a go:Person .

ex:statement_Hisham a go:Information ;
    go:HasSource ex:Hisham_al_Jawaliqi ;
    go:describes ex:Successful_Young_Man .

ex:Successful_Young_Man a go:Person .
go:Event rdfs:label "رخداد"@fa.
go:Information rdfs:label "اطلاعات"@fa.
