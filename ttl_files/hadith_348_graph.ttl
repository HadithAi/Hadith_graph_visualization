@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/348-2> rdf:type owl:Ontology .

ex:Spirit a go:Substance ;
    rdfs:label "Spirit" .

ex:RuhName a ex:Name ;
    rdfs:label "Ruh" ;
    ex:isNameOf ex:Spirit ;
    ex:isDerivedFrom ex:Wind .

ex:Wind a go:Substance ;
    rdfs:label "Wind" .

ex:NamingEvent a go:Event ;
    go:HasTheme ex:Spirit ;
    go:HasResult ex:RuhName ;
    go:HasCause ex:Wind .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
