@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/348-3> rdf:type owl:Ontology .

ex:God a go:Deity ;
    rdfs:label "God"@en .

ex:SpiritTerm a ex:Term ;
    rdfs:label "Term 'spirit'"@en .

ex:WindTerm a ex:Term ;
    rdfs:label "Term 'wind'"@en .

ex:Spirit a go:Substance ;
    rdfs:label "Spirit"@en .

ex:Wind a go:Substance ;
    rdfs:label "Wind"@en .

ex:TermDistinctionEvent a go:Event ;
    rdfs:label "Term Distinction Event"@en .

ex:separationEvent a ex:TermDistinctionEvent ;
    go:HasAgent ex:God ;
    go:HasTheme ex:SpiritTerm , ex:WindTerm ;
    go:HasCause ex:Homogeneity .

ex:Homogeneity a go:Relation ;
    rdfs:label "Homogeneity between spirits and wind"@en ;
    go:HasSubject ex:Spirit ;
    go:HasObject ex:Wind .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
