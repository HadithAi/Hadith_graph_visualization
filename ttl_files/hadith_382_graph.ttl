ex:event1 a go:Event, ex:ExemptionEvent ;
    go:HasAgent ex:God_Almighty ;
    go:HasTheme ex:Muhammad ;
    ex:ExceptFor ex:Inevitable .

ex:God_Almighty a go:God ;
    go:label "God Almighty"@en .

ex:Muhammad a go:Person ;
    go:label "Muhammad, peace be upon him and his family"@en .

