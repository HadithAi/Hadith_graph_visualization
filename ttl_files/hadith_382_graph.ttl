@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/382-1> rdf:type owl:Ontology .

ex:event1 a go:CommunicationEvent ;
  go:HasAgent ex:God_Almighty ;
  go:HasRecipient ex:Muhammad ;
  go:HasTheme ex:Information_since_world_existence .

ex:God_Almighty a go:Substance ;
  rdfs:label "God Almighty" .

ex:Muhammad a go:Substance ;
  rdfs:label "Muhammad (peace be upon him and his family)" .

ex:Information_since_world_existence a go:InformationContent ;
  rdfs:label "What has been since the existence of the world" .
go:Information rdfs:label "اطلاعات"@fa.
go:Substance rdfs:label "شی"@fa.
