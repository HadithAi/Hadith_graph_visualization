@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/296-4> rdf:type owl:Ontology .

ex:WritingEvent1 a go:WritingEvent ;
    go:HasAgent ex:AbuAlHasan_AlayhisSalam ;
    go:HasTheme ex:DivineStatement .

ex:AbuAlHasan_AlayhisSalam a go:Person ;
    rdfs:label "Abu al-Hasan (peace be upon him)"@en .

ex:DivineStatement a go:Text ;
    go:content "God has always known the things before He created the things."@en .

ex:God a go:Deity ;
    go:agentIn ex:DivineKnowledgeEvent , ex:CreationEvent .

ex:DivineKnowledgeEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Things ;
    go:temporalExtent "Before creation" .

ex:CreationEvent a go:CreationEvent ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Things .

ex:Things a go:Substance ;
    rdfs:label "Things" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
