<http://etratnet.ir/ontology/430-6> rdf:type owl:Ontology .

ex:QuotationAbuAbdullah a go:StatementEvent ;
    go:HasAgent ex:AbuAbdullah ;
    go:HasTheme ex:GoodIntentEvent ;
    go:hasText "When God intends good for a servant, the servant hears a kindness only if he recognizes it." .

ex:AbuAbdullah a go:Person .
ex:God a go:Deity .
ex:Servant a go:Person .
ex:Kindness a go:AbstractConcept .

ex:GoodIntentEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Servant ;
    go:Causes ex:PerceptionKindnessEvent .

ex:PerceptionKindnessEvent a go:Event ;
    go:HasAgent ex:Servant ;
    go:HasTheme ex:Kindness .
