<http://etratnet.ir/graph/ontology/430> rdf:type go:ontology ;
        go:hascontent "1.  امام صادق عليه‌السلام فرمود: «اى ثابت، شما را با مردم چه كار؟ از مردم دست بداريد، و كسى را به عقيده خود دعوت نكنيد؛ زيرا، به خدا سوگند، اگر همه آسمانيان و زمينيان دست به دست هم دهند تا بنده‌اى را كه خدا گمراهى او را خواسته است هدايت كنند، نتوانند هدايتش كنند؛ و اگر همه آسمانيان و زمينيان دست به دست هم دهند تا بنده‌اى را كه خدا هدايت او را خواسته است گمراه كنند نتوانند گمراهش كنند. از مردم دست بداريد، و كسى نگويد: عمويم، برادرم، پسر عمويم، و همسايه‌ام. چرا كه خداوند هرگاه براى بنده‌اى خيرى بخواهد، روان او را پاك گرداند چنان كه هر معروفى را بشنود آن را بپذيرد و هر منكرى را بشنود آن را رد كند. سپس خداوند در دل او كلمه‌اى [و عقيده‌اى]را بيفكند كه با آن، كار او را سامان بخشد»."@fa,
        "1.  قَالَ أَبُو عَبْدِ اللّه‌ِ عليه‌السلام: «يَا ثَابِتُ، مَا لَكُمْ وَلِلنَّاسِ، كُفُّوا عَنِ النَّاسِ، وَلاَ تَدْعُوا أَحَداً إِلى أَمْرِكُمْ؛ فَوَ اللّه‌ِ، لَوْ أَنَّ أَهْلَ السَّمَاوَاتِ وَأَهْلَ الْأَرَضِينَ اجْتَمَعُوا عَلى أَنْ يَهْدُوا عَبْداً يُرِيدُ اللّه‌ُ ضَلاَلَتَهُ، مَا اسْتَطَاعُوا عَلى أَنْ يَهْدُوهُ؛ وَلَوْ أَنَّ أَهْلَ السَّمَاوَاتِ وَأَهْلَ الْأَرَضِينَ اجْتَمَعُوا عَلى أَنْ يُضِلُّوا عَبْداً يُرِيدُ اللّه‌ُ هِدَايَتَهُ، مَا اسْتَطَاعُوا أَنْ يُضِلُّوهُ، كُفُّوا عَنِ النَّاسِ، وَلاَ يَقُولُ أَحَدٌ: عَمِّي وَأَخِي وَابْنُ عَمِّي وَجَارِي؛ فَإِنَّ اللّه‌َ إِذَا أَرَادَ بِعَبْدٍ خَيْراً، طَيَّبَ رُوحَهُ، فَلاَ يَسْمَعُ مَعْرُوفاً إِلاَّ عَرَفَهُ، وَلاَ مُنْكَراً إِلاَّ أَنْكَرَهُ، ثُمَّ يَقْذِفُ اللّه‌ُ فِي قَلْبِهِ كَلِمَةً يَجْمَعُ بِهَا أَمْرَهُ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/430-1> rdf:type owl:ontology .

ex:imam_abu_abdullah a go:agent, go:substance ;
    rdfs:label "imam abu abdullah" .

ex:thabit a go:agent, go:substance ;
    rdfs:label "thabit" .

ex:people a go:group, go:substance ;
    rdfs:label "people" .

ex:callevent_1 a go:event ;
    go:hasagent ex:imam_abu_abdullah ;
    go:hasrecipient ex:thabit ;
    go:hastheme ex:action_stop ;
    go:hastheme ex:action_notinvite .

ex:action_stop a go:action ;
    rdfs:label "stop actions towards people" ;
    go:hastheme ex:people .

ex:action_notinvite a go:action ;
    rdfs:label "do not invite anyone to his matter" .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/430-2> rdf:type owl:ontology .

ex:abu_abdullah a go:person ;
    rdfs:label "abu abdullah" ;
    go:ishonoredby "peace be upon him" .

ex:god a go:deity ;
    rdfs:label "god" .

ex:people_heavens a go:group ;
    go:haslabel "people of the heavens" .

ex:people_earth a go:group ;
    go:haslabel "people of the earth" .

ex:servant a go:person ;
    go:haslabel "servant" .

# event: abu abdullah speaking
ex:speakingevent_1 a go:event ;
    go:hasagent ex:abu_abdullah ;
    go:hastheme ex:statement_1 .

# the statement content
ex:statement_1 a go:statement ;
    go:describesevent ex:hypothetical_union_guidance .

# hypothetical event: people of heavens and earth unite and attempt to guide the servant
ex:hypothetical_union_guidance a go:hypotheticalevent ;
    go:hasagent ex:people_heavens , ex:people_earth ;
    go:hastheme ex:guidance_attempt .

ex:guidance_attempt a go:event ;
    go:hasagent ex:people_heavens , ex:people_earth ;
    go:hastheme ex:servant ;
    go:result ex:failed_guidance .

ex:failed_guidance a go:negativeresult ;
    go:description "failure to guide the servant despite joint effort" .

# god's intention for servant
ex:godintends_misguidance a go:intention ;
    go:hasagent ex:god ;
    go:hastheme ex:servant ;
    go:hasintention "misguidance" .

ex:hypothetical_union_guidance go:condition ex:godintends_misguidance .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/430-3> rdf:type owl:ontology .

ex:speech_abuabdullah a go:speechact ;
    go:hasagent ex:abuabdullah ;
    go:hastheme ex:hypotheticalevent_gathermisguide .

ex:abuabdullah a go:person .
ex:god a go:person .
ex:servant a go:person .
ex:peopleofheavens a go:group ; go:hasdomain "heavens"^^xsd:string .
ex:peopleofearth a go:group ; go:hasdomain "earth"^^xsd:string .

ex:hypotheticalevent_gathermisguide a go:event ;
    go:hasagent ex:peopleofheavens, ex:peopleofearth ;
    go:hastheme ex:servant ;
    ex:hasintendedaction ex:misguideservant ;
    ex:iscounteredby ex:godsguidance ;
    ex:hasoutcome "could not misguide"^^xsd:string .

ex:misguideservant a go:event ;
    go:hasagent ex:peopleofheavens, ex:peopleofearth ;
    go:hastheme ex:servant .

ex:godsguidance a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:servant ;
    ex:wills "guidance"^^xsd:string .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/430-4> rdf:type owl:ontology .

ex:abuabdullah rdf:type go:person ;
    rdfs:label "abu abdullah" .

ex:people rdf:type go:group ;
    rdfs:label "people" .

ex:orderevent1 rdf:type go:orderevent ;
    go:hasagent ex:abuabdullah ;
    go:hastheme ex:people ;
    go:hascontent ex:prohibitiononcalling .

ex:prohibitiononcalling rdf:type go:prohibition ;
    go:relatedtoterms ex:termlist .

ex:termlist rdf:type go:list ;
    go:hasitem ex:termuncle , ex:termbrother , ex:termcousin , ex:termneighbor .

ex:termuncle rdf:type go:expression ;
    go:haslabel "uncle" .

ex:termbrother rdf:type go:expression ;
    go:haslabel "brother" .

ex:termcousin rdf:type go:expression ;
    go:haslabel "cousin" .

ex:termneighbor rdf:type go:expression ;
    go:haslabel "neighbor" .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/430-5> rdf:type owl:ontology .

# entities
ex:abuabdullah a go:person ;
    rdfs:label "abu abdullah (عليه السلام)" .

ex:god a go:deity ;
    rdfs:label "الله (god)" .

ex:servant a go:person ;
    go:hasrole "servant" .

ex:servantsoul a go:substance ;
    go:ispartof ex:servant ;
    rdfs:label "soul" .

# events
ex:sayingevent a go:speechevent ;
    go:hasagent ex:abuabdullah ;
    go:hastheme ex:sayingcontent .

ex:sayingcontent a go:proposition ;
    go:expresses ex:goodintentionevent , ex:purificationevent ;
    rdf:type go:statement .

ex:goodintentionevent a go:intentevent ;
    go:hasagent ex:god ;
    go:hastheme ex:servant ;
    go:resultsin ex:purificationevent .

ex:purificationevent a go:purificationevent ;
    go:hasagent ex:god ;
    go:hastheme ex:servantsoul .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/430-6> rdf:type owl:ontology .

ex:quotationabuabdullah a go:statementevent ;
    go:hasagent ex:abuabdullah ;
    go:hastheme ex:goodintentevent ;
    go:hastext "when god intends good for a servant, the servant hears a kindness only if he recognizes it." .

ex:abuabdullah a go:person .
ex:god a go:deity .
ex:servant a go:person .
ex:kindness a go:abstractconcept .

ex:goodintentevent a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:servant ;
    go:causes ex:perceptionkindnessevent .

ex:perceptionkindnessevent a go:event ;
    go:hasagent ex:servant ;
    go:hastheme ex:kindness .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/430-7> rdf:type owl:ontology .

ex:utterance1 a go:event ;
  go:hasagent ex:abu_abdullah ;
  go:hastheme ex:truthstatement1 .

ex:truthstatement1 a go:informationcontent ;
  go:refersto ex:servantbehavior .

ex:servantbehavior a go:state ;
  go:hasagent ex:servant ;
  go:hascause ex:god ;
  go:hasproperty ex:rejectwrongdoing .

ex:rejectwrongdoing a go:event ;
  go:hasagent ex:servant ;
  go:hastheme ex:wrongdoing .

ex:hearingevent a go:event ;
  go:hasagent ex:servant ;
  go:hastheme ex:wrongdoing ;
  go:precedes ex:rejectwrongdoing .

ex:wrongdoing a go:substance .
ex:servant a go:person .
ex:abu_abdullah a go:person .
ex:god a go:deity .
go:event rdfs:label "رخداد"@fa.
go:information rdfs:label "اطلاعات"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/430-8> rdf:type owl:ontology .

ex:abu_abdullah a go:person ;
    go:uttered ex:statement_1 .

ex:statement_1 a go:speechact ;
    go:hasspeaker ex:abu_abdullah ;
    go:content ex:event_1 .

ex:god a go:divinebeing .

ex:servant a go:person ;
    go:haspart ex:servant_heart .

ex:servant_heart a go:anatomicalpart .

ex:word a go:concept ;
    go:hasattribute ex:gathers_affairs .

ex:gathers_affairs a go:attribute .

ex:event_1 a go:event ;
    go:type "plantingword" ;
    go:hasagent ex:god ;
    go:hastheme ex:word ;
    go:hasrecipient ex:servant_heart .
go:event rdfs:label "رخداد"@fa.