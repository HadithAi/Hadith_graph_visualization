@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/332-2> rdf:type owl:Ontology .

ex:event1 a go:DescriptionEvent ;
    go:HasAgent ex:God ;
    go:HasTheme ex:GodSelf ;
    go:describesAttribute ex:AttributeOne, ex:AttributeSelfManifested, ex:AttributeSeparate .

ex:God a go:Deity .
ex:GodSelf a go:DivineSelf .
ex:AttributeOne a go:DivineAttribute ;
    go:hasValue "One" .
ex:AttributeSelfManifested a go:DivineAttribute ;
    go:hasValue "Self-Manifested" .
ex:AttributeSeparate a go:DivineAttribute ;
    go:hasValue "Separate from Creation" ;
    go:relatedTo go:Creation .

go:Creation a go:Creation .
