@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/376-4> rdf:type owl:Ontology .

ex:TheSecondKnowledge a go:Knowledge ;
    go:label "the second knowledge" .

ex:Allah a go:DivineBeing ;
    go:label "Allah" .

ex:Angels a go:SpiritualBeings ;
    go:label "angels" .

ex:Messengers a go:SpiritualBeings ;
    go:label "messengers" .

ex:Prophets a go:SpiritualBeings ;
    go:label "prophets" .

ex:TeachingEvent a go:TeachingEvent ;
    go:hasAgent ex:Allah ;
    go:hasTheme ex:TheSecondKnowledge ;
    go:hasRecipient ex:Angels , ex:Messengers , ex:Prophets .
@prefix : <http://etratnet.ir/graph/> .
@prefix ex: <http://etratnet.ir/graph/proposition/376#> .
@prefix bt: <http://borhan-onto.ir/ontology/time#> .
@prefix go: <http://etratnet.ir/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
