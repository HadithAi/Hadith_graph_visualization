@prefix ex: <http://etratnet.ir/graph/example/376#> .
    @prefix bt: <http://borhan-onto.ir/ontology/time#> .
    @prefix go: <http://etratnet.ir/ontology#> .
    @prefix owl: <http://www.w3.org/2002/07/owl#> .
    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix xml: <http://www.w3.org/xml/1998/namespace> .
    @prefix xsd: <http://www.w3.org/2001/xmlschema#> .
    @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
    @prefix time: <http://www.w3.org/2006/time#> .
    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/376> rdf:type go:ontology ;
        go:hascontent "9.  «خداوند را دو علم است: علمى نهفته و اندوخته كه جز خودش كسى آن را نمى‌داند ـ و بداء از اين نوع علم است ـ، و علمى كه آن را به فرشتگان و فرستادگان و پيامبرانش آموخت؛ بنابراين، ما نيز آن را مى‌دانيم»."@fa,
        "9.  عَنْ أَبِي عَبْدِ اللّه‌ِ عليه‌السلام، قَالَ: «إِنَّ لِلّهِ عِلْمَيْنِ: عِلْمٌ مَكْنُونٌ مَخْزُونٌ لاَ يَعْلَمُهُ إِلاَّ هُوَ، مِنْ ذلِكَ يَكُونُ الْبَدَاءُ؛ وَعِلْمٌ عَلَّمَهُ مَلاَئِكَتَهُ وَرُسُلَهُ وَأَنْبِيَاءَهُ، فَنَحْنُ نَعْلَمُهُ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/376-1> rdf:type owl:ontology .

ex:god a go:deity .

ex:science1 a go:science .
ex:science2 a go:science .

ex:god go:hastheme ex:science1 .
ex:god go:hastheme ex:science2 .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/376-2> rdf:type owl:ontology .

ex:firstknowledge rdf:type go:substance ;
    rdfs:label "the first knowledge"@en ;
    rdfs:comment "hidden and stored knowledge"@en .

ex:god rdf:type go:agent ;
    rdfs:label "god"@en .

ex:knowingevent1 rdf:type go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:firstknowledge ;
    rdfs:comment "an event of knowing that only god participates as agent"@en .

# to express exclusivity, a note as comment or a property stating no other agent knows this knowledge can be added, but since standard ontologies don't usually encode exclusivity in triples directly, a comment is sufficient here.

ex:firstknowledge ex:knownonlyby ex:god .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/376-3> rdf:type owl:ontology .

ex:god a go:agent ;
    go:hasknowledge ex:hiddenknowledge .

ex:hiddenknowledge a go:substance ;
    go:hasattribute "hidden" , "stored" .

ex:beginningevent a go:event ;
    go:hascause ex:hiddenknowledge ;
    go:hasagent ex:god .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/376-4> rdf:type owl:ontology .

ex:thesecondknowledge a go:knowledge ;
    go:label "the second knowledge" .

ex:allah a go:divinebeing ;
    go:label "allah" .

ex:angels a go:spiritualbeings ;
    go:label "angels" .

ex:messengers a go:spiritualbeings ;
    go:label "messengers" .

ex:prophets a go:spiritualbeings ;
    go:label "prophets" .

ex:teachingevent a go:teachingevent ;
    go:hasagent ex:allah ;
    go:hastheme ex:thesecondknowledge ;
    go:hasrecipient ex:angels , ex:messengers , ex:prophets .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/376-5> rdf:type owl:ontology .

ex:god a go:agent, ex:divinebeing .
ex:angels a go:recipientgroup .
ex:messengers a go:recipientgroup .
ex:prophets a go:recipientgroup .
ex:secondknowledge a go:knowledge .

ex:teachingevent a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:secondknowledge ;
    go:hasrecipient ex:angels , ex:messengers , ex:prophets .

ex:we a go:agent, ex:groupofpeople .

ex:knowingevent a go:event ;
    go:hasagent ex:we ;
    go:hastheme ex:secondknowledge .
go:event rdfs:label "رخداد"@fa.
go:agent rdfs:label "عامل"@fa.