ex:sonofadam a go:human ;
    go:substancecategory go:agent .

ex:saying1 a go:speechact ;
    go:hasagent ex:god ;
    go:hastheme ex:sonofadam ;
    go:hascontent ex:content1 .

ex:content1 a go:statement ;
    go:hasproposition ex:proposition1 .

ex:proposition1 a go:proposition ;
    go:hassubject ex:god ;
    go:haspredicate ex:will ;
    go:hasobject ex:desirerelation .

ex:desirerelation a go:relation ;
    go:hasagent ex:sonofadam ;
    go:hastheme ex:desiredthings .

ex:desiredthings a go:concept ;
    go:description "whatever you desire for yourself" .
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.


