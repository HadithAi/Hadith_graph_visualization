<http://etratnet.ir/ontology/275-20> rdf:type owl:Ontology .

ex:eventLook a go:PerceptionEvent ;
    go:HasAgent ex:Muhammad ;
    go:HasTheme ex:Lord ;
    go:HasManner ex:Heart .

ex:eventLight a go:Event ;
    go:HasAgent ex:Lord ;
    go:HasTheme ex:Muhammad ;
    go:resultsIn ex:LightInVeils .

ex:LightInVeils a go:Substance ;
    rdfs:label "Light like light of veils" .

ex:eventLight go:HasCause ex:eventLook .

ex:Muhammad a go:Person ;
    rdfs:label "Muhammad" .

ex:Lord a go:Deity ;
    rdfs:label "Lord" .

ex:Heart a go:AnatomicalStructure ;
    rdfs:label "Heart" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
