    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/228> rdf:type go:ontology ;
        go:hascontent "7.  از امام باقر عليه‌السلام سؤال شد: آيا اطلاق «چيز» بر خدا رواست؟ فرمود: «آرى، اين كار او را از دو وضعيتِ تعطيل (توقف در شناخت خدا) و تشبيه (مانند كردن او به مخلوق) بيرون مى‌بَرَد»."@fa,
        "7. سُئِلَ أَبُو جَعْفَرٍ عليه‌السلام: أَيَجُوزُ أَنْ يُقَالَ: إِنَّ اللّه‌َ شَيْءٌ؟ قَالَ: «نَعَمْ، يُخْرِجُهُ مِنَ الْحَدَّيْنِ: حَدِّ التَّعْطِيلِ، وَحَدِّ التَّشْبِيهِ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/228-1> rdf:type owl:ontology .

ex:event1 a go:event ;
    go:hasagent ex:speaker .

ex:speaker a go:agent .

ex:god a go:substance ;
    rdfs:label "god" .

ex:thing a go:substance ;
    rdfs:label "thing" .

ex:proposition1 a go:proposition ;
    go:subject ex:god ;
    go:predicate ex:isa ;
    go:object ex:thing .

ex:event1 go:hastheme ex:proposition1 ;
    go:modalproperty ex:permissible .

ex:isa a go:relation .

ex:permissible a go:modalproperty .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/228-2> rdf:type owl:ontology .

ex:event_elimination a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:extremenegation, ex:extremelikening .

ex:god a go:agent .

ex:extremenegation a go:concept ;
    rdfs:label "the extreme of negation"@en .

ex:extremelikening a go:concept ;
    rdfs:label "the extreme of likening"@en .
go:event rdfs:label "رخداد"@fa.
go:agent rdfs:label "عامل"@fa.