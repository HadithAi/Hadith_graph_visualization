    @base <http://etratnet.ir/ontology/> .
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/228-1> rdf:type owl:Ontology .

ex:event1 a go:Event ;
    go:hasAgent ex:Speaker .

ex:Speaker a go:Agent .

ex:God a go:Substance ;
    rdfs:label "God" .

ex:Thing a go:Substance ;
    rdfs:label "thing" .

ex:Proposition1 a go:Proposition ;
    go:subject ex:God ;
    go:predicate ex:IsA ;
    go:object ex:Thing .

ex:event1 go:hasTheme ex:Proposition1 ;
    go:modalProperty ex:Permissible .

ex:IsA a go:Relation .

ex:Permissible a go:ModalProperty .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/228-2> rdf:type owl:Ontology .

ex:event_elimination a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:ExtremeNegation, ex:ExtremeLikening .

ex:God a go:Agent .

ex:ExtremeNegation a go:Concept ;
    rdfs:label "the extreme of negation"@en .

ex:ExtremeLikening a go:Concept ;
    rdfs:label "the extreme of likening"@en .
go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.