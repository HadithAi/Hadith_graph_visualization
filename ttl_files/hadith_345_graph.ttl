ex:SayEvent1 a go:Event , go:SayingEvent ;
    go:HasAgent ex:People ;
    go:HasTheme ex:Content1 .

ex:Content1 a go:Content ;
    go:statesPosition ex:Position1 , ex:Position2 .

ex:Position1 a go:SpatialRelation ;
