<http://etratnet.ir/ontology/345-5> rdf:type owl:Ontology .

ex:ClaimingEvent a go:Event ;
  go:HasAgent ex:Claimant ;
  go:HasTheme ex:Claim .

ex:DescribingEvent a go:Event ;
  go:HasAgent ex:Claimant ;
  go:HasTheme ex:GodDescription .

ex:Claimant a go:Person .

ex:God a go:Thing ;
  rdf:type ex:God .

ex:CreatedAttribute a go:Attribute ;
  rdfs:label "attribute of a created being"@en .

ex:GodDescription a go:Description ;
  go:describes ex:God ;
  go:hasAttribute ex:CreatedAttribute .

ex:Claim a go:Claim .

# Causal relation: Claiming leads to Describing
ex:ClaimingEvent go:causes ex:DescribingEvent .
