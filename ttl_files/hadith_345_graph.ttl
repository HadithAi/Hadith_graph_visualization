ex:sayEvent1 a go:SpeechAct, ex:EventInstance ;
    go:HasAgent ex:God_Almighty ;
    go:HasTheme ex:Statement1 .

ex:God_Almighty a go:Deity ;
    rdfs:label "الله عز وجل" .

ex:Statement1 a go:Proposition ;
    go:hasContent "وَ كَانَ عَرْشُهُ عَلَى الْمَآءِ" ;
    go:refersTo ex:ThroneOverWater .

ex:ThroneOverWater a go:State ;
    go:hasSubject ex:His_Throne ;
    go:hasLocation ex:Water .

ex:His_Throne a go:Object ;
    go:hasPossessor ex:God_Almighty ;
    go:label "عرشه" .

ex:Water a go:Substance ;
    go:label "الماء" .
