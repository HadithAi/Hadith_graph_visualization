@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/345-18> rdf:type owl:Ontology .

ex:angels rdf:type go:agent ;
    rdfs:label "the angels" .

ex:speechEvent1 rdf:type go:communicationEvent ;
    go:HasAgent ex:angels ;
    go:HasTheme ex:content1 .

ex:content1 rdf:type go:statement ;
    rdfs:label "We bear witness that they will not say their fathers were polytheists and we are their descendants; should we be destroyed for others' deeds?" .

ex:ancestors rdf:type go:group ;
    rdfs:label "our fathers" .

ex:descendants rdf:type go:group ;
    rdfs:label "our descendants" .
@prefix : <http://etratnet.ir/graph/> .
@prefix ex: <http://etratnet.ir/graph/proposition/345#> .
@prefix bt: <http://borhan-onto.ir/ontology/time#> .
@prefix go: <http://etratnet.ir/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/345-19> rdf:type owl:Ontology .

ex:SpeechEvent1 a go:SpeechEvent ;
    go:hasAgent ex:Some ;
    go:hasTheme ex:Question1 .

ex:Some a go:Agent .

ex:Question1 a go:Proposition ;
    go:hasContent ex:DestructionEvent1 .

ex:DestructionEvent1 a go:Event ;
    go:hasAgent ex:CorruptAgents .

ex:CorruptAgents a go:Agent .
go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.
@prefix : <http://etratnet.ir/graph/> .
@prefix ex: <http://etratnet.ir/graph/proposition/345#> .
@prefix bt: <http://borhan-onto.ir/ontology/time#> .
@prefix go: <http://etratnet.ir/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/345-20> rdf:type owl:Ontology .

ex:event1 a go:Event ;
    go:HasAgent ex:God ;
    go:HasRecipient ex:David ;
    go:HasTheme ex:Statement1 .

ex:God a go:Substance ;
    go:hasLabel "God" .

ex:David a go:Substance ;
    go:hasLabel "David" .

ex:Statement1 a go:concept ;
    go:hasLabel "Punishment in the covenant with them will not come upon us hastily" ;
    go:relatedTo ex:Punishment , ex:Covenant .

ex:Punishment a go:concept ;
    go:hasLabel "Punishment" .

ex:Covenant a go:concept ;
    go:hasLabel "Covenant" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.