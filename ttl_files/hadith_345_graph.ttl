ex:Water a go:Substance .
ex:Lord a go:Agent .

ex:ClaimEvent a go:Claim ;
    go:HasAgent ex:Claimant ;
    go:HasTheme ex:ClaimedProposition .

ex:ClaimedProposition a go:Proposition ;
    ex:assertsRelation [ ex:subject ex:Throne ; ex:relation ex:isOn ; ex:object ex:Water ] ,
                        [ ex:subject ex:Lord ; ex:relation ex:isAbove ; ex:object ex:Throne ] .

ex:LieEvent a go:Lie ;
    go:HasAgent ex:Claimant ;
    go:Refutes ex:ClaimedProposition .
