@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/345-6> rdf:type owl:Ontology .

# Entities
ex:God a go:Substance ;
    rdfs:label "God"@en .

ex:ThroneBearer a go:Substance ;
    rdfs:label "Thing that carries the throne of God"@en .

# Events
ex:ClaimEvent a go:Event ;
    go:HasAgent ex:Claimant ;
    go:HasTheme ex:ClaimContent .

ex:AcceptClaimEvent a go:Event ;
    go:HasAgent ex:Claimant ;
    go:HasTheme ex:ClaimContent .

# Claim content as a statement node
ex:ClaimContent a go:statement ;
    ex:strongerThanRel ex:God , ex:ThroneBearer .

# Relations
ex:strongerThanRel a rdf:Property ;
    rdfs:comment "Relates a thing being stronger than another"@en .

# Structured claim content to state ThroneBearer is stronger than God
ex:ClaimContent ex:strongerThan ex:God ;
                ex:subject ex:ThroneBearer .

# Agent
ex:Claimant a go:agent ;
    rdfs:label "Whoever claimed that"@en .

# Necessary acceptance causally implied by the claim
ex:ClaimEvent go:causes ex:AcceptClaimEvent .
go:Event rdfs:label "رخداد"@fa.
