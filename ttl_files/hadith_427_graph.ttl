@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/427-1> rdf:type owl:ontology .

ex:event1 a ex:askevent ;
    go:hasagent ex:person1 ;
    go:hasrecipient ex:abuabdullah_pbuh ;
    go:hastheme ex:question1 .

ex:person1 a go:person .
ex:abuabdullah_pbuh a go:person .

ex:question1 a go:concept ;
    go:hascontent "is there any benefit for someone who does not know anything?"^^xsd:string .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/427-2> rdf:type owl:ontology .

ex:abu_abdullah a go:person ;
    rdfs:label "abu abdullah" ;
    go:honorific "peace be upon him" .

event:response_1 a go:responseevent ;
    go:hasagent ex:abu_abdullah ;
    go:hastheme "no" .