ex:event_enter a go:event ;
    go:hastheme ex:thing ;
    go:hasdestination ex:place .

ex:event_inform a go:event ;
    go:hasagent ex:thing ;
    go:hastheme ex:ex:corruption ;
    go:hasrecipient ex:place .

# statement that no corrupt thing enters the place to inform about corruption

# this can be represented as a negative statement or a rule outside rdf; here we will annotate the absence:

ex:place  go:forbidsentry [
    a go:substancerestriction ;
    go:restrictedentity ex:thing ;
    go:restrictioncondition [
        a go:condition ;
        go:property go:iscorrupt ;
        go:hasvalue true
    ]
] .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


