    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/343> rdf:type go:ontology ;
        go:hascontent "5.  از امام صادق عليه‌السلام درباره سخن خداى عز و جل كه مى‌فرمايد: «وَسِعَ كُرْسِيُّهُ السَّمَوَ تِ وَالْأَرْضَ» پرسيدم [و گفتم]: آيا آسمان‌ها و زمين كرسى را در برگرفته‌اند يا كرسى آسمان‌ها و زمين را؟ فرمود: «كرسى همه چيز را در برگرفته است»."@fa,
        "5.  سَأَلْتُ أَبَا عَبْدِ اللّه‌ِ عليه‌السلام عَنْ قَوْلِ اللّه‌ِ عَزَّ وَجَلَّ: «وَسِعَ كُرْسِيُّهُ السَّمَوَ تِ وَالْأَرْضَ»: السَّمَاوَاتُ وَالْأَرْضُ وَسِعْنَ الْكُرْسِيَّ، أَوِ الْكُرْسِيُّ وَسِعَ السَّمَاوَاتِ وَالْأَرْضَ؟ فَقَالَ: «إِنَّ كُلَّ شَيْءٍ فِي الْكُرْسِيِّ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/343-1> rdf:type owl:ontology .

ex:godalmighty a go:deity ;
    rdfs:label "god almighty" .

ex:histhrone a go:artifact ;
    rdfs:label "god's throne" .

ex:heavens a go:celestialbody ;
    rdfs:label "heavens" .

ex:earth a go:celestialbody ;
    rdfs:label "earth" .

ex:speechevent1 a go:utterance ;
    rdfs:label "god almighty's statement about his throne" ;
    go:hasagent ex:godalmighty ;
    go:hastheme ex:utterancecontent1 .

ex:utterancecontent1 a go:conceptualentity ;
    rdfs:label "his throne extends over the heavens and the earth" .

ex:throneextension a go:spatialrelation ;
    go:hasagent ex:histhrone ;
    go:hastheme ex:heavens ;
    go:hastheme ex:earth ;
    rdfs:comment "the throne extends over the heavens and the earth" .

ex:histhrone go:extendsover ex:heavens, ex:earth .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/343-2> rdf:type owl:ontology .

ex:heavens a go:physicalentity ;
    rdfs:label "heavens" .

ex:earth a go:physicalentity ;
    rdfs:label "earth" .

ex:throne a go:object ;
    rdfs:label "throne" .

ex:encompassevent a go:event ;
    go:hasagent ex:heavens , ex:earth ;
    go:hastheme ex:throne ;
    rdfs:label "encompassment of throne by heavens and earth" .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/343-3> rdf:type owl:ontology .

ex:expansionevent1 a go:event ;
    go:hasagent ex:throne ;
    go:hastheme ex:heavens , ex:earth .

ex:throne a go:substance .
ex:heavens a go:substance .
ex:earth a go:substance .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/343-4> rdf:type owl:ontology .

ex:everything a go:entity .
ex:chair a go:physicalobject .

ex:everything go:islocatedon ex:chair .