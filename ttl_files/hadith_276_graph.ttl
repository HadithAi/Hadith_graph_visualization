<http://etratnet.ir/graph/ontology/276> rdf:type go:ontology ;
        go:hascontent "4.  «اگر آسمانيان و زمينيان [همه] گرد آيند تا عظمت خدا را بيان كنند، نتوانند»."@fa,
        "4.  عَنْ عَلِيِّ بْنِ الْحُسَيْنِ عليهماالسلام، قَالَ: قَالَ: «لَوِ اجْتَمَعَ أَهْلُ السَّمَاءِ وَالْأَرْضِ أَنْ يَصِفُوا اللّه‌َ بِعَظَمَتِهِ، لَمْ يَقْدِرُوا»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/276-1> rdf:type owl:ontology .

ex:god a go:deity ;
    go:hasquality "great" .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/276-2> rdf:type owl:ontology .

ex:inhabitantsheavens a go:substance ;
    go:label "inhabitants of the heavens" .

ex:inhabitantsearth a go:substance ;
    go:label "inhabitants of the earth" .

ex:god a go:deity ;
    go:label "god" ;
    go:hasattribute ex:greatness .

ex:greatness a go:attribute ;
    go:label "greatness" .

ex:describingevent a go:event, ex:hypotheticalevent ;
    go:hasagent ex:inhabitantsheavens, ex:inhabitantsearth ;
    go:hastheme ex:god ;
    go:description "describing god with his greatness" ;
    ex:canbecompleted "false"^^xsd:boolean .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.