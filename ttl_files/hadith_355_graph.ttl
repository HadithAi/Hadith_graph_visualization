    go:HasAgent ex:Who ;
    go:HasTheme ex:God .

ex:DefineGodEvent a go:Event ;
    go:HasTheme ex:God .

# The causal relation: describing God causes defining God
ex:DescribeGodEvent go:Causes ex:DefineGodEvent .
