    go:hasParts ex:Vision, ex:CommonAndEliteNarration .

ex:Vision a go:concept ;
    rdfs:label "Vision" .

ex:CommonAndEliteNarration a go:Event ;
    go:HasAgent ex:CommonPeople, ex:ElitePeople ;
    go:HasTheme ex:NarrationContent .

ex:CommonPeople a go:group ;
    rdfs:label "common people" .

ex:ElitePeople a go:group ;
    rdfs:label "elite people" .

