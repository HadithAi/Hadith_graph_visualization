    @base <http://etratnet.ir/ontology/> .
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/414-1> rdf:type owl:Ontology .

ex:God a go:Agent , go:Deity ;
    go:hasProperty ex:Generosity .

ex:People a go:Person .

ex:BurdeningEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:ImpossibleBurden ;
    go:HasRecipient ex:People ;
    go:Status ex:NotPerformed .

ex:ImpossibleBurden a go:Burden ;
    go:CanBeFinishedBy ex:People ;
    go:Possibility go:Impossible .

ex:Generosity a go:Property ;
    rdfs:label "Generosity"@en .

ex:NotPerformed a go:Status ;
    rdfs:label "Not Performed"@en .

go:Impossible a go:Possibility ;
    rdfs:label "Impossible"@en .
go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/414-2> rdf:type owl:Ontology .

ex:God a ex:DivineEntity ;
    ex:hasAttribute ex:Might .

ex:Might a go:Quality ;
    go:HasSubject ex:God .

ex:Dominion a go:Region ;
    go:ControlledBy ex:God .

ex:GodWillingState a go:State ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Dominion ;
    ex:excludesUndesiredContent true .
go:Quality rdfs:label "ویژگی"@fa.