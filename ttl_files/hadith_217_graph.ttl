@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/217-71> rdf:type owl:Ontology .

# Entity: The scholar
ex:Scholar_AlayhisSalaam a go:Person ;
    go:hasTitle "Alayhis Salaam" .

# Event: The scholar says something
ex:SpeechEvent1 a go:CommunicationEvent ;
    go:hasAgent ex:Scholar_AlayhisSalaam ;
    go:hasTheme ex:SpeechContent1 .

# Content of the speech - complex proposition
ex:SpeechContent1 a go:Statement ;
    go:expressesCondition ex:ConditionalProposition1 .

# The conditional proposition: If the matter is as we say
ex:ConditionalProposition1 a go:ConditionalStatement ;
    go:condition ex:MatterAsWeSay ;
    go:consequence ex:ConsequenceEvent1 .

# The condition: the matter is as we say
ex:MatterAsWeSay a go:State ;
    go:description "The matter is as we say" .

# The consequence event: we are saved and destruction occurs
ex:ConsequenceEvent1 a go:CompositeEvent ;
    go:hasPart ex:SalvationEvent , ex:DestructionEvent .

ex:SalvationEvent a go:State ;
    go:affects ex:We ;
    go:description "Our salvation" .

ex:DestructionEvent a go:Event ;
    go:description "Destruction occurs" .

ex:We a go:Group .
go:Event rdfs:label "رخداد"@fa.
