ex:CreationEvent1 a ex:eventInstance , go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:CreatedThings ;
    go:HasPurpose ex:ManifestWisdomAndLordship .

ex:God a ex:DivineEntity ;
    go:HasQuality ex:Wisdom ;
    go:HasQuality ex:Lordship .

ex:CreatedThings a go:Substance .

ex:Wisdom a go:Quality .
ex:Lordship a go:Quality .

ex:ManifestWisdomAndLordship a go:abstractConcept ;
    go:comprises ex:Wisdom , ex:Lordship .
