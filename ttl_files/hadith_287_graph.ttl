    go:isAttributeOf ex:God .

ex:Minds a go:Substance .

ex:ControlEvent a go:ControlEvent ;
    go:HasAgent ex:GodPower ;
    go:HasTheme ex:Minds ;
    ex:doesNotOccur "true"^^<http://www.w3.org/2001/XMLSchema#boolean> .
