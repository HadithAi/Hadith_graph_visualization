@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/287-1> rdf:type owl:Ontology .

ex:God rdf:type go:Substance ;
    go:HasAttribute ex:Power, ex:Wisdom .

ex:Things rdf:type go:Substance .

ex:CreationEvent rdf:type go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Things ;
    go:HasAttribute ex:Creation, ex:Innovation .

ex:Creation rdf:type go:attribute .

ex:Innovation rdf:type go:attribute .

ex:Power rdf:type go:attribute .

ex:Wisdom rdf:type go:attribute .

ex:NoOtherSource rdf:type go:negation ;
    go:Negates ex:CreationEvent .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
