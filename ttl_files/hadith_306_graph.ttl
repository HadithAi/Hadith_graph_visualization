@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/306-2> rdf:type owl:Ontology .

ex:Satisfaction a go:State ;
    rdfs:label "Satisfaction" .

ex:Creature a go:Creature ;
    rdfs:label "Creature" .

ex:TransitionEvent a go:Event ;
    go:HasAgent ex:Satisfaction ;
    go:HasTheme ex:Creature ;
    go:HasCause ex:Satisfaction ;
    go:HasStartState [ a go:State ; rdfs:label "Initial State" ] ;
    go:HasEndState [ a go:State ; rdfs:label "Resulting State" ] .
go:Event rdfs:label "رخداد"@fa.
