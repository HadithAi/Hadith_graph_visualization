@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/306-6> rdf:type owl:Ontology .

ex:Pleasure a go:EmotionalState ;
    rdfs:label "Pleasure"@en .

ex:Displeasure a go:EmotionalState ;
    rdfs:label "Displeasure"@en .

ex:Creator a go:Agent ;
    rdfs:label "Creator"@en .

ex:PleasureOccurrence a go:Event ;
    go:HasTheme ex:Pleasure ;
    go:HasAgent ex:Creator ;
    go:OccursWithoutIntervention _:noEntity .

ex:DispleasureOccurrence a go:Event ;
    go:HasTheme ex:Displeasure ;
    go:HasAgent ex:Creator ;
    go:OccursWithoutIntervention _:noEntity .

_:noEntity a go:Nothing .
go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.
