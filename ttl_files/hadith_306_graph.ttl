@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/306-7> rdf:type owl:Ontology .

ex:ExcitingAndMoving a go:Property ;
    rdfs:label "Exciting and Moving from one state to another" .

ex:Creator a go:Substance ;
    rdfs:label "Creator" .

ex:Creature a go:Substance ;
    rdfs:label "Creatures" ;
    ex:hasAttribute ex:Incapacitated ;
    ex:hasAttribute ex:Needy .

ex:Incapacitated a go:Attribute ;
    rdfs:label "Incapacitated" .

ex:Needy a go:Attribute ;
    rdfs:label "Needy" .

# Negation of attribute for Creator
ex:Creator ex:doesNotHaveAttribute ex:ExcitingAndMoving .

# Creatures have the attribute
ex:Creature ex:hasAttribute ex:ExcitingAndMoving .
go:Substance rdfs:label "شی"@fa.