@prefix ex: <http://etratnet.ir/graph/proposition/374#> .
    @prefix bt: <http://borhan-onto.ir/ontology/time#> .
    @prefix go: <http://etratnet.ir/ontology#> .
    @prefix owl: <http://www.w3.org/2002/07/owl#> .
    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix xml: <http://www.w3.org/XML/1998/namespace> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
    @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


    @base <http://etratnet.ir/ontology/> .
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/374-1> rdf:type owl:Ontology .

ex:Science a go:entity ;
  ex:equivalentTo ex:TwoSciences .

ex:TwoSciences a go:entity ;
  ex:quantity "2"^^xsd:integer ;
  rdfs:label "sciences" .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/374-2> rdf:type owl:Ontology .

ex:FirstKnowledge a go:Substance ;
    rdfs:label "the first knowledge" .

ex:God a go:person ;
    rdfs:label "God" .

ex:CreaturesOfGod a go:population ;
    rdfs:label "the creatures of God" .

# Event: God stores the first knowledge
ex:StorageEvent1 a go:Event ;
    ex:HasAgent ex:God ;
    ex:HasTheme ex:FirstKnowledge .

# Event: No creature has accessed the knowledge
ex:NoAccessEvent1 a go:Event ;
    ex:HasTheme ex:FirstKnowledge ;
    ex:HasAgent ex:CreaturesOfGod ;
    ex:AccessType "no access"^^xsd:string .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/374-3> rdf:type owl:Ontology .

ex:SecondKnowledge a go:Concept ;
    rdfs:label "The second knowledge taught by God." .

ex:God a go:Agent .
ex:Angels a go:AgentGroup .
ex:Messengers a go:AgentGroup .

ex:TeachingEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:SecondKnowledge ;
    go:HasRecipient ex:Angels , ex:Messengers .
go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/374-4> rdf:type owl:Ontology .

ex:God a go:Agent .
ex:AngelGroup a go:Agent ;
    rdfs:label "Angels"@en .
ex:MessengerGroup a go:Agent ;
    rdfs:label "Messengers"@en .

ex:TeachingEvent1 a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:AngelGroup, ex:MessengerGroup .
go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/374-5> rdf:type owl:Ontology .

ex:event1 a go:Lying ;
    go:IsNegated true ;
    go:HasAgent ex:God ;
    go:HasTheme ex:God, ex:Angels, ex:Messengers .

ex:God a go:DivineEntity .
ex:Angels a go:SpiritualBeings .
ex:Messengers a go:DivineMessengers .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/374-6> rdf:type owl:Ontology .

ex:knowledge a go:Substance .
ex:God a go:Agent .

# Knowledge is stored with God
ex:storageEvent a go:Event ;
    go:HasTheme ex:knowledge ;
    go:HasAgent ex:God ;
    go:EventType "storage" .

# God gives knowledge as He wills
ex:givingEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:knowledge ;
    go:HasCause ex:GodsWill .

ex:GodsWill a go:CognitiveState ;
    go:HasAgent ex:God ;
    go:Description "God's will to give knowledge" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/374-7> rdf:type owl:Ontology .

ex:Knowledge a go:Substance ;
    go:hasLocation ex:God .

ex:God a foaf:Agent .

ex:DelayEvent a go:Event ;
    go:hasAgent ex:God ;
    go:hasTheme ex:Knowledge ;
    go:causedBy ex:GodWill .

ex:GodWill a go:Disposition ;
    go:hasHolder ex:God .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/374-8> rdf:type owl:Ontology .

ex:Knowledge a go:Substance .
ex:God a go:Agent .

ex:StorageEvent a go:Event ;
    go:HasTheme ex:Knowledge ;
    go:HasAgent ex:God ;
    go:LocatedAt ex:God .

ex:ConfirmationEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Knowledge ;
    go:Confirms ex:DesiredContent .

ex:DesiredContent a go:Concept ;
    go:HasCause ex:God .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.