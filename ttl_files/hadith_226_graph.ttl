@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/226-1> rdf:type owl:Ontology .

ex:God a go:Agent ;
    rdfs:label "God" .

ex:Creatures a go:Substance ;
    rdfs:label "Creatures" .

ex:CreationEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Creatures ;
    rdfs:label "Creation Event" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.
