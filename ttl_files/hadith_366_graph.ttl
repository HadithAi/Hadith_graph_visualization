    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/366> rdf:type go:ontology ;
        go:hascontent "10.  از امام باقر عليه‌السلام شنيدم كه مى‌فرمايد: «به واسطه ما خدا پرستش شد، به واسطه ما خدا شناخته شد، به واسطه ما يگانگى خداوند ـ تبارك و تعالى ـ دانسته شد، و محمد پرده‌دار خداى ـ تبارك و تعالى ـ است»."@fa,
        "10.  سَمِعْتُ أَبَا جَعْفَرٍ عليه‌السلام يَقُولُ: «بِنَا عُبِدَ اللّه‌ُ، وَبِنَا عُرِفَ اللّه‌ُ، وَبِنَا وُحِّدَ اللّه‌ُ تَبَارَكَ وَتَعَالى، وَمُحَمَّدٌ حِجَابُ اللّه‌ِ تَبَارَكَ وَتَعَالى»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/366-1> rdf:type owl:ontology .

ex:abdullah a go:person .
ex:buildingevent1 a go:constructionevent ;
    go:hasagent ex:abdullah .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/366-2> rdf:type owl:ontology .

ex:eventinstance1 a go:knowingevent ;
    go:hastheme ex:god ;
    go:hasagent ex:us .

ex:god a go:substance ;
    rdfs:label "god"@en .

ex:us a go:agent ;
    rdfs:label "us"@en .
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/366-3> rdf:type owl:ontology .

ex:event_unification a go:event ;
    go:hastheme ex:entity_allah ;
    go:hastime ex:time_beginning .

ex:entity_allah a go:substance ;
    rdfs:label "allah, the blessed and exalted"@en .

ex:time_beginning a go:time ;
    rdfs:label "in the beginning"@en .
go:event rdfs:label "رخداد"@fa.
go:time rdfs:label "زمان"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/366-4> rdf:type owl:ontology .

ex:muhammad rdf:type foaf:person ;
    rdfs:label "muhammad" ;
    go:hastitle ex:veilofallah .

ex:allah rdf:type go:deity ;
    rdfs:label "allah" ;
    go:hasattribute "blessed" , "exalted" .

ex:veilofallah a go:role ;
    rdfs:label "veil of allah" ;
    go:relatedto ex:allah .