@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/218-20> rdf:type owl:ontology .

ex:abu_al_hasan a go:person ;
    rdfs:label "abu al-hasan" .

ex:body_abu_al_hasan a go:physicalobject ;
    go:ispartof ex:abu_al_hasan ;
    go:hasproperty ex:width ;
    go:hasproperty ex:length .

# saying event
ex:saying_1 a go:event, go:speechevent ;
    go:hasagent ex:abu_al_hasan ;
    go:hastheme ex:saying_content_1 .

ex:saying_content_1 a go:proposition ;
    go:hascontent ex:looking_at_body_1 .

# looking event
ex:looking_at_body_1 a go:event, go:perceptionevent ;
    go:hasagent ex:abu_al_hasan ;
    go:hastheme ex:body_abu_al_hasan ;
    go:hasresult ex:finding_no_change_1 .

# finding event
ex:finding_no_change_1 a go:event, go:finding ;
    go:hasagent ex:abu_al_hasan ;
    go:hastheme ex:no_increase_decrease_width_length .

ex:no_increase_decrease_width_length a go:qualitystatement ;
    go:qualifies ex:body_abu_al_hasan ;
    go:hasproperty ex:width ;
    go:hasproperty ex:length ;
    go:hasvalue "no increase or decrease" .
go:event rdfs:label "رخداد"@fa.
go:quality rdfs:label "ویژگی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/218-21> rdf:type owl:ontology .

ex:abu_al-hasan a go:person .
ex:body_abu_al-hasan a go:anatomicalstructure ;
    go:ispartof ex:abu_al-hasan ;
    ex:cannotincrease true ;
    ex:cannotdecrease true .

evt:speechevent1 a go:communication ;
    go:hasagent ex:abu_al-hasan ;
    go:hascontent evt:content1 .

evt:content1 a go:statement ;
    go:expresses [ a go:state ;
                 go:about ex:body_abu_al-hasan ;
                 ex:hasproperty [ a go:negation ;
                                 go:negatedproperty ex:canincrease ] ,
                               [ a go:negation ;
                                 go:negatedproperty ex:candecrease ]
               ] .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/218-22> rdf:type owl:ontology .

ex:abualhasan a go:person ;
    go:haspart ex:abualhasanbody .

ex:abualhasanbody a go:physiologicalobject ;
    go:ispartof ex:abualhasan .

ex:harms a go:substance .
ex:benefits a go:substance .

ex:repelharmsevent a go:event ;
    go:hasagent ex:abualhasan ;
    go:hastheme ex:harms ;
    go:hastarget ex:abualhasanbody ;
    ex:eventtype "repetitionofrepelling"^^xsd:string .

ex:attractbenefitsevent a go:event ;
    go:hasagent ex:abualhasan ;
    go:hastheme ex:benefits ;
    go:hastarget ex:abualhasanbody ;
    ex:eventtype "repetitionofattracting"^^xsd:string .

ex:obligation a go:modalcondition ;
    go:modalfor ex:repelharmsevent , ex:attractbenefitsevent ;
    ex:status "obligatory"^^xsd:string .

ex:speechevent a go:communicationevent ;
    go:hasagent ex:abualhasan ;
    go:utterance "it is obligatory to repel harms from my body and to attract benefits to it."^^xsd:string .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


