    @prefix bt: <http://borhan-onto.ir/ontology/time#> .
    @prefix go: <http://etratnet.ir/ontology#> .
    @prefix owl: <http://www.w3.org/2002/07/owl#> .
    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix xml: <http://www.w3.org/xml/1998/namespace> .
    @prefix xsd: <http://www.w3.org/2001/xmlschema#> .
    @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
    @prefix time: <http://www.w3.org/2006/time#> .
    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/337> rdf:type go:ontology ;
        go:hascontent "10. «هر كس معتقد باشد كه خدا از چيزى است او را حادث دانسته است، و هر كس معتقد باشد كه او در چيزى است او را محصور كرده است، و هر كس معتقد باشد كه او بر روى چيزى است او را حمل شده پنداشته است»."@fa,
        "10. «مَنْ زَعَمَ أَنَّ اللّه‌َ مِنْ شَيْءٍ، فَقَدْ جَعَلَهُ مُحْدَثاً؛ وَمَنْ زَعَمَ أَنَّهُ فِي شَيْءٍ، فَقَدْ جَعَلَهُ مَحْصُوراً؛ وَمَنْ زَعَمَ أَنَّهُ عَلى شَيْءٍ، فَقَدْ جَعَلَهُ مَحْمُولاً»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/337-1> rdf:type owl:ontology .

ex:claimevent1 a go:event ;
  go:hasagent ex:claimant1 ;
  go:hastheme ex:god .

ex:god a go:divinebeing ;
  go:hasstate ex:createdentity .

ex:createdentity a go:createdstate ;
  go:causedby ex:claimevent1 .

ex:claimant1 a go:person .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/337-2> rdf:type owl:ontology .

ex:claimevent a go:event ;
    go:hasagent ex:whoever ;
    go:hastheme ex:godinsomethingstate .

ex:confineevent a go:event ;
    go:hasagent ex:whoever ;
    go:hastheme ex:god .

ex:whoever a go:agent .
ex:god a go:deity .
ex:something a go:object .

ex:godinsomethingstate a go:state ;
    go:hastheme ex:god ;
    go:haslocation ex:something .
go:event rdfs:label "رخداد"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/337-3> rdf:type owl:ontology .

ex:claimevent1 a go:event ;
    go:hasagent ex:whoever ;
    go:hastheme ex:claimcontent1 .

ex:claimcontent1 a go:assertion ;
    go:hassubject ex:god ;
    go:haspredicate ex:onsomething .

ex:onsomething a go:relation ;
    go:hasobject ex:something .

ex:makeevent1 a go:event ;
    go:hasagent ex:whoever ;
    go:hastheme ex:godcarried .

ex:godcarried a go:propertyassertion ;
    go:hassubject ex:god ;
    go:haspredicate go:carried .

ex:god a go:substance .
ex:something a go:substance .
ex:whoever a go:agent .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.