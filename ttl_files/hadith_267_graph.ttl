@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/267-5> rdf:type owl:Ontology .

ex:Sun a go:Substance .
ex:Clouds a go:Substance .
ex:They a go:agent .

ex:HonestyCondition a go:Event ;
  a ex:ConditionalEvent ;
  go:HasAgent ex:They ;
  ex:condition "honest"^^xsd:boolean .

ex:FillingEyes a go:Event ;
  a ex:ImperativeEvent ;
  go:HasAgent ex:They ;
  go:HasTheme ex:Sun ;
  ex:action "fill eyes with sun" .

ex:NoCloudsBelowSun a go:Event ;
  a ex:SpatialRelationEvent ;
  ex:subject ex:Clouds ;
  ex:location "below sun" ;
  ex:existence "absent"^^xsd:boolean .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.