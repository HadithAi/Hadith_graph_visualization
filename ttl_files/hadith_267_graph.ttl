ex:Throne a go:Substance ;
    go:isPartOf ex:SeventyPartsLightOfVeil .

ex:SeventyPartsLightOfVeil a go:quantifiedEntity ;
    go:quantity "70"^^<http://www.w3.org/2001/XMLSchema#integer> ;
    go:partOf ex:LightOfVeil .

ex:LightOfVeil a go:Substance .
