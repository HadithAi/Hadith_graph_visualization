@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/266-3> rdf:type owl:Ontology .

ex:Event_Speech_1 a go:CommunicationEvent ;
    rdfs:label "Speech by ink to Commander of the Faithful"@en ;
    go:HasAgent ex:Ink ;
    go:HasRecipient ex:CommanderOfTheFaithful ;
    go:HasContent ex:Question_1 .

ex:Ink a go:Substance ;
    rdfs:label "Ink"@en .

ex:CommanderOfTheFaithful a go:Person ;
    rdfs:label "Commander of the Faithful"@en .

ex:Lord a go:Person ;
    rdfs:label "Your Lord"@en .

ex:Question_1 a go:Question ;
    rdfs:label "And how did you see your Lord?"@en ;
    go:HasTheme ex:Lord ;
    go:DirectedTo ex:CommanderOfTheFaithful .
go:Substance rdfs:label "شی"@fa.
