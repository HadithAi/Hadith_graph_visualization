@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/266-2> rdf:type owl:Ontology .

ex:Commander_of_the_Faithful a go:Person ;
    rdfs:label "Amir al-Mu'minin (peace be upon him)" .

ex:Scholar a go:Person ;
    rdfs:label "the scholar (al-Ḥabir)" .

ex:SayingEvent1 a go:Saying ;
    go:HasAgent ex:Commander_of_the_Faithful ;
    go:HasRecipient ex:Scholar ;
    go:HasTheme ex:ReproachContent .

ex:ReproachContent a go:MessageContent ;
    go:hasText "Woe to you, I never worshipped a Lord whom I did not see." .
