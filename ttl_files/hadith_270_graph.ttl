@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/270-1> rdf:type owl:Ontology .

ex:God a go:Substance ;
    rdfs:label "God Almighty" .

ex:Eyes a go:Substance ;
    rdfs:label "the eyes" .

ex:Event1 a go:Event ;
    go:hasAgent ex:Eyes ;
    go:hasTheme ex:God ;
    go:hasAction "perceive" ;
    go:hasNegation true .

ex:Event2 a go:Event ;
    go:hasAgent ex:God ;
    go:hasTheme ex:Eyes ;
    go:hasAction "perceive" ;
    go:hasNegation false .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
