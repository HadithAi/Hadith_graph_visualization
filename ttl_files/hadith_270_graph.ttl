@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/270-5> rdf:type owl:Ontology .

ex:Event1 rdf:type go:PerceptionEvent ;
    go:HasAgent ex:God_Almighty ;
    go:HasTheme ex:Illusions_of_Hearts .

ex:God_Almighty rdf:type go:Agent ;
    rdfs:label "God Almighty" .

ex:Illusions_of_Hearts rdf:type go:MentalState ;
    go:possessedBy ex:Hearts ;
    rdfs:label "illusions of the hearts" .

ex:Hearts rdf:type go:AnatomicalEntity ;
    rdfs:label "hearts" .
go:Agent rdfs:label "عامل"@fa.