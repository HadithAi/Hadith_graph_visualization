@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/246-3> rdf:type owl:Ontology .

ex:RevelationEvent1 a go:Event, ex:QuranicRevelation ;
    go:HasTheme ex:Verse_001001 ;
    ex:RevealedUntil ex:EndOfChapter_001 .

ex:Verse_001001 a go:Substance ;
    go:hasLiteral "قُلْ هُوَ اللّهُ أَحَدٌ"@ar ;
    ex:verseNumber "1"^^<http://www.w3.org/2001/XMLSchema#integer> ;
    ex:chapterNumber "1"^^<http://www.w3.org/2001/XMLSchema#integer> .

ex:EndOfChapter_001 a go:Substance ;
    ex:partOfChapter ex:Chapter_001 .

ex:Chapter_001 a go:Substance ;
    ex:chapterNumber "1"^^<http://www.w3.org/2001/XMLSchema#integer> .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.