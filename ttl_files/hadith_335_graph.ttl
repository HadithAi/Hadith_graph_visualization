    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/335> rdf:type go:ontology ;
        go:hascontent "8.  از امام صادق عليه‌السلام درباره سخن خداى عز و جل كه: «خداى مهر گستر بر عرش استوا دارد»پرسيدم. فرمود: «يعنى با همه اشيا نسبت يكسان دارد. پس، چيزى به او نزديك‌تر از چيز ديگر نيست. هيچ دورى از او دور و هيچ نزديكى به او نزديك نيست. با همه چيز نسبت يكسان دارد»."@fa,
        "8.  سَأَلْتُ أَبَا عَبْدِ اللّه‌ِ عليه‌السلام عَنْ قَوْلِ اللّه‌ِ عَزَّ وَجَلَّ: «الرَّحْمَنُ عَلَى الْعَرْشِ اسْتَوى» فَقَالَ: «اسْتَوى فِي كُلِّ شَيْءٍ؛ فَلَيْسَ شَيْءٌ أَقْرَبَ إِلَيْهِ مِنْ شَيْءٍ، لَمْ يَبْعُدْ مِنْهُ بَعِيدٌ، وَلَمْ يَقْرُبْ مِنْهُ قَرِيبٌ، اسْتَوى فِي كُلِّ شَيْءٍ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/335-1> rdf:type owl:ontology .

ex:utterance1 a go:event ;
    go:hasagent ex:god_almighty ;
    go:hastheme ex:prophecy1 .

ex:prophecy1 a go:proposition ;
    go:hasagent ex:the_most_merciful ;
    go:hasaction ex:establishment1 .

ex:establishment1 a go:event ;
    go:hasagent ex:the_most_merciful ;
    go:hastheme ex:self ;
    go:haslocation ex:throne .

ex:god_almighty a go:deity ;
    rdfs:label "god almighty" .

ex:the_most_merciful a go:attributeofdeity ;
    go:refersto ex:god_almighty ;
    rdfs:label "the most merciful" .

ex:throne a go:place ;
    rdfs:label "throne" .

ex:self a go:agent ;
    go:refersto ex:the_most_merciful .
go:event rdfs:label "رخداد"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/335-2> rdf:type owl:ontology .

ex:ar-rahman a go:substance ;
    rdfs:label "ar-rahman (the merciful)"@en .

ex:everything a go:substance ;
    rdfs:label "everything"@en .

ex:state_1 a go:state ;
    go:hasagent ex:ar-rahman ;
    go:hastheme ex:everything .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/335-3> rdf:type owl:ontology .

ex:allah_almighty a go:substance ;
    rdfs:label "allah almighty"@en .

ex:nothing a go:concept ;
    rdfs:label "nothing"@en .

ex:anything a go:concept ;
    rdfs:label "anything"@en .

ex:proximitycomparison_3 a ex:comparativeproximityevent ;
    ex:closerto ex:allah_almighty ;
    ex:comparedentities ex:nothing , ex:anything ;
    ex:isnegation true .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/335-4> rdf:type owl:ontology .

:allah a go:deity ;
       ex:hasnofarentity go:substance .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/335-5> rdf:type owl:ontology .

ex:allah a go:substance ;
    go:hasproperty ex:maxclosenesstodivine .

ex:maxclosenesstodivine a go:property ;
    go:defines "maximum closeness, no other entity is closer." .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/335-6> rdf:type owl:ontology .

ex:eventinstance1 a go:establishmentevent ;
    go:hasagent ex:allah ;
    go:hastheme ex:everything .

ex:allah a go:agent ;
    rdfs:label "allah" .

ex:everything a go:substance ;
    rdfs:label "everything" .
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.