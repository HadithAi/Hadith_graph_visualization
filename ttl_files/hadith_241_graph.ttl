@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-1> rdf:type owl:Ontology .

ex:event1 a go:Event ;
    go:HasAgent ex:Jews ;
    go:HasLocation ex:HeadOfGalut .

ex:Jews a go:Substance ;
    go:denotation "Jews" .

ex:HeadOfGalut a go:location ;
    go:denotation "Head of the Galut" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-2> rdf:type owl:Ontology .

ex:speechEvent1 rdf:type go:Event ;
    go:HasAgent ex:Jews ;
    go:HasRecipient ex:HeadOfGoliath ;
    go:HasTheme ex:speechContent1 .

ex:Jews rdf:type go:human .
ex:HeadOfGoliath rdf:type go:human .
ex:CommanderOfTheFaithful rdf:type go:human ;
    rdfs:label "Commander of the Faithful (peace be upon him)" .

ex:speechContent1 rdf:type go:utteranceContent ;
    go:HasSubject ex:CommanderOfTheFaithful ;
    go:HasProperty ex:knowledgeable .

ex:knowledgeable rdf:type go:Quality .
go:Event rdfs:label "رخداد"@fa.
go:Quality rdfs:label "ویژگی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-3> rdf:type owl:Ontology .

ex:Jews a foaf:Group ;
    a go:Agent .

ex:HeadOfCommunity a foaf:Person ;
    a go:Agent .

ex:ThisMan a foaf:Person .

# Event: The request by the Jews to the head of the community
event:Request1 a go:Event ;
    go:HasAgent ex:Jews ;
    go:HasRecipient ex:HeadOfCommunity ;
    go:HasTheme event:MovementToThisMan .

# Event: Movement of head to this man
event:MovementToThisMan a go:Event ;
    go:HasAgent ex:HeadOfCommunity ;
    go:HasDestination ex:ThisMan ;
    go:HasPurpose event:AskQuestion .

# Event: Asking a question
event:AskQuestion a go:Event ;
    go:HasAgent ex:HeadOfCommunity ;
    go:HasTheme ex:Question .

ex:Question a go:Information .
go:Event rdfs:label "رخداد"@fa.
go:Information rdfs:label "اطلاعات"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-4> rdf:type owl:Ontology .

ex:event1 rdf:type go:Event , ex:ComingEvent ;
          go:HasAgent ex:Jews ;
          go:HasGoal ex:CommanderOfTheFaithful .

ex:Jews rdf:type go:ethnicGroup ;
        rdfs:label "the Jews"@en .

ex:CommanderOfTheFaithful rdf:type go:person ;
                           rdfs:label "Commander of the Faithful (peace be upon him)"@en .
go:Event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-5> rdf:type owl:Ontology .

ex:SpeechEvent1 rdf:type go:SpeechEvent ;
    go:HasRecipient ex:Jews ;
    go:HasTheme ex:CommanderOfTheFaithful_PeaceBeUponHim .

ex:Jews rdf:type go:Group .

ex:CommanderOfTheFaithful_PeaceBeUponHim rdf:type go:Person ;
    go:location ex:Palace .

ex:Palace rdf:type go:Location .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-6> rdf:type owl:Ontology .

ex:Jews a go:Group ;
    foaf:name "The Jews" .

ex:CommanderOfTheFaithful a foaf:Person ;
    foaf:name "Commander of the Faithful (peace be upon him)" .

ex:WaitingEvent a go:Event ;
    go:HasAgent ex:Jews ;
    go:IsTemporallyBoundedBy ex:ComingOutEvent .

ex:ComingOutEvent a go:Event ;
    go:HasAgent ex:CommanderOfTheFaithful .
go:Event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-7> rdf:type owl:Ontology .

ex:event1 a go:SpeechEvent ;
    go:HasAgent ex:Goliath ;
    go:HasRecipient ex:CommanderOfTheFaithful ;
    go:HasTheme ex:RequestToAsk .

ex:Goliath a go:Agent ;
    rdfs:label "Goliath" .

ex:CommanderOfTheFaithful a go:Agent ;
    go:hasTitle "Commander of the Faithful" ;
    go:hasHonorific "peace be upon him" .

ex:RequestToAsk a go:Purpose ;
    go:hasContent "We have come to ask you" .
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-8> rdf:type owl:Ontology .

ex:SpeechEvent1 a go:Event ;
    go:HasAgent ex:AmirAlMu_minin ;
    go:HasRecipient ex:Jew ;
    go:HasTheme ex:AskCommand .

ex:AmirAlMu_minin a go:Person ;
    go:hasTitle "Commander of the Faithful (peace be upon him)" .

ex:Jew a go:Person ;
    go:hasEthnicIdentity "Jew" .

ex:AskCommand a go:Command ;
    go:hasContent "Ask about whatever you desire" .
go:Event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-9> rdf:type owl:Ontology .

ex:AskingEvent_1 a go:Event ;
    go:HasAgent ex:Ras_Al_Jalut ;
    go:HasTheme ex:Commander_of_the_Faithful ;
    go:HasPurpose ex:QuestionAboutLordExistence .

ex:Ras_Al_Jalut a go:Agent, go:Person .
ex:Commander_of_the_Faithful a go:Agent, go:Person .
ex:Lord a go:Agent, go:Person .

ex:QuestionAboutLordExistence a go:InformationContent ;
    go:About ex:Lord ;
    go:TemporalQuestion "When does the Lord exist?" .
go:Event rdfs:label "رخداد"@fa.
go:Information rdfs:label "اطلاعات"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-10> rdf:type owl:Ontology .

ex:CommanderOfFaithful a go:Person ;
    rdfs:label "Commander of the Faithful"@en .

ex:He a go:Person ;
    rdfs:label "He"@en .

ex:OntologicalStatus a go:Quality ;
    rdfs:label "Ontological Status"@en .

ex:NoOntologicalStatus a ex:OntologicalStatus ;
    ex:hasValue "false"^^xsd:boolean ;
    rdfs:label "Without ontological status"@en .

ex:ExistenceState a go:State ;
    ex:hasQuality ex:NoOntologicalStatus ;
    go:HasTheme ex:He .

ex:SpeechEvent a go:Event ;
    go:HasAgent ex:CommanderOfFaithful ;
    go:HasTheme ex:ExistenceState ;
    rdfs:label "Saying that He was without ontological status"@en .
go:Event rdfs:label "رخداد"@fa.
go:Quality rdfs:label "ویژگی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-11> rdf:type owl:Ontology .

ex:CommanderOfFaithful a go:Agent ;
    rdfs:label "Amir al-Mu'minin" ;
    go:hasHonorific "Peace be upon him" .

ex:kif a go:Substance ;
    rdfs:label "kif" .

ex:He a go:Agent ;
    rdfs:label "He" .

ex:speechEvent_11 a go:SpeechAct, go:Event ;
    go:HasAgent ex:CommanderOfFaithful ;
    go:HasTheme ex:statement_11 .

ex:statement_11 a go:proposition ;
    go:states "He was without kif" ;
    go:HasAgent ex:He ;
    go:negatesPossession ex:kif .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-12> rdf:type owl:Ontology .

ex:Amir_al_Mu_minin a go:Person ;
    rdfs:label "Amir al-Mu'minin (peace be upon him)" .

ex:SayingEvent1 a go:Event ;
    go:HasAgent ex:Amir_al_Mu_minin ;
    go:HasTheme ex:Quotation1 .

ex:Quotation1 a go:Statement ;
    go:hasText "He was always without 'how much' and without 'how'" .
go:Event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-13> rdf:type owl:Ontology .

ex:CommanderOfTheFaithful a go:Agent ;
    go:hasRole "Commander of the Faithful" .

ex:SpeakingEvent1 a go:CommunicationEvent ;
    go:HasAgent ex:CommanderOfTheFaithful ;
    go:HasTheme ex:Statement1 .

ex:Statement1 a go:Proposition ;
    go:asserts ex:EntityHe .

ex:EntityHe a go:Individual ;
    ex:hasPredecessor "false"^^xsd:boolean .
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-14> rdf:type owl:Ontology .

ex:SpeechEvent_AmirAlMu'minin_001 a go:SpeechEvent ;
    go:HasAgent ex:AmirAlMu'minin ;
    go:HasTheme ex:Quote_Before_before_Limit_Concept .

ex:AmirAlMu'minin a go:Person ;
    rdfs:label "Amir al-Mu'minin (peace be upon him)" .

ex:Quote_Before_before_Limit_Concept a go:AbstractConcept ;
    go:hasText "It is before the 'before', without a 'before' and without an end or limit." .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-15> rdf:type owl:Ontology .

ex:sayingEvent1 a ex:SayingEvent ;
    go:HasAgent ex:CommanderOfTheFaithful ;
    ex:hasContent ex:UltimateGoalCutOff .

ex:CommanderOfTheFaithful a go:Person ;
    go:name "Commander of the Faithful (peace be upon him)" .

ex:UltimateGoalCutOff a go:Event ;
    go:HasTheme ex:UltimateGoal ;
    ex:CutOffFrom ex:UnknownRecipient .

ex:UltimateGoal a go:Substance ;
    go:name "Ultimate Goal" .

ex:UnknownRecipient a go:Person ;
    go:name "him" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-16> rdf:type owl:Ontology .

ex:CommanderOfFaithful a go:Person ;
  go:hasProperName "Amir al-Mu'minin" .

ex:He a go:Substance ;
  rdfs:label "ultimate of every ultimate" .

ex:SayingEvent a go:Event ;
  go:HasAgent ex:CommanderOfFaithful ;
  go:HasTheme ex:Statement1 .

ex:Statement1 a go:Statement ;
  go:refersTo ex:He ;
  go:hasContent "He is the ultimate of every ultimate" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


