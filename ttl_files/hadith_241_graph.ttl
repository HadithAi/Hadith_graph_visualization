@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/241-3> rdf:type owl:ontology .

ex:jews a foaf:group ;
    a go:agent .

ex:headofcommunity a foaf:person ;
    a go:agent .

ex:thisman a foaf:person .

# event: the request by the jews to the head of the community
event:request1 a go:event ;
    go:hasagent ex:jews ;
    go:hasrecipient ex:headofcommunity ;
    go:hastheme event:movementtothisman .

# event: movement of head to this man
event:movementtothisman a go:event ;
    go:hasagent ex:headofcommunity ;
    go:hasdestination ex:thisman ;
    go:haspurpose event:askquestion .

# event: asking a question
event:askquestion a go:event ;
    go:hasagent ex:headofcommunity ;
    go:hastheme ex:question .

ex:question a go:information .
go:event rdfs:label "رخداد"@fa.
go:information rdfs:label "اطلاعات"@fa.
go:agent rdfs:label "عامل"@fa.


