    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/271> rdf:type go:ontology ;
        go:hascontent "11.  به ابو جعفر [امام جواد] عليه‌السلام عرض كردم: «ديده‌ها او را در نمى‌يابند و او ديده‌ها را در مى‌يابد» [يعنى چه]؟ فرمود: «اى ابا هاشم، وهم‌هاى دل‌ها باريك‌تر از بينايى‌هاى چشم‌هاست [؛ زيرا] تو گاه با قوه وهم خود سِند و هند و شهرهايى را كه به آنها نرفته‌اى ادراك [و تصور] مى‌كنى، در حالى كه با چشم خود دريافتشان نمى‌كنى. وهم‌هاى دل‌ها خدا را دريافت نمى‌كند، پس چگونه بينايى‌هاى چشم‌ها دريافتش كنند؟»"@fa,
        "11.  قُلْتُ لِأَبِي جَعْفَرٍ عليه‌السلام: «لاَ تُدْرِكُهُ الْأَبْصارُ وَهُوَ يُدْرِكُ الْأَبْصارَ»؟ فَقَالَ: «يَا أَبَا هَاشِمٍ، أَوْهَامُ الْقُلُوبِ أَدَقُّ مِنْ أَبْصَارِ الْعُيُونِ؛ أَنْتَ قَدْ تُدْرِكُ بِوَهْمِكَ السِّنْدَ وَالْهِنْدَ وَالْبُلْدَانَ الَّتِي لَمْ تَدْخُلْهَا وَلاَ تُدْرِكُهَا بِبَصَرِكَ، وَأَوْهَامُ الْقُلُوبِ لاَ تُدْرِكُهُ، فَكَيْفَ أَبْصَارُ الْعُيُونِ؟!»"@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/271-1> rdf:type owl:ontology .

ex:emotionalillusionsofheart a go:phenomenon ;
    go:associatedwith ex:heart ;
    go:hasquality ex:finenesslevel1 .

ex:sightofeyes a go:phenomenon ;
    go:associatedwith ex:eyes ;
    go:hasquality ex:finenesslevel2 .

ex:finenesslevel1 go:isfinerthan ex:finenesslevel2 .

ex:heart a go:organ .
ex:eyes a go:organ .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/271-2> rdf:type owl:ontology .

# entities
ex:human a go:agent ;
    rdfs:label "human" .

ex:sind a go:place ;
    rdfs:label "sind" .

ex:india a go:place ;
    rdfs:label "india" .

ex:countries a go:place ;
    rdfs:label "countries not entered or perceived" .

ex:illusions a go:substance ;
    rdfs:label "illusions" .

# event
ex:perceivingevent a go:event, ex:perception ;
    go:hasagent ex:human ;
    go:hastheme ex:sind, ex:india, ex:countries ;
    go:hasmedium ex:illusions ;
    ex:canperceivewithoutentry "true"^^xsd:boolean ;
    ex:canperceivewithoutsight "true"^^xsd:boolean .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/271-3> rdf:type owl:ontology .

ex:illusionsofhearts a ex:abstractconcept ;
    rdfs:label "illusions of the hearts"@en .

ex:he a go:agent ;
    rdfs:label "he"@en .

ex:perceptionevent a go:event ;
    ex:negation true ;
    go:hasagent ex:illusionsofhearts ;
    go:hastheme ex:he ;
    rdfs:label "perception event - negated"@en .
go:event rdfs:label "رخداد"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/271-4> rdf:type owl:ontology .

ex:event_eyesperceive a go:perceptionevent ;
  go:hasagent ex:eyes ;
  go:hastheme ex:he .

ex:event_illusionsheartsnotperceive a go:perceptionevent ;
  go:hasagent ex:illusions_hearts ;
  go:hastheme ex:he ;
  go:result "false"^^<http://www.w3.org/2001/xmlschema#boolean> .

ex:eyes a go:organismpart ;
  go:partof ex:human .

ex:illusions_hearts a go:conceptualentity .

ex:he a go:person .