<http://etratnet.ir/ontology/235-6> rdf:type owl:Ontology .

ex:CommanderOfFaithful a go:Person ;
    rdfs:label "Commander of the Faithful" .

ex:CompanionsOfCommanderOfFaithful a go:Group ;
    rdfs:label "Companions of the Commander of the Faithful" ;
    go:HasRelationTo ex:CommanderOfFaithful .

ex:Meaning a go:Substance ;
    rdfs:label "Meaning" .

ex:Names a go:Concept ;
    rdfs:label "Names" .

ex:Attributes a go:Concept ;
    rdfs:label "Attributes" .

ex:WorshipEvent a go:Event ;
    rdfs:label "Worshiping the Meaning" ;
    go:HasAgent ex:CompanionsOfCommanderOfFaithful ;
    go:HasTheme ex:Meaning ;
    go:HasInstrument ex:AttributingNamesPerAttributes .

ex:AttributingNamesPerAttributes a go:Event ;
    rdfs:label "Attributing names according to attributes" ;
    go:HasTheme ex:Meaning ;
    go:HasInstrument ex:Names, ex:Attributes .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
