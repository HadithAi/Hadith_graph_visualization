@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/265-13> rdf:type owl:ontology .

ex:man_a a go:substance ;
    go:label "man" .

ex:god_a a go:substance ;
    go:label "god" .

ex:message_a a go:substance ;
    go:label "his message" .

# event: man leaves
ex:leave_event_1 a go:event ;
    go:hasagent ex:man_a ;
    go:label "man leaves" .

# event: man says something
ex:saying_event_1 a go:event ;
    go:hasagent ex:man_a ;
    go:hastheme ex:speech_content_1 ;
    go:label "man saying" .

# speech content node
ex:speech_content_1 a go:substance ;
    go:label "speech: 'god knows best where he places his message.'" .

# event: god knows
ex:knowing_event_1 a go:event ;
    go:hasagent ex:god_a ;
    go:hastheme ex:placing_event_1 ;
    go:label "god knows" .

# event: god places his message
ex:placing_event_1 a go:event ;
    go:hasagent ex:god_a ;
    go:hastheme ex:message_a ;
    go:label "god places message" .

# linking the knowing theme to placing event
ex:knowing_event_1 go:hastheme ex:placing_event_1 .

# linking speech content to knowing event
ex:speech_content_1 go:hastheme ex:knowing_event_1 .

# linking the two main events: leaving and saying (implied concurrency)
ex:leave_event_1 go:precededby ex:saying_event_1 .

go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.