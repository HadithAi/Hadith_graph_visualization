@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/227-62> rdf:type owl:Ontology .

ex:SpeechEvent1 a go:Event, ex:SpeechEvent ;
    go:HasAgent ex:Questioner ;
    go:HasTheme ex:Question1 .

ex:Questioner a go:Agent, go:Person .

ex:Question1 a go:Proposition ;
    go:HasSubject ex:Thing ;
    go:HasEvent ex:SufferingEvent1 .

ex:Thing a go:IndependentContinuant .

ex:SufferingEvent1 a go:Event, ex:SufferingEvent ;
    go:HasAgent ex:Thing .

ex:Thing go:HasSelfCause ex:SufferingEvent1 .
go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.
