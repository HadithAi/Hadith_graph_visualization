@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/227-61> rdf:type owl:Ontology .

ex:AbuAbdullah a go:Person ;
    rdfs:label "Abu Abdullah" .

ex:SpeechEvent1 a go:CommunicationEvent ;
    go:HasAgent ex:AbuAbdullah ;
    go:HasTheme ex:Content1 .

ex:Content1 a go:Assertion ;
    go:asserts ex:UniqueNatureOfHe .

ex:UniqueNatureOfHe a go:Property ;
    go:about ex:He ;
    go:hasProperty ex:UniqueHow .

ex:He a go:Person .

ex:UniqueHow a go:Quality ;
    ex:NotDeservedByOthers true ;
    ex:NotSharedByOthers true ;
    ex:NotEncompassedByOthers true ;
    ex:NotKnownByOthers true .
go:Quality rdfs:label "ویژگی"@fa.
