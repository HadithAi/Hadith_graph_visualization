ex:He a go:Person .

ex:Supreme a go:Quality ;
    rdfs:label "supreme"@en .

ex:Will a go:Concept ;
    rdfs:label "will"@en .

ex:Desire a go:Concept ;
    rdfs:label "desire"@en .

ex:StateInstance a go:State .

ex:StateInstance go:HasAgent ex:He ;
    go:HasTheme ex:Supreme , ex:Will , ex:Desire .
go:Quality rdfs:label "ویژگی"@fa.
