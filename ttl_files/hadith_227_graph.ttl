@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/227-59> rdf:type owl:Ontology .

ex:Abu_Abdullah a go:Person ;
    rdfs:label "Abu Abdullah" .

ex:Statement1 a go:SpeechEvent ;
    go:HasAgent ex:Abu_Abdullah ;
    go:HasTheme ex:DenialConsequences .

ex:DenialConsequences a go:Concept ;
    ex:denialRepudiates ex:Repudiation ;
    ex:denialRejects ex:LordshipRejection ;
    ex:denialInvalidates ex:Invalidation .

ex:Repudiation a go:Concept .
ex:LordshipRejection a go:Concept .
ex:Invalidation a go:Concept .
