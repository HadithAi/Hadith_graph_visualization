<http://etratnet.ir/ontology/227-64> rdf:type owl:Ontology .

ex:DirectingThings a go:Capability ;
    rdfs:label "Directing things" .

ex:ProcessingThings a go:Capability ;
    rdfs:label "Processing things" .

ex:DirectingProcessingProperty a go:Property ;
    rdfs:label "Directing and processing property" ;
    go:HasTheme ex:DirectingThings , ex:ProcessingThings .

ex:Creature a go:LivingBeing ;
    rdfs:label "Creature" ;
    go:HasProperty ex:DirectingProcessingProperty .
