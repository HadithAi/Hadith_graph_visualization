@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/227-60> rdf:type owl:Ontology .

ex:AbuAbdullah a go:Person ;
    rdfs:label "Abu Abdullah" ;
    go:honorific "عليه السلام" .

ex:SayStatement1 a go:Saying, ex:EventInstance ;
    go:HasAgent ex:AbuAbdullah ;
    go:HasTheme ex:ConditionalStatement1 .

ex:ConditionalStatement1 a ex:ConditionalStatement ;
    ex:condition ex:LikeningEvent1 ;
    ex:consequence ex:Affirmation1 .

ex:LikeningEvent1 a ex:Attribution ;
    go:HasAgent ex:UnknownPerson ;
    go:HasTheme ex:Deity ;
    ex:likenedTo ex:Others .

ex:Deity a go:Deity ;
    rdfs:label "Him" .

ex:Others a go:CreatedMadeBeings .

ex:Affirmation1 a ex:AttributeAssertion ;
    go:HasTheme ex:Deity ;
    ex:hasAttributes ex:CreatedMadeAttributes ;
    ex:context "Which do not deserve divinity" .

ex:CreatedMadeAttributes a go:AttributeSet ;
    go:includes ex:CreatedBeings, ex:MadeBeings .

ex:CreatedBeings a go:CreatedBeing .
ex:MadeBeings a go:MadeBeing .

ex:UnknownPerson a go:Person ;
    go:role "who likens" .
