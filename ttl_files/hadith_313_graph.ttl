    go:HasAgent ex:Person1 .

ex:Person1 ex:hasState ex:Disbeliever .

# Negation of true worship
ex:TrueWorship a go:Event ;
    go:HasAgent ex:Person1 ;
    go:HasTheme ex:anything .

ex:WorshipNegation a go:Negation ;
    go:negates ex:TrueWorship .
