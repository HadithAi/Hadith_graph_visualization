ex:MeaningNegation a go:Negation ;
    go:negates ex:Meaning .

ex:WorshipEvent1 a go:Event ;
    go:HasAgent ex:Person1 ;
    go:HasTheme [ a go:Concept ;
                  go:hasPart ex:Name ;
                  go:hasPart ex:MeaningNegation
                ] .

