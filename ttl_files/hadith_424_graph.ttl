@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/424-7> rdf:type owl:Ontology .

ex:GrantEvent a go:Event ;
  go:HasAgent go:God ;
  go:HasRecipient ex:Person ;
  go:HasTheme ex:Honor , ex:Beauty .

ex:Honor a go:Quality ;
  go:QualityIn ex:ex:Home .

ex:Beauty a go:Quality ;
  go:QualityIn ex:ex:Appearance .

ex:Person a go:Human .

go:God a go:Deity .

ex:ProhibitionEvent a go:NormativeEvent ;
  go:HasAgent ex:Person ;
  go:DeniesAction ex:ArroganceTowardsOthers .

ex:ArroganceTowardsOthers a go:Action ;
  go:ActionTarget ex:Others .

ex:Others a go:HumanGroup .

ex:Home a go:Place .
ex:Appearance a go:PhysicalAttribute .
go:Event rdfs:label "رخداد"@fa.
go:Quality rdfs:label "ویژگی"@fa.
