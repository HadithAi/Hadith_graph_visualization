    go:HasAgent ex:God ;
    go:HasTheme ex:RecipientPerson ;
    go:hasObject ex:Sustenance .

ex:MaintenanceAfterAlmsDecline a go:Event ;
    go:HasAgent ex:RecipientPerson ;
    go:HasTheme ex:PoorPeople ;
    go:hasCondition ex:VoluntaryAlmsDecrease .

ex:VoluntaryAlmsDecrease a go:Event ;
    go:HasTheme ex:VoluntaryAlms .

ex:God a go:Deity .
ex:RecipientPerson a go:Person .
ex:Sustenance a go:Substance .
ex:PoorPeople a go:Group ;
    go:isClassifiedUnder ex:Poor .
