ex:God a go:Deity .

ex:Person_Strong a go:Person .
ex:Persons_Weaker a go:Group .

ex:GrantingStrength a go:Event ;
    go:HasAgent ex:God ;
    go:HasRecipient ex:Person_Strong ;
    rdf:type go:GrantingPowerEvent .

ex:BearingWeakPersons a go:Event ;
    rdf:type go:EnduranceEvent ;
