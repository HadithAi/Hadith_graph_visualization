<http://etratnet.ir/ontology/424-1> rdf:type owl:Ontology .

ex:eventBlessing a go:Event ;
  go:HasAgent ex:God ;
  go:HasTheme ex:Blessing ;
  go:HasRecipient ex:Servant .

ex:eventObligation a go:Event ;
  go:HasAgent ex:God ;
  go:HasTheme ex:Accountability ;
  go:HasRecipient ex:Servant ;
  go:causes ex:eventBlessing .

ex:God a go:Substance ;
  go:name "God" .

ex:Servant a go:Individual ;
  go:name "Servant" .

ex:Blessing a go:Substance ;
  go:name "Blessing" .

ex:Accountability a go:Substance ;
  go:name "Accountability" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
