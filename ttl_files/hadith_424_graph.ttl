ex:God a go:Agent .
ex:Person1 a go:Person .
ex:Sustenance a go:Resource .
ex:Wealth a go:Resource .

ex:GrantingSustenanceEvent1 a go:Event ;
    go:HasAgent ex:God ;
    go:HasRecipient ex:Person1 ;
