<http://etratnet.ir/ontology/424-6> rdf:type owl:Ontology .

# Entities
ex:God a go:Substance ;
    go:hasRole "Agent" .

ex:Person a go:Substance .

ex:House a go:physicalEntity .
ex:Appearance a go:physicalEntity .

# Events
ex:GrantHonorBeauty a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Person ;
    ex:hasHonorLocation ex:House ;
    ex:hasBeautyAspect ex:Appearance .

ex:JudgmentPraise a go:Event ;
    go:HasAgent ex:Person ;
    go:HasTheme ex:God ;
    ex:judgment "Person should praise God" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
