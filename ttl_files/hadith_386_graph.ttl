@prefix ex: <http://etratnet.ir/graph/example/386#> .
    @prefix bt: <http://borhan-onto.ir/ontology/time#> .
    @prefix go: <http://etratnet.ir/ontology#> .
    @prefix owl: <http://www.w3.org/2002/07/owl#> .
    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix xml: <http://www.w3.org/xml/1998/namespace> .
    @prefix xsd: <http://www.w3.org/2001/xmlschema#> .
    @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
    @prefix time: <http://www.w3.org/2006/time#> .
    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/386> rdf:type go:ontology ;
        go:hascontent "2.  «هيچ چيزى در آسمان‌ها و زمين نباشد [و بود نشود] مگر با هفت چيز: با حكم و اندازه‌گيرى و اراده و خواست و نبشته و مدت و پروانه. بنابراين، هر كه جز اين پندارد، هر آينه بر خدا دروغ بسته، يا سخن خداوند عز و جل را رد كرده است»."@fa,
        "2.  عَنْ أَبِي الْحَسَنِ مُوسَى بْنِ جَعْفَرٍ عليهماالسلام، قَالَ: «لاَ يَكُونُ شَيْءٌ فِي السَّمَاوَاتِ وَلاَ فِي الْأَرْضِ إِلاَّ بِسَبْعٍ: بِقَضَاءٍ، وَقَدَرٍ، وَإِرَادَةٍ، وَمَشِيئَةٍ، وَكِتَابٍ، وَأَجَلٍ، وَإِذْنٍ، فَمَنْ زَعَمَ غَيْرَ هذَا، فَقَدْ كَذَبَ عَلَى اللّه‌ِ، أَوْ رَدَّ عَلَى اللّه‌ِ عَزَّ وَجَلَّ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/386-1> rdf:type owl:ontology .

ex:heavens a go:substance .
ex:earth a go:substance .
ex:seventhings a go:thing .

ex:existenceevent a go:event ;
    go:hastheme ex:heavens, ex:earth ;
    go:hascause ex:seventhings .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/386-2> rdf:type owl:ontology .

ex:seventhings rdf:type ex:setofsevenconcepts .

ex:seventhings ex:hasmember ex:decree, ex:destiny, ex:will, ex:desire, ex:thebook, ex:lifespan, ex:permission .

ex:decree rdf:type go:substance, ex:concept ;
    rdfs:label "decree"@en .
ex:destiny rdf:type go:substance, ex:concept ;
    rdfs:label "destiny"@en .
ex:will rdf:type go:substance, ex:concept ;
    rdfs:label "will"@en .
ex:desire rdf:type go:substance, ex:concept ;
    rdfs:label "desire"@en .
ex:thebook rdf:type go:substance, ex:concept ;
    rdfs:label "the book"@en .
ex:lifespan rdf:type go:substance, ex:concept ;
    rdfs:label "lifespan"@en .
ex:permission rdf:type go:substance, ex:concept ;
    rdfs:label "permission"@en .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/386-3> rdf:type owl:ontology .

ex:unspecifiedperson a go:agent .
ex:claimingevent a go:event ;
    go:hasagent ex:unspecifiedperson ;
    go:hastheme ex:otherthanseven .

ex:otherthanseven a go:concept .

ex:lyingevent a go:event ;
    go:hasagent ex:unspecifiedperson ;
    go:hastheme ex:god .

ex:contradictingevent a go:event ;
    go:hasagent ex:unspecifiedperson ;
    go:hastheme ex:god .

ex:god a go:deity ;
    rdfs:label "god almighty" .
go:event rdfs:label "رخداد"@fa.
go:agent rdfs:label "عامل"@fa.