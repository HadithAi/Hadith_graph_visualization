ex:OurLord rdf:type go:God ;
    go:hasAttribute ex:GloriousAndExalted ;
    ex:isIndependentFromNecessity ex:CreationEvent .

ex:CreationEvent rdf:type go:Creation ;
    go:HasAgent ex:OurLord ;
    go:HasTheme ex:Creatures .

ex:Creatures rdf:type go:Substance .

ex:GloriousAndExalted rdf:type go:Attribute ;
    rdfs:label "Glorious and Exalted"@en .
