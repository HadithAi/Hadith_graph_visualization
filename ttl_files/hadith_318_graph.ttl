    go:HasAttribute ex:GlorifiedAndExalted .

ex:GlorifiedAndExalted a go:Quality .

ex:CharacteristicsOfCreation a go:Quality ;
    go:describes ex:Creation .

ex:Creation a go:LivingBeing .

ex:StateAboveAttributes a go:Event , go:State ;
    go:HasAgent ex:OurLord ;
    go:HasTheme ex:CharacteristicsOfCreation ;
    go:description "The Lord is above the characteristics of His creation." .
