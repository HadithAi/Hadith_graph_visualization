
@prefix : <http://etratnet.ir/graph/> .
@prefix ex: <http://etratnet.ir/graph/proposition/295#> .
@prefix bt: <http://borhan-onto.ir/ontology/time#> .
@prefix go: <http://etratnet.ir/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/295-1> rdf:type owl:Ontology .

ex:Munha a go:Person .
ex:God a go:Deity .

ex:MunhaKnowledgeOfGod a go:Knowledge ;
    go:hasAgent ex:Munha ;
    go:hasTheme ex:God ;
    go:hasProperty ex:InfiniteKnowledge .

ex:InfiniteKnowledge a go:PropertyValue ;
    go:property go:hasEnd ;
    go:value "false"^^xsd:boolean .
@prefix : <http://etratnet.ir/graph/> .
@prefix ex: <http://etratnet.ir/graph/proposition/295#> .
@prefix bt: <http://borhan-onto.ir/ontology/time#> .
@prefix go: <http://etratnet.ir/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/295-2> rdf:type owl:Ontology .

ex:sayingInPrayer a go:Event ;
  go:HasTheme ex:phraseInPrayer ;
  go:HasContext ex:prayer .

ex:phraseInPrayer a go:concept ;
  go:hasLiteral "Praise be to God, the ultimate of His satisfaction."^^xsd:string .

ex:God a go:Substance ;
  go:hasRoleInPhrase "SubjectOfPraise" .

ex:prayer a go:Event .

ex:sayingInPrayer go:HasAgent ex:speaker .

ex:speaker a go:person .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.