<http://etratnet.ir/graph/ontology/269> rdf:type go:ontology ;
        go:hascontent "9. ـ در تفسير آيه شريفه «ديدگان او را در نمى‌يابند» ـ: «مقصود، در ميان گرفتن وهم‌ها [و انديشه‌ها] است. آيا نمى‌بينى كه مى‌فرمايد: «ديده‌هايى از جانب خداوندگارتان براى شما آمده»؟ مقصود بينايى چشم‌ها نيست. «پس هر كه از روى بصيرت ديد به سود اوست»؛ مقصود، ديدن با چشم نيست. «و هر كه ديده بر هم گذاشت به زيان خود كرده است»؛ مقصود، كورى چشم‌ها نيست، بلكه مقصود در ميان گرفتن وهم و انديشه است؛ چنان كه گفته مى‌شود: فلانى به شعر بيناست [= شعرشناس است]، فلانى در فقه بيناست، فلانى در درهم بيناست، و فلانى در پارچه و لباس بيناست، خدا با شكوه‌تر از آن است كه با چشم ديده شود»."@fa,
        "9.  عَنْ أَبِي عَبْدِ اللّه‌ِ عليه‌السلام فِي قَوْلِهِ: «لاَ تُدْرِكُهُ الْأَبْصارُ» قَالَ: «إِحَاطَةُ الْوَهْمِ؛ أَ لاَ تَرى إِلى قَوْلِهِ: «قَدْ جاءَكُمْ بَصائِرُ مِنْ رَبِّكُمْ»؟ لَيْسَ يَعْنِي بَصَرَ الْعُيُونِ «فَمَنْ أَبْصَرَ فَلِنَفْسِهِ»: لَيْسَ يَعْنِي مِنَ الْبَصَرِ بِعَيْنِهِ «وَمَنْ عَمِىَ فَعَلَيْها»: لَيْسَ يَعْنِي عَمَى الْعُيُونِ، إِنَّمَا عَنى إِحَاطَةَ الْوَهْمِ، كَمَا يُقَالُ: فُلاَنٌ بَصِيرٌ بِالشِّعْرِ، وَفُلاَنٌ بَصِيرٌ بِالْفِقْهِ، وَفُلاَنٌ بَصِيرٌ بِالدَّرَاهِمِ، وَفُلاَنٌ بَصِيرٌ بِالثِّيَابِ، اللّه‌ُ أَعْظَمُ مِنْ أَنْ يُرى بِالْعَيْنِ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/269-1> rdf:type owl:ontology .

ex:phrase_latudrikhual-absar a go:phrase ;
    go:label "لا تُدركه الأبصار" .

ex:concept_ihatatal-wahm a go:concept ;
    go:label "إحاطة الوهم" .

ex:meaningevent1 a ex:meaningevent ;
    go:hasagent ex:phrase_latudrikhual-absar ;
    go:hastheme ex:concept_ihatatal-wahm .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/269-2> rdf:type owl:ontology .

ex:saying1 a go:event , ex:utterance ;
  go:hasagent ex:he ;
  go:hastheme ex:clearproofsstatement .

ex:clearproofsstatement a ex:quotedstatement ;
  ex:statementtext "indeed, clear proofs have come to you from your lord"^^xsd:string .

ex:clearproofs a go:substance ;
  ex:derivedfrom ex:lord .

ex:lord a go:agent .

ex:eyesight a go:quality .

ex:notequalrelation a ex:negationrelation ;
  ex:source ex:clearproofs ;
  ex:negatedmeaning ex:eyesight .

ex:he a go:agent .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:quality rdfs:label "ویژگی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/269-3> rdf:type owl:ontology .

ex:phrase1 a ex:phrase ;
    ex:hastext "فمن أبصر فلنفسه" ;
    go:hasinterpretation ex:interpretation1 .

ex:interpretation1 a go:interpretation ;
    go:interprets ex:phrase1 ;
    go:negatesliteralmeaning ex:literalsight ;
    go:affirmsfigurativemeaning ex:figurativeperception .

ex:literalsight a go:event ;
    go:hasagent ex:eye ;
    rdfs:label "literal sight with the eye"@en .

ex:figurativeperception a go:event ;
    go:hasagent ex:self ;
    rdfs:label "figurative perception for oneself"@en .

ex:eye a go:agent ;
    rdfs:label "eye"@en .

ex:self a go:agent ;
    rdfs:label "self"@en .
go:event rdfs:label "رخداد"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/269-4> rdf:type owl:ontology .

ex:phraseblindness a go:linguisticentity ;
    rdfs:label "ومن عمى فعليها"@ar ;
    rdfs:comment "the arabic phrase 'and from blindness upon it'" .

ex:eyeblindness a go:condition ;
    rdfs:label "eye blindness"@en ;
    rdfs:comment "physical blindness of the eyes" .

ex:negationmeaningevent a go:negationevent ;
    go:hastheme ex:phraseblindness ;
    go:negatesmeaning ex:eyeblindness ;
    rdfs:comment "an event representing that the phrase does not mean eye blindness." .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/269-5> rdf:type owl:ontology .

ex:phrase_not_perceived_by_eyes a go:concept ;
    go:means [ a go:meaning ;
               go:hastheme ex:encompassing_illusion ;
               go:hasagent ex:perception_negated_by_eyes
             ] .

ex:encompassing_illusion a go:concept .
ex:perception_negated_by_eyes a go:eventinstance ;
    go:hasagent ex:eyes ;
    go:hastheme ex:phrase_not_perceived_by_eyes ;
    go:hasoutcome ex:false_perception .

ex:eyes a go:substance .
ex:illusion a go:substance .

ex:false_perception a go:concept ;
    go:contrastswith ex:sensory_sight .

ex:sensory_sight a go:substance .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/269-6> rdf:type owl:ontology .

ex:fulaan a go:individual .
ex:poetry a go:subject .

ex:insightevent1 a go:intellectualinsight ;
    go:hasagent ex:fulaan ;
    go:hastheme ex:poetry ;
    go:description "intellectual insight or understanding in poetry"^^xsd:string .

ex:expression1 a ex:expression ;
    ex:denotes ex:insightevent1 ;
    ex:expressiontext "فُلان بصير بالشعر"^^xsd:string ;
    ex:contrastswith go:sensorysight .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/269-7> rdf:type owl:ontology .

# the expression
ex:expression_fulan_is_insightful a go:expression ;
    go:refersto ex:event_insight_in_jurisprudence .

# the event of insight
ex:event_insight_in_jurisprudence a go:state ;
    go:hasagent ex:entity_fulan ;
    go:hastheme ex:jurisprudence ;
    go:hasquality ex:intellectual_insight ;
    go:nothasquality ex:sensory_sight .

# entities
ex:entity_fulan a go:person .
ex:jurisprudence a go:knowledgedomain .
ex:intellectual_insight a go:cognitivequality .
ex:sensory_sight a go:sensoryquality .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/269-8> rdf:type owl:ontology .

ex:fulán a go:person ;
    rdfs:label "fulán" .

ex:money a go:substance ;
    rdfs:label "money" .

ex:understandingmoneyevent a go:event , ex:mentalperceptionevent ;
    go:hasagent ex:fulán ;
    go:hastheme ex:money ;
    rdfs:comment "the event representing fulán's mental understanding of money, i.e. being perceptive about money." .

ex:sensorysightevent a go:event , ex:sensoryperceptionevent ;
    go:hasagent ex:fulán ;
    go:hastheme ex:money ;
    rdfs:comment "literal sensory sight, which is not the meaning intended by the expression." .

ex:expression_fulanperceptive a ex:expression ;
    ex:denotes ex:understandingmoneyevent ;
    rdfs:label "fulán is perceptive about money" .

ex:expression_fulanperceptive ex:contrastwith ex:sensorysightevent .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/269-9> rdf:type owl:ontology .

ex:expression1 a go:linguisticexpression ;
    go:hasagent ex:person_fulan ;
    go:hastheme ex:clothes ;
    go:denotes ex:understandingevent .

ex:person_fulan a go:agent .

ex:clothes a go:substance .

ex:understandingevent a go:event ;
    go:referstosensorydomain ex:clothes ;
    go:ismetaphoricalmeaning true .

ex:sensorysight a go:event ;
    go:isliteralmeaning true .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/269-10> rdf:type owl:ontology .

ex:god a go:deity .

ex:visiblebyeye a go:property .

ex:comparisongreatness1 a go:comparisonevent ;
    go:hasagent ex:god ;
    go:comparedto ex:visiblebyeye ;
    go:comparisontype go:greaterthan .