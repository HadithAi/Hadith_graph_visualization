    @prefix bt: <http://borhan-onto.ir/ontology/time#> .
    @prefix go: <http://etratnet.ir/ontology#> .
    @prefix owl: <http://www.w3.org/2002/07/owl#> .
    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix xml: <http://www.w3.org/xml/1998/namespace> .
    @prefix xsd: <http://www.w3.org/2001/xmlschema#> .
    @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
    @prefix time: <http://www.w3.org/2006/time#> .
    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/385> rdf:type go:ontology ;
        go:hascontent "1.  «هيچ چيزى در زمين و در آسمان نباشد مگر با اين هفت چيز: با خواست و اراده و اندازه‌گيرى و حُكم و پروانه و نبشته و مدت [يا سررسيد]. پس، هر كه پندارد كه مى‌تواند يكى از اينها را از قلم بيندازد هر آينه كافر شده است»."@fa,
        "1.  عَنْ أَبِي عَبْدِ اللّه‌ِ عليه‌السلام، أَنَّهُ قَالَ: «لاَ يَكُونُ شَيْءٌ فِي الْأَرْضِ وَلاَ فِي السَّمَاءِ إِلاَّ بِهذِهِ الْخِصَالِ السَّبْعِ: بِمَشِيئَةٍ، وَإِرَادَةٍ، وَقَدَرٍ، وَقَضَاءٍ، وَإِذْنٍ، وَكِتَابٍ، وَأَجَلٍ، فَمَنْ زَعَمَ أَنَّهُ يَقْدِرُ عَلى نَقْضِ وَاحِدَةٍ، فَقَدْ كَفَرَ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/385-1> rdf:type owl:ontology .

ex:thing rdf:type go:object .
ex:earth rdf:type go:location .
ex:sky rdf:type go:location .

ex:will rdf:type go:attribute ;
    rdfs:label "will" .
ex:desire rdf:type go:attribute ;
    rdfs:label "desire" .
ex:fate rdf:type go:attribute ;
    rdfs:label "fate" .
ex:decree rdf:type go:attribute ;
    rdfs:label "decree" .
ex:permission rdf:type go:attribute ;
    rdfs:label "permission" .
ex:book rdf:type go:attribute ;
    rdfs:label "book" .
ex:term rdf:type go:attribute ;
    rdfs:label "term" .

ex:thing go:existsin ex:earth .
ex:thing go:existsin ex:sky .

ex:thing go:hasattribute ex:will, ex:desire, ex:fate, ex:decree, ex:permission, ex:book, ex:term .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/385-2> rdf:type owl:ontology .

# entities
ex:whoever a go:agent ;
    go:label "whoever" .

ex:seventraits a go:collection ;
    go:label "these seven traits" ;
    go:hasmember ex:trait1, ex:trait2, ex:trait3, ex:trait4, ex:trait5, ex:trait6, ex:trait7 .

ex:trait1 a go:quality .
ex:trait2 a go:quality .
ex:trait3 a go:quality .
ex:trait4 a go:quality .
ex:trait5 a go:quality .
ex:trait6 a go:quality .
ex:trait7 a go:quality .

# events
ex:claimevent a go:event ;
    go:hasagent ex:whoever ;
    go:hastheme ex:abilitytorefuteonetrait .

ex:abilitytorefuteonetrait a go:capability ;
    go:hastheme ex:onetrait .

ex:onetrait a go:quality ;
    go:partof ex:seventraits .

ex:becomingunbeliever a go:statechangeevent ;
    go:haspatient ex:whoever ;
    go:hascause ex:claimevent ;
    go:label "becoming unbeliever" .
go:event rdfs:label "رخداد"@fa.
go:quality rdfs:label "ویژگی"@fa.
go:agent rdfs:label "عامل"@fa.