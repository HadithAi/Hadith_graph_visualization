<http://etratnet.ir/ontology/279-1> rdf:type owl:Ontology .

ex:Quran a go:Substance ;
    rdfs:label "Quran"@en .

ex:Attribute a go:AbstractEntity ;
    rdfs:label "Attribute"@en .

ex:InquiryAboutAttribute a go:Event ;
    rdfs:label "Inquiry About Attribute"@en ;
    go:HasTheme ex:Attribute .

ex:ProhibitionOfExceedingQuran a go:ProhibitionEvent ;
    rdfs:label "Prohibition of exceeding Quran in attribute inquiry"@en ;
    go:HasAgent ex:Person ;
    go:HasTheme ex:InquiryAboutAttribute ;
    go:HasLimit ex:Quran .

ex:Person a go:Person ;
    rdfs:label "Person"@en .
