@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/327-2> rdf:type owl:Ontology .

ex:GodAlmighty a go:Substance ;
  rdfs:label "God Almighty"@en .

ex:HighnessOfEssence a go:state ;
  rdfs:label "Highness of Essence"@en ;
  go:hasTheme ex:GodAlmighty .

ex:Oneness a go:state ;
  rdfs:label "Oneness"@en ;
  go:hasTheme ex:GodAlmighty .

ex:UnificationEvent a go:Event ;
  rdfs:label "Unification by Oneness"@en ;
  go:hasAgent ex:GodAlmighty ;
  go:hasTheme ex:Oneness .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
