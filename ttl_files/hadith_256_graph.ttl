    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/256> rdf:type go:ontology ;
        go:hascontent "7.  «پادشاهى بلندپايه در يكى از نشست‌هايش درباره خداوند بزرگ و بلندمرتبه به بحث پرداخت، و پس از آن ناپديد گشت و معلوم نشد كجا رفت»."@fa,
        "7.  عَنْ أَبِي عَبْدِ اللّه‌ِ عليه‌السلام، قَالَ: «إِنَّ مَلِكاً عَظِيمَ الشَّأْنِ كَانَ فِي مَجْلِسٍ لَهُ، فَتَنَاوَلَ الرَّبَّ تَبَارَكَ وَتَعَالى، فَفُقِدَ، فَمَا يُدْرى أَيْنَ هُوَ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/256-1> rdf:type owl:ontology .

ex:event1 a go:state ;
    go:hastheme ex:king1 .

ex:king1 a go:person ;
    go:hasquality ex:greatimportance ;
    go:haslocation ex:council1 .

ex:greatimportance a go:quality .

ex:council1 a go:organization .
go:quality rdfs:label "ویژگی"@fa.
go:organization rdfs:label "سازمان"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/256-2> rdf:type owl:ontology .

ex:event1 a go:consumeevent ;
    go:hasagent ex:lord ;
    go:hastheme ex:greathonorableking .

ex:lord a go:substance ;
    go:title "الرب تبارك وتعالى" .

ex:greathonorableking a go:substance ;
    go:title "الملك العظيم الشأن" .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/256-3> rdf:type owl:ontology .

ex:thegreatking a go:person ;
    rdfs:label "the great king"@en .

ex:deathevent1 a go:event ;
    rdfs:label "death of the great king"@en ;
    go:hastheme ex:thegreatking .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/256-4> rdf:type owl:ontology .

ex:unknownlocationevent a go:event ;
    go:hastheme ex:greatking .

ex:greatking a go:person ;
    go:hastitle "great king" .
go:event rdfs:label "رخداد"@fa.