@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/328-10> rdf:type owl:ontology .

# entity: allah
ex:allah rdf:type ex:deity ;
        ex:hasattribute ex:alaziz , ex:alhakim .

# attributes as entities
ex:alaziz rdf:type ex:attribute ;
          ex:label "al-aziz" .
	ex:alhakim rdf:type ex:attribute ;
          ex:label "al-hakim" .

# statement about exclusivity of allah as sole deity
# represented as a universal negative over other gods
_:exclusivity a ex:uniquenessconstraint ;
              ex:subject ex:allah ;
              ex:predicate ex:isonlydeity .


