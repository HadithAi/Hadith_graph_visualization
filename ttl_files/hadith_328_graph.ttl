ex:Almighty a go:Attribute .
ex:Wise a go:Attribute .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/328-10> rdf:type owl:Ontology .

# Entity: Allah
ex:Allah rdf:type ex:Deity ;
        ex:hasAttribute ex:AlAziz , ex:AlHakim .

# Attributes as entities
ex:AlAziz rdf:type ex:Attribute ;
          ex:label "Al-Aziz" .
	ex:AlHakim rdf:type ex:Attribute ;
          ex:label "Al-Hakim" .

# Statement about exclusivity of Allah as sole deity
# Represented as a universal negative over other gods
_:exclusivity a ex:UniquenessConstraint ;
              ex:subject ex:Allah ;
              ex:predicate ex:isOnlyDeity .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/328-11> rdf:type owl:Ontology .

ex:StatementOfGodsDescent a go:CommunicationEvent ;
    go:HasAgent ex:UnknownSpeaker ;
    go:HasTheme ex:GodDescendsStatement ;
    go:HasEffect ex:AttributesDeficiencyOrIncreaseToGod .

ex:GodDescendsStatement a go:Proposition ;
    go:HasTheme ex:God , ex:Descend ;
    go:asserts true .

ex:God a go:Substance ;
    go:name "Allah, Blessed and Exalted" .

ex:Descend a go:Concept .

ex:AttributesDeficiencyOrIncreaseToGod a go:AttributionEvent ;
    go:HasTheme ex:God ;
    go:HasProperty ex:DeficiencyOrIncrease .

ex:DeficiencyOrIncrease a go:Property ;
    go:description "Deficiency or Increase attributed to God" .

ex:UnknownSpeaker a go:Agent ;
    go:name "Unknown Speaker" .
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/328-12> rdf:type owl:Ontology .

ex:MovingThing a go:Substance ;
    go:hasQuality ex:Movement .

ex:Movement a go:Event .

# The general movement instance
ex:MovementInstance a go:Event ;
    go:HasTheme ex:MovingThing ;
    go:Requires [ a go:hasAgent ; go:agentRole ex:Mover ] , [ a go:hasMedium ; go:mediumRole ex:Medium ] .

ex:Mover a go:agent .
ex:Medium a go:medium .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


