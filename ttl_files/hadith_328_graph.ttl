@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/328-1> rdf:type owl:Ontology .

ex:God_Almighty a go:Deity ;
    rdfs:label "God Almighty" .

ex:Lowest_Heaven a go:Location ;
    rdfs:label "the lowest heaven" .

ex:Descend_Event a go:Event ;
    go:HasAgent ex:God_Almighty ;
    go:HasGoal ex:Lowest_Heaven ;
    go:hasPolarity go:Negative .
go:Event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/328-2> rdf:type owl:Ontology .

ex:GodAlmighty a go:Deity ;
  rdfs:label "God Almighty" .

ex:DescendEvent a go:Event ;
  rdfs:label "descend" ;
  go:HasAgent ex:GodAlmighty .

ex:NeedToDescend a go:Property ;
  go:appliesTo ex:GodAlmighty ;
  go:describes ex:DescendEvent ;
  go:isNecessary false .
go:Event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/328-3> rdf:type owl:Ontology .

ex:God a go:deity ;
    go:hasTitle "Allah سبحانه وتعالى" .

ex:ViewOfGod a go:concept ;
    go:relatesTo ex:God ;
    ex:hasProperty ex:SameFromNearAndFar .

ex:Near a go:position .
ex:Far a go:position .

ex:SameFromNearAndFar a go:property ;
    go:appliesTo ex:ViewOfGod ;
    go:relatesPositions ex:Near , ex:Far .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/328-4> rdf:type owl:Ontology .

ex:Allah a go:DivineEntity ;
    rdfs:label "Allah"@en .

ex:Person a go:Agent ;
    rdfs:label "Person"@en .

ex:Distantness a go:Event ;
    rdfs:label "being distant"@en .

# Negation of distantness: no person is distant from Allah
# Instead of asserting distancedness, we assert closeness

ex:IsCloseTo a go:ObjectProperty ;
    rdfs:label "is close to"@en ;
    rdfs:comment "Inverse of is distant from"@en .

# For all persons, person is close to Allah

_:bnode a owl:AllDifferent ;
    owl:distinctMembers ( ex:Person ) .

# Since we can not write universal restrictions easily here, we state for example

ex:Person1 a ex:Person ;
    ex:IsCloseTo ex:Allah .

# This triple captures the meaning that personal entities (like Person1) are close to Allah, meaning they are not distant.

go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/328-5> rdf:type owl:Ontology .

ex:ApproachEvent1 a go:Event ;
