<http://etratnet.ir/graph/ontology/299> rdf:type go:ontology ;
        go:hascontent "1. امام باقر عليه‌السلام درباره صفت خداى ديرين فرمود: «او يكتاست، بى‌نياز است، ذاتش يگانه [و بسيط]است، و كثرت و گوناگونى در ذاتش راه ندارد». عرض كردم: فدايت گردم، گروهى از عراقيان مى‌گويند او به غير از آن چيزى مى‌شنود كه مى‌بيند، و به جز از آن چيزى مى‌بيند كه مى‌شنود [شنيدن و ديدن عين ذات او نيست بلكه جهات مختلف در اوست]. حضرت فرمود: «بيجا مى‌گويند و به كجراهه رفته‌اند و مانند كرده‌اند. خدا برتر از اين حرف‌هاست. او شنوا و بيناست. با همان چيزى مى‌شنود كه مى‌بيند و با همان چيزى مى‌بيند كه مى‌شنود». عرض كردم: مى‌گويند او بيناست به همان معنايى كه خود از ديدن مى‌فهمند. فرمود: «خدا برتر است. اين گونه ديدن از ويژگى‌هاى مخلوق است و خدا مخلوق نيست»."@fa,
        "1. عَنْ أَبِي جَعْفَرٍ عليه‌السلام أَنَّهُ قَالَ فِي صِفَةِ الْقَدِيمِ: «إِنَّهُ وَاحِدٌ، صَمَدٌ، أَحَدِيُّ الْمَعْنى، لَيْسَ بِمَعَانِي كَثِيرَةٍ مُخْتَلِفَةٍ». قَالَ: قُلْتُ: جُعِلْتُ فِدَاكَ، يَزْعُمُ قَوْمٌ مِنْ أَهْلِ الْعِرَاقِ أَنَّهُ يَسْمَعُ بِغَيْرِ الَّذِي يُبْصِرُ، وَيُبْصِرُ بِغَيْرِ الَّذِي يَسْمَعُ؟ قَالَ: فَقَالَ: «كَذَبُوا، وَأَلْحَدُوا، وَشَبَّهُوا، تَعَالَى اللّه‌ُ عَنْ ذلِكَ؛ إِنَّهُ سَمِيعٌ بَصِيرٌ، يَسْمَعُ بِمَا يُبْصِرُ، وَيُبْصِرُ بِمَا يَسْمَعُ». قَالَ: قُلْتُ: يَزْعُمُونَ أَنَّهُ بَصِيرٌ عَلى مَا يَعْقِلُونَهُ؟ قَالَ: فَقَالَ: «تَعَالَى اللّه‌ُ، إِنَّمَا يُعْقَلُ مَا كَانَ بِصِفَةِ الْمَخْلُوقِ وَلَيْسَ اللّه‌ُ كَذلِكَ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/299-1> rdf:type owl:ontology .

ex:alqadeem a go:substance ;
    go:hasquality ex:qualityoneness , ex:qualitysteadfastness , ex:qualityunivocality .

ex:qualityoneness a go:quality ;
    go:qualityvalue "one" .

ex:qualitysteadfastness a go:quality ;
    go:qualityvalue "steadfast" .

ex:qualityunivocality a go:quality ;
    go:qualityvalue "univocal" .
go:substance rdfs:label "شی"@fa.
go:quality rdfs:label "ویژگی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/299-2> rdf:type owl:ontology .

ex:alqadim a go:linguisticexpression ;
    go:hasmeaningcount "few" .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/299-3> rdf:type owl:ontology .

ex:iraq a go:geopoliticalentity .

ex:god a go:deity .

ex:peoplefromiraq a go:persongroup ;
    go:hasorigin ex:iraq .

ex:claimevent1 a go:event ;
    go:hasagent ex:peoplefromiraq ;
    go:hastheme ex:claimcontent1 .

ex:claimcontent1 a go:event ;
    go:hasagent ex:god ;
    ex:property "hears without seeing" ;
    ex:explanation "god hears without seeing" .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/299-4> rdf:type owl:ontology .

ex:claimevent a go:event ;
    go:hasagent ex:peoplefromiraq ;
    go:hastheme ex:claimcontent .

ex:peoplefromiraq a go:substance ;
    rdfs:label "people from the inhabitants of iraq" .

ex:god a go:substance ;
    rdfs:label "god" .

ex:claimcontent a go:conceptualentity ;
    go:expresses "god sees without that which hears" ;
    go:referto ex:god .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/299-5> rdf:type owl:ontology .

ex:claim1 a go:statement ;
  go:hasquality ex:falsehood , ex:atheism , ex:metaphor .

ex:falsehood a go:quality .
ex:atheism a go:quality .
ex:metaphor a go:quality .
go:quality rdfs:label "ویژگی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/299-6> rdf:type owl:ontology .

ex:allah a go:agent ;
    go:hasquality ex:allhearing , ex:allseeing .

ex:allhearing a go:quality ;
    rdfs:label "all-hearing"@en .

ex:allseeing a go:quality ;
    rdfs:label "all-seeing"@en .
go:quality rdfs:label "ویژگی"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/299-7> rdf:type owl:ontology .

ex:event1 a go:event ;
    go:hasagent ex:god_almighty ;
    go:hasinstrument ex:perception .

ex:god_almighty a go:agent ;
    rdfs:label "الله تعالى"@ar .

ex:perception a go:concept ;
    go:hasdefinition "what is perceived" .
go:event rdfs:label "رخداد"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/299-8> rdf:type owl:ontology .

ex:allah a go:namedentity ;
    go:hastitle "allah almighty" .

ex:perceptionevent a go:event ;
    go:hasagent ex:allah ;
    go:hastheme ex:thatwhichisheard .

ex:thatwhichisheard a go:perceptualinput ;
    go:hasmodality go:hearing .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/299-9> rdf:type owl:ontology .

ex:eventclaim a go:event ;
    go:hasagent ex:people ;
    go:hastheme ex:eventgodallseeing .

ex:people a go:substance ;
    go:haslabel "people"@en .

ex:god a go:substance ;
    go:haslabel "god"@en ;
    go:hasattribute ex:allseeingofunderstanding .

ex:eventgodallseeing a go:event ;
    go:hasagent ex:god ;
    go:hasproperty ex:allseeingofunderstanding .

ex:allseeingofunderstanding a go:property ;
    go:qualifies ex:god ;
    go:relatesto ex:whattheyunderstand .

ex:whattheyunderstand a go:concept ;
    go:hasagent ex:people .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/299-10> rdf:type owl:ontology .

ex:event_saygod a go:event ;
    go:hasagent ex:god_almighty ;
    go:hastheme ex:event_utterance1 .

ex:god_almighty a go:deity .

ex:event_utterance1 a go:utterance ;
    go:hascontent ex:concept_comprehension .

ex:concept_comprehension a go:concept ;
    go:expresses "only what was in the nature of the created can be comprehended" .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/299-11> rdf:type owl:ontology .

ex:god a go:divinebeing ;
    go:nothasattribute ex:createdbeing .

ex:createdbeing a go:substanceclass .

# note: go:nothasattribute indicates negation of attribute possession as per ontology.
go:substance rdfs:label "شی"@fa.