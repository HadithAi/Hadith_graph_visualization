@prefix ex: <http://etratnet.ir/graph/proposition/363#> .
    @prefix bt: <http://borhan-onto.ir/ontology/time#> .
    @prefix go: <http://etratnet.ir/ontology#> .
    @prefix owl: <http://www.w3.org/2002/07/owl#> .
    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix xml: <http://www.w3.org/XML/1998/namespace> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
    @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


    @base <http://etratnet.ir/ontology/> .
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/363-1> rdf:type owl:Ontology .

ex:We a go:Group ;
    ex:isProofOf ex:God .

ex:God a go:Deity .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/363-2> rdf:type owl:Ontology .

ex:We a go:Agent ;
    rdfs:label "We"@en ;
    go:hasIdentity ex:BabAllah .

ex:BabAllah a go:Agent ;
    rdfs:label "Bab Allah"@en .
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/363-3> rdf:type owl:Ontology .

ex:We a go:Group ;
    ex:isTongueOf ex:God .

ex:God a go:Deity .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/363-4> rdf:type owl:Ontology .

ex:We a ex:Group ;
    ex:isIdentifiedAs ex:FaceOfGod .

ex:FaceOfGod a ex:Concept .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/363-5> rdf:type owl:Ontology .

ex:We a go:Agent ;
    ex:isEyesOf ex:God ;
    ex:within ex:Creation .

ex:God a go:Agent ;
    ex:isDivine true .

ex:Creation a go:Substance .
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/363-6> rdf:type owl:Ontology .

# Entities
ex:We a go:Person ;
    go:hasRole ex:RulerRole .

ex:God a go:Deity .

ex:Servants a go:Person .

# Event: Appointment or governance
ex:RulingEvent a go:Event ;
    go:HasAgent ex:We ;
    go:HasCause ex:God ;
    go:HasTheme ex:Servants .

# Role definition
ex:RulerRole a go:Role ;
    go:roleHeldBy ex:We ;
    go:roleAuthority ex:God ;
    go:roleOver ex:Servants .
go:Event rdfs:label "رخداد"@fa.