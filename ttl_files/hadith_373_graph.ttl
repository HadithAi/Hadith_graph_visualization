@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/373-5> rdf:type owl:Ontology .

ex:AbuAbdullah a go:Person ;
    go:label "Abu Abdullah (peace be upon him)" .

exevent:Speech1 a go:SpeechEvent ;
    go:HasAgent ex:AbuAbdullah ;
    go:HasTheme exevent:PredeterminationState .

exevent:PredeterminationState a go:State ;
    go:description "It was predetermined but not mentioned" ;
    go:hasProperty exevent:Predetermined, exevent:Unmentioned .

exevent:Predetermined a go:Property ;
    go:label "predetermined" .

exevent:Unmentioned a go:Property ;
    go:label "unmentioned" .