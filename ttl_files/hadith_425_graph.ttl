<http://etratnet.ir/ontology/425-4> rdf:type owl:Ontology .

ex:People a go:Agent ;
    rdfs:label "People" .

ex:Anger a go:Emotion ;
    rdfs:label "Anger" .

ex:LossOfControlInAnger a go:State ;
    go:HasAgent ex:People ;
    go:HasCause ex:Anger ;
    ex:isControlled false ;
    rdfs:label "Lack of control in anger" .
go:Agent rdfs:label "عامل"@fa.
