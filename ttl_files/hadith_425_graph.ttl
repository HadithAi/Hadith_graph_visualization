@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/425-3> rdf:type owl:Ontology .

ex:Contentment a go:State ;
    rdfs:label "Contentment" .

ex:Humans a go:Agent ;
    rdfs:label "Humans" .

ex:ControlEvent a go:Event ;
    go:HasAgent ex:Humans ;
    go:HasTheme ex:Contentment ;
    rdfs:label "Control over contentment" .

ex:NegationEvent a go:Event ;
    go:HasTheme ex:ControlEvent ;
    rdfs:label "Negation of control" .
go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.
