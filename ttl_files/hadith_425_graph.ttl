@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/425-6> rdf:type owl:Ontology .

ex:Wakefulness a go:State ;
    rdfs:label "Wakefulness"@en .

ex:People a go:Agent ;
    rdfs:label "People"@en .

ex:NoPowerInWakefulness a go:Event ;
    rdfs:comment "An event expressing that people have no power during wakefulness"@en ;
    go:HasAgent ex:People ;
    go:HasState ex:Wakefulness ;
    go:HasTheme ex:Power .

ex:Power a go:Substance ;
    rdfs:label "Power"@en .

ex:NoPowerInWakefulness go:hasPolarity "negative"^^xsd:boolean .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.