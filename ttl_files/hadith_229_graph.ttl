@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/229-8> rdf:type owl:ontology .

# entities
ex:noone a go:person .
ex:sensitiveconscioussoul a go:substance ;
    go:hasproperty ex:sensitivity ;
    go:hasproperty ex:consciousness .
ex:anything a go:object .
ex:cause a go:object .

# event: possession
ex:possessionevent a go:event ;
    go:hasagent ex:noone ;
    go:hastheme ex:anything , ex:cause ;
    go:pertainsto ex:creationofsensitiveconscioussoul .

# creation event
ex:creationofsensitiveconscioussoul a go:event ;
    go:hastheme ex:sensitiveconscioussoul .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/229-9> rdf:type owl:ontology .

ex:god a go:agent ;
    rdfs:label "god" .

ex:souls a go:substance ;
    rdfs:label "souls" .

ex:bodies a go:substance ;
    rdfs:label "bodies" .

ex:creationevent a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:souls, ex:bodies ;
    rdfs:label "creation of souls and bodies" .

ex:god ex:uniquecreatorof ex:creationevent .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/229-10> rdf:type owl:ontology .

ex:negationoftwosimilarities a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:similarityofbodies, ex:similarityofspirits .

ex:god a go:substance .

ex:similarityofbodies a go:quality ;
    go:relatedto ex:bodies .

ex:similarityofspirits a go:quality ;
    go:relatedto ex:spirits .

ex:bodies a go:substance .

ex:spirits a go:substance .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:quality rdfs:label "ویژگی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/229-11> rdf:type owl:ontology .

:person1 a go:agent ;
    rdfs:label "person" .

:bodies a go:substance ;
    rdfs:label "bodies" .

:spirits a go:substance ;
    rdfs:label "spirits" .

:god a go:agent ;
    rdfs:label "god" .

:negationevent1 a ex:negationevent ;
    go:hasagent :person1 ;
    go:hastheme :bodies , :spirits ;
    ex:negationabout :god .

:knowledgeevent1 a ex:knowledgeevent ;
    go:hasagent :person1 ;
    go:hastheme :god ;
    ex:knowledgeby :god .

:conditionalrelation1 a ex:conditionalrelation ;
    ex:ifevent :negationevent1 ;
    ex:thenevent :knowledgeevent1 .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/229-12> rdf:type owl:ontology .

ex:human a go:agent ;
    rdfs:label "human" .

ex:god a go:agent ;
    rdfs:label "god" .

ex:spirit a go:substance ;
    rdfs:label "spirit" .

ex:body a go:substance ;
    rdfs:label "body" .

ex:light a go:substance ;
    rdfs:label "light" .

ex:compareevent a go:process ;
    rdfs:label "comparison of god by human" ;
    go:hasagent ex:human ;
    go:hastheme ex:god ;
    go:hastheme ex:spirit, ex:body, ex:light .

ex:notknowevent a go:process ;
    rdfs:label "human does not know god" ;
    go:hasagent ex:human ;
    go:hastheme ex:god ;
    go:haslogicalnegation ex:know .

ex:know a go:process ;
    rdfs:label "knowing god" .

# the condition linking compareevent causing notknowevent
ex:compareevent go:hasconsequence ex:notknowevent .
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.