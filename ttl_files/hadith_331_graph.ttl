@prefix ex: <http://etratnet.ir/graph/proposition/331#> .
    @prefix bt: <http://borhan-onto.ir/ontology/time#> .
    @prefix go: <http://etratnet.ir/ontology#> .
    @prefix owl: <http://www.w3.org/2002/07/owl#> .
    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix xml: <http://www.w3.org/XML/1998/namespace> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
    @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


    @base <http://etratnet.ir/ontology/> .
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/331-1> rdf:type owl:Ontology .

ex:event1 rdf:type go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Throne ;
    go:HasPlace ex:PlaceWithoutPlace .

ex:God rdf:type go:Substance .
ex:Throne rdf:type go:Substance .
ex:PlaceWithoutPlace rdf:type go:place .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/331-2> rdf:type owl:Ontology .

ex:DivineDescendEvent a go:DivineAction ;
    rdfs:label "God descends" ;
    go:HasAgent ex:God ;
    go:HasDestination ex:LowestHeaven ;
    ex:occursDuring ex:LatterHalfOfNight .

ex:God a go:Deity ;
    rdfs:label "God" .

ex:LowestHeaven a go:Location ;
    rdfs:label "lowest heaven" .

ex:LatterHalfOfNight a go:TemporalInterval ;
    rdfs:label "latter half of the night" ;
    ex:isPartOf ex:Night .

ex:Night a go:TemporalInterval ;
    rdfs:label "night" .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/331-3> rdf:type owl:Ontology .

ex:God a go:Agent , go:Substance .
ex:EveOfArafah a go:Time .
ex:HisPlace a go:Place .

ex:descendEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTime ex:EveOfArafah .

ex:returnEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasDestination ex:HisPlace .
go:Event rdfs:label "رخداد"@fa.
go:Time rdfs:label "زمان"@fa.
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/331-4> rdf:type owl:Ontology .

ex:Air a go:Substance ;
    go:hasCharacteristic ex:Subtle .

ex:Subtle a go:Characteristic .

ex:EnvelopingEvent a go:Event ;
    go:hasAgent ex:Air ;
    go:hasTheme ex:Everything ;
    go:hasExtent "according to its extent" .

ex:Everything a go:Substance .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/331-5> rdf:type owl:Ontology .

ex:God_Almighty a go:Deity .
ex:His_Place a go:Place .

ex:Knowing_Place_Event a go:Event ;
    go:hasAgent ex:God_Almighty ;
    go:hasTheme ex:His_Place ;
    a go:Knowing .

ex:Estimating_Place_Event a go:Event ;
    go:hasAgent ex:God_Almighty ;
    go:hasTheme ex:His_Place ;
    a go:Estimating ;
    go:hasQuality ex:Best_Estimation .

ex:Best_Estimation a go:Quality ;
    rdfs:label "Best Estimation" .
go:Event rdfs:label "رخداد"@fa.
go:Quality rdfs:label "ویژگی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/331-6> rdf:type owl:Ontology .

ex:Allah a go:Agent ;
  rdfs:label "Allah"@en .

ex:LowestHeaven a go:Place ;
  rdfs:label "Lowest Heaven"@en .

ex:Throne a go:Place ;
  rdfs:label "Throne"@en .

ex:ExistenceInLowestHeaven a go:Event ;
  go:HasAgent ex:Allah ;
  go:HasLocation ex:LowestHeaven .

ex:ExistenceOnThrone a go:Event ;
  go:HasAgent ex:Allah ;
  go:HasLocation ex:Throne .

ex:ExistenceInLowestHeaven ex:equivalentState ex:ExistenceOnThrone .
go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/331-7> rdf:type owl:Ontology .

ex:AllThings a go:Substance .
ex:God a go:Substance .

ex:BelongingToGod a go:State ;
    go:HasTheme ex:AllThings ;
    go:HasAgent ex:God ;
    go:hasAspect ex:Knowledge , ex:Power , ex:Dominion , ex:Comprehensiveness .

ex:Knowledge a go:Attribute .
ex:Power a go:Attribute .
ex:Dominion a go:Attribute .
ex:Comprehensiveness a go:Attribute .
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/331-8> rdf:type owl:Ontology .

ex:sayingEvent a go:Event ;
  go:HasAgent ex:God ;
  go:HasTheme ex:secretCouncilStatement .

ex:God a go:Agent ;
  rdfs:label "God Almighty" .

ex:secretCouncilStatement a go:statement ;
  go:hasText "There is no secret counsel by three except that He is the fourth among them." .
go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.