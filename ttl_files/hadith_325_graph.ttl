@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/325-95> rdf:type owl:Ontology .

ex:names a go:entity ;
    rdfs:label "Names" .

ex:allNames a go:entity ;
    rdfs:label "All Names" ;
    rdfs:subClassOf ex:names .

ex:we a go:agent ;
    rdfs:label "We" .

ex:eventInstance1 a go:Event ;
    rdfs:label "Gathering Event" ;
    go:HasAgent ex:we ;
    go:HasTheme ex:allNames ;
    ex:gatheringStatus "incomplete" .
go:Event rdfs:label "رخداد"@fa.
@prefix : <http://etratnet.ir/graph/> .
@prefix ex: <http://etratnet.ir/graph/proposition/325#> .
@prefix bt: <http://borhan-onto.ir/ontology/time#> .
@prefix go: <http://etratnet.ir/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/325-96> rdf:type owl:Ontology .

# Entities
ex:Consideration a go:Concept ;
    go:hasDisplayName "consideration" .
ex:You a go:Agent ;
    go:hasDisplayName "you" .
ex:PresentedContent a go:Concept ;
    go:hasDisplayName "presented content" .

# Events
ex:PresentationEvent a go:Process, ex:eventInstance ;
    go:HasTheme ex:PresentedContent ;
    go:HasRecipient ex:You .

ex:ConsiderationLimitationEvent a go:Process, ex:eventInstance ;
    go:HasTheme ex:Consideration ;
    ex:limitedTo ex:PresentedContent .

# Relationships
ex:PresentedContent ex:hasSourceEvent ex:PresentationEvent .
go:Agent rdfs:label "عامل"@fa.
@prefix : <http://etratnet.ir/graph/> .
@prefix ex: <http://etratnet.ir/graph/proposition/325#> .
@prefix bt: <http://borhan-onto.ir/ontology/time#> .
@prefix go: <http://etratnet.ir/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/325-97> rdf:type owl:Ontology .

ex:God a go:Substance ;
    ex:hasRole ex:HelperRole .

ex:HelperRole a ex:Role ;
    ex:roleHolder ex:God ;
    ex:roleRecipients ex:You, ex:Us ;
    ex:roleInvolves ex:GuidanceEvent, ex:ProvisionEvent .

ex:You a go:agent .
ex:Us a go:agent .

ex:GuidanceEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Us ;
    ex:description "Guiding us" .

ex:ProvisionEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Us ;
    ex:description "Providing for us" .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.