<http://etratnet.ir/ontology/325-31> rdf:type owl:Ontology .

ex:Allah a go:divineEntity ;
	go:HasProperty ex:IgnorantCondition, ex:WeakCondition .

ex:ThatKnowledge a go:Substance .

ex:AttendKnowledgeEvent a go:Event ;
	go:HasAgent ex:Allah ;
	go:HasTheme ex:ThatKnowledge .

ex:AbsenceOfAllahFromKnowledge a go:Event ;
	rdf:type go:absenceEvent ;
	go:HasTheme ex:ThatKnowledge ;
	go:HasAgent ex:Allah .

ex:IgnorantCondition a go:property ;
	go:isPropertyOf ex:Allah ;
	rdf:value false .

ex:WeakCondition a go:property ;
	go:isPropertyOf ex:Allah ;
	rdf:value false .

# Conditional representation: If ex:AttendKnowledgeEvent does not occur and ex:AbsenceOfAllahFromKnowledge occurs, then ex:Allah has properties ex:IgnorantCondition and ex:WeakCondition true.

ex:ConditionalStatement a go:conditionalEvent ;
	go:hasIfCondition [ a go:Event ;
		go:Negation true ;
		go:refersTo ex:AttendKnowledgeEvent ;
	], [ a go:Event ;
		go:refersTo ex:AbsenceOfAllahFromKnowledge
	] ;
	go:hasThenEffect [
		a go:stateChange ;
		go:ChangesProperty ex:IgnorantCondition ;
		rdf:value true
	], [
		a go:stateChange ;
		go:ChangesProperty ex:WeakCondition ;
		rdf:value true
	] .
