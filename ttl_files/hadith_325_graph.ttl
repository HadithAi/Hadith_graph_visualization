@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/325-17> rdf:type owl:Ontology .

# Entities
ex:These_Liars a go:Agent .
ex:God a go:Agent .
ex:We a go:Agent .
ex:These_Names a go:Substance .
ex:Good_Names a go:Substance .

# Event: confirmation
ex:Confirmation_Event a go:CommunicationEvent ;
    go:HasAgent ex:These_Liars ;
    go:denotes ex:Claim_Proposition .

# Proposition/content: claim about naming
ex:Claim_Proposition a go:Proposition ;
    go:HasTheme ex:Naming_Event ;
    go:assertsSimilarityBetween ex:We , ex:God ;
    go:hasCondition ex:All_Conditions .

# Naming Event
ex:Naming_Event a go:NamingEvent ;
    go:HasTheme ex:These_Names .

# Condition nodes
ex:All_Conditions a go:ConditionNode .
ex:Some_Conditions a go:ConditionNode .

# Further detail about the likeness claim (like.We is similar to God in all or some conditions)
ex:Claim_Proposition go:hasCondition ex:All_Conditions , ex:Some_Conditions .

# Cause: gathering event
ex:Gathering_Event a go:GatheringEvent ;
    go:HasAgent ex:We ;
    go:HasTheme ex:Good_Names .

# Link cause to claim
ex:Claim_Proposition go:hasCause ex:Gathering_Event .
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.
