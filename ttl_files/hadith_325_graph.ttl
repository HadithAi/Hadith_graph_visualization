@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/325-16> rdf:type owl:Ontology .

ex:Entity_Liars rdf:type go:Group ;
    go:label "هؤلاء الكذّابون"@ar ;
    go:label "These liars"@en .

ex:Entity_God rdf:type go:Substance ;
    go:label "الله"@ar ;
    go:label "God"@en .

# The Speech Event where the liars say the proposition
ex:SpeechEvent1 rdf:type go:Event, ex:SpeechEvent ;
    go:HasAgent ex:Entity_Liars ;
    go:HasTheme ex:Proposition1 .

# Proposition: It is claimed that there is no likeness or similitude to God
ex:Proposition1 rdf:type go:Proposition ;
    go:concerns ex:Entity_God ;
    go:hasAttribute ex:Attribute_NoLikeness, ex:Attribute_NoSimilitude .

ex:Attribute_NoLikeness rdf:type go:Attribute ;
    go:label "no likeness to God"@en ;
    go:deniesSimilarityTo ex:Entity_God .

ex:Attribute_NoSimilitude rdf:type go:Attribute ;
    go:label "no similitude to God"@en ;
    go:deniesSimilarityTo ex:Entity_God .

# The Inquiry Event where the liars question the addressee about naming
ex:InquiryEvent1 rdf:type go:Event, ex:InquiryEvent ;
    go:HasAgent ex:Entity_Liars ;
    go:HasTheme ex:Naming ;
    go:HasRecipient ex:Addressee .

ex:Naming rdf:type go:Action ;
    go:label "Naming with all these names"@en .

ex:Addressee rdf:type go:Agent ;
    go:label "Addressee" .

go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.
