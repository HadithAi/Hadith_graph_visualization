    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/253> rdf:type go:ontology ;
        go:hascontent "4. امام صادق عليه‌السلام فرمود: «اى محمد، مردم هميشه در هر موضوعى بحث مى‌كنند تا جايى كه درباره ذات خدا هم سخن مى‌گويند. هرگاه شما چنين سخنانى را شنيديد، بگوييد: معبودى نيست جز خداى يگانه و بى‌همتا»."@fa,
        "4.  قَالَ أَبُو عَبْدِ اللّه‌ِ عليه‌السلام: «يَا مُحَمَّدُ، إِنَّ النَّاسَ لاَ يَزَالُ بِهِمُ الْمَنْطِقُ حَتّى يَتَكَلَّمُوا فِي اللّه‌ِ، فَإِذَا سَمِعْتُمْ ذلِكَ، فَقُولُوا: لاَ إِلهَ إِلاَّ اللّه‌ُ الْوَاحِدُ الَّذِي لَيْسَ كَمِثْلِهِ شَيْءٌ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/253-1> rdf:type owl:ontology .

ex:people a go:population ;
    go:hasreason ex:reason .

ex:reason a go:concept ;
    rdfs:label "reason" .

ex:god a go:deity ;
    rdfs:label "god" .

ex:speakingaboutgod a go:eventinstance ;
    go:hasagent ex:people ;
    go:hastheme ex:god ;
    rdfs:label "speaking about god" .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/253-2> rdf:type owl:ontology .

ex:hearingevent a go:perceptionevent ;
    go:hasagent ex:you ;
    go:hastheme ex:speakingevent .

ex:speakingevent a go:communicationevent ;
    go:hasagent ex:people ;
    go:hastheme ex:god .

ex:sayingevent a go:communicationevent ;
    go:hasagent ex:you ;
    go:hastheme ex:statement .

ex:people a go:group .
ex:you a go:group .
ex:god a go:deity .

ex:statement a go:message ;
    rdf:value "there is no deity but allah, the one, who is unlike anything else." .