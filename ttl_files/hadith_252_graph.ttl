    @base <http://etratnet.ir/ontology/> .
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/252-1> rdf:type owl:Ontology .

ex:GodAlmighty a go:Agent, go:Person ;
    rdfs:label "Allah"@ar .

ex:SayingEvent1 a go:Communication ;
    go:HasAgent ex:GodAlmighty ;
    go:HasTheme ex:Utterance1 .

ex:Utterance1 a go:InformationContent ;
    go:hasText "And that to your Lord is the final return."@en .
go:Information rdfs:label "اطلاعات"@fa.
go:Agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/252-2> rdf:type owl:Ontology .

ex:ConversationEndingEvent a go:Event ;
    go:HasTheme ex:Conversation ;
    go:HasAgent ex:God .

ex:RefrainFromSpeakingEvent a go:Event ;
    go:HasTheme ex:Speech ;
    go:HasAgent ex:Person ;
    go:isConsequenceOf ex:ConversationEndingEvent .

ex:Conversation a go:Action .
ex:God a go:Substance .
ex:Speech a go:Action .
ex:Person a go:Agent .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.