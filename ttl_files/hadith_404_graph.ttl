ex:Jonas a go:Substance ;
    rdfs:label "يونس" .

ex:Something a go:Substance ;
    rdfs:label "شيئا" .

ex:SayEvent a go:Event , ex:SpeechEvent ;
    go:HasAgent ex:Jonas ;
    go:HasTheme ex:OpeningEvent .

ex:OpeningEvent a go:Event ;
    go:HasAgent ex:Something ;
    go:HasRecipient ex:Jonas ;
    ex:PreviousState ex:HeedlessState .

ex:HeedlessState a go:state ;
    go:hasTheme ex:Jonas ;
    go:description "He was heedless about the thing before it opened to him" .

ex:ContentOfSpeech a go:content ;
    go:referent ex:OpeningEvent .

ex:SayEvent go:HasTheme ex:ContentOfSpeech .
