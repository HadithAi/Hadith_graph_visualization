ex:event12 a go:SpeechEvent ;
    go:HasAgent _:speaker ;
    go:HasRecipient _:listener ;
    go:HasTheme ex:utterance12 .

_:speaker a go:Agent .
_:listener a go:Agent .

ex:utterance12 a go:Utterance ;
    ex:statesEquivalence (
        ex:Judgment
        ex:Execution
        ex:EstablishmentOfObligation
    ) .

ex:Judgment a go:Substance ;
    rdfs:label "Judgment"@en .
ex:Execution a go:Substance ;
    rdfs:label "Execution"@en .
ex:EstablishmentOfObligation a go:Substance ;
    rdfs:label "Establishment of Obligation"@en .
