@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/429-20> rdf:type owl:Ontology .

ex:God a go:Substance ;
    rdfs:label "God"@en ;
    go:hasQuality ex:Forgiving , ex:Merciful .

ex:Forgiving a go:Quality ;
    rdfs:label "Forgiving"@en .

ex:Merciful a go:Quality ;
    rdfs:label "Merciful"@en .
go:Substance rdfs:label "شی"@fa.
go:Quality rdfs:label "ویژگی"@fa.
@prefix : <http://etratnet.ir/graph/> .
@prefix ex: <http://etratnet.ir/graph/proposition/429#> .
@prefix bt: <http://borhan-onto.ir/ontology/time#> .
@prefix go: <http://etratnet.ir/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/429-21> rdf:type owl:Ontology .

ex:Event10 a go:Event, ex:CarryingEvent ;
    go:HasAgent ex:ThoseWhoCome ;
    go:HasTheme ex:Them .

ex:ThoseWhoCome a go:Person .

ex:Them a go:Substance .

ex:NoBlameOnThose a go:Negation ;
    go:NegatesProperty go:blame ;
    go:about ex:ThoseWhoCome .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
@prefix : <http://etratnet.ir/graph/> .
@prefix ex: <http://etratnet.ir/graph/proposition/429#> .
@prefix bt: <http://borhan-onto.ir/ontology/time#> .
@prefix go: <http://etratnet.ir/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/429-22> rdf:type owl:Ontology .

ex:forest a go:naturalEntity ;
    go:hasTopic ex:forestTopic .

ex:forestTopic a go:topic ;
    go:about ex:peopleWhoCome .

ex:peopleWhoCome a go:group ;
    go:hasMember ex:person .

ex:person a go:person .

ex:comingEvent a go:Event ;
    go:hasAgent ex:person ;
    go:hasTheme ex:forest .

ex:carryingEvent a go:Event ;
    go:hasAgent ex:others ;
    go:hasTheme ex:person .

ex:inabilityToFind a go:condition ;
    go:affects ex:person .

ex:causeRelation a go:cause ;
    go:hasCause ex:inabilityToFind ;
    go:hasEffect ex:carryingEvent .
go:Event rdfs:label "رخداد"@fa.