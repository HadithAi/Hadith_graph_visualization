    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/433> rdf:type go:ontology ;
        go:hascontent "4.  به امام صادق عليه‌السلام عرض كردم: مردم را به اين امر (ولايت و تشيّع) دعوت كنيم؟ فرمود: «نه، اى فضيل، خداوند هرگاه براى بنده‌اى خيرى بخواهد به فرشته‌اى فرمان دهد كه گردنش را بگيرد و او را به اين امر درآورد، بخواهد يا نخواهد»."@fa,
        "4. قُلْتُ لِأَبِي عَبْدِ اللّه‌ِ عليه‌السلام: نَدْعُو النَّاسَ إِلى هذَا الْأَمْرِ؟ فَقَالَ: «لاَ، يَا فُضَيْلُ، إِنَّ اللّه‌َ إِذَا أَرَادَ بِعَبْدٍ خَيْراً، أَمَرَ مَلَكاً فَأَخَذَ بِعُنُقِهِ، فَأَدْخَلَهُ فِي هذَا الْأَمْرِ طَائِعاً أَوْ كَارِهاً»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/433-1> rdf:type owl:ontology .

ex:god a go:deity ;
    go:name "god" .

ex:servant a go:person ;
    go:name "servant" .

ex:angel a go:spirit ;
    go:name "angel" .

ex:desiregoodfor a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:servant ;
    go:description "god desires good for a servant" .

ex:commandangel a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:angel ;
    go:causes ex:angelseizingservant ;
    go:description "god commands an angel" .

ex:angelseizingservant a go:event ;
    go:hasagent ex:angel ;
    go:hastheme ex:servant ;
    go:description "angel seizes servant by the neck" .

ex:seizurelocation a go:location ;
    go:name "neck" .

ex:angelseizingservant go:haslocation ex:seizurelocation .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/433-2> rdf:type owl:ontology .

ex:king a go:person ;
    go:label "king" .

ex:slave a go:person ;
    go:label "slave" .

ex:takingsweatevent a go:event ;
    go:hasagent ex:king ;
    go:hastheme ex:slavesweat .

ex:slavesweat a go:substance ;
    go:label "slave's sweat" .

ex:forcingevent a go:event ;
    go:hasagent ex:king ;
    go:hastheme ex:slave ;
    go:hasrecipient ex:matter ;
    go:hasmodality "willingly or unwillingly" .

ex:matter a go:object ;
    go:label "this matter" .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/433-3> rdf:type owl:ontology .

ex:invitationevent1 a go:event ;
    go:hastheme ex:thismatter ;
    go:hasrecipient ex:people ;
    go:eventpolarity "negative"^^<http://www.w3.org/2001/xmlschema#string> .

ex:people a go:agent .
ex:thismatter a go:substance .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.