    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/426> rdf:type go:ontology ;
        go:hascontent "1.  «خدا را بر آفريدگانش اين حق نيست كه بشناسند. بلكه آفريدگان را بر خدا اين حق است كه به آنان بشناساند، و چون به آفريدگان شناساند آنان وظيفه دارند كه بپذيرند»."@fa,
        "1.  عَنْ أَبِي عَبْدِ اللّه‌ِ عليه‌السلام، قَالَ: «لَيْسَ لِلّهِ عَلى خَلْقِهِ أَنْ يَعْرِفُوا، وَلِلْخَلْقِ عَلَى اللّه‌ِ أَنْ يُعَرِّفَهُمْ، وَلِلّهِ عَلَى الْخَلْقِ إِذَا عَرَّفَهُمْ أَنْ يَقْبَلُوا»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/426-1> rdf:type owl:ontology .

ex:god a go:deity ;
   rdfs:label "god" .

ex:creation a go:substance ;
   rdfs:label "creation" .

ex:knowevent a go:event ;
   rdfs:label "knowing event" ;
   go:hasagent ex:creation ;
   go:hastheme ex:god .

ex:negativeassertion a ex:negation ;
   ex:negates ex:knowevent .

# asserts that it is negated that creation knows god, corresponding to the sentence meaning.
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/426-2> rdf:type owl:ontology .

ex:definingevent a go:eventinstance ;
    go:hasagent ex:god ;
    go:hastheme ex:them ;
    go:hasrecipient ex:creation .

ex:god a go:substance ;
    rdfs:label "god"@en .

ex:them a go:substance ;
    rdfs:label "an unspecified group"@en .

ex:creation a go:substance ;
    rdfs:label "creation"@en .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/426-3> rdf:type owl:ontology .

ex:god a go:substance .
ex:creation a go:substance .
ex:commandacceptance a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:creation ;
    ex:predicate "accept" ;
    ex:condition ex:godknowscreation .

ex:godknowscreation a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:creation ;
    ex:predicate "know" .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.