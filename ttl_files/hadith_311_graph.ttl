@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/311-2> rdf:type owl:ontology .

ex:thing a go:substance ;
    rdfs:label "thing" .

# define the class of all created beings named "thing"
ex:createdbeing a go:substance .

ex:god a go:substance ;
    a ex:divinebeing .

# universal statement: every entity named 'thing' is a createdbeing except god
_:x a go:substance ;
    rdfs:label "thing" .

# implicit rule (represented here as a triple):
# if an entity has the name "thing" and is not god, then it is a createdbeing
_:rule a go:rule ;
    go:condition [
        rdfs:label "thing" ;
        go:notequalto ex:god
    ] ;
    go:assertion [ a ex:createdbeing ] .

# god is explicitly not included as createdbeing
ex:god a ex:divinebeing .
go:substance rdfs:label "شی"@fa.


