ex:Person1 a go:Substance .
ex:God a go:Substance .

ex:KnowledgeEvent1 a go:Event ;
    go:HasAgent ex:Person1 ;
    go:HasTheme ex:God .

ex:KnowledgeEvent2 a go:Event ;
    go:HasAgent ex:Person1 ;
    go:HasTheme ex:God ;
    ex:HasCause ex:God .
