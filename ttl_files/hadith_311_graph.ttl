<http://etratnet.ir/ontology/311-23> rdf:type owl:Ontology .

ex:God a go:Deity .
ex:UnspecifiedPerson a go:Agent .

ex:KnowGodThroughGod a go:Event ;
    go:HasAgent ex:UnspecifiedPerson ;
    go:HasTheme ex:God ;
    go:hasManner ex:ThroughGod .

ex:NotKnowGodThroughGod a go:Event ;
    go:HasAgent ex:UnspecifiedPerson ;
    go:HasTheme ex:God ;
    go:negation true ;
    go:hasManner ex:ThroughGod .

ex:NotTrulyKnowGod a go:Event ;
    go:HasAgent ex:UnspecifiedPerson ;
    go:HasTheme ex:God ;
    go:negation true ;
    go:hasCondition ex:NotKnowGodThroughGod .
go:Event rdfs:label "رخداد"@fa.
go:Agent rdfs:label "عامل"@fa.
