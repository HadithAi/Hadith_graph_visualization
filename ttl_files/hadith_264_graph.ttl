@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/264-5> rdf:type owl:Ontology .

# Entities
ex:Perceiver a go:Substance ;
  rdfs:label "Perceiver"@en .

ex:Perceived a go:Substance ;
  rdfs:label "Perceived"@en .

ex:PositiveCause a go:Cause ;
  rdfs:label "Positive Cause"@en .

# Events
ex:VisionEvent a go:Event ;
  rdfs:label "Vision Event"@en ;
  go:HasAgent ex:Perceiver ;
  go:HasTheme ex:Perceived ;
  go:HasCause ex:PositiveCause .

ex:EqualityCondition a go:Event ;
  rdfs:label "Equality of positive cause between perceiver and perceived"@en ;
  go:HasAgent ex:Perceiver ;
  go:HasTheme ex:Perceived ;
  go:HasCause ex:PositiveCause .

ex:ConfusionEvent a go:Event ;
  rdfs:label "Confusion"@en ;
  go:resultsIn ex:EqualityCondition .

# Conditional statement
ex:EqualityCondition go:resultsIn ex:ConfusionEvent .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
