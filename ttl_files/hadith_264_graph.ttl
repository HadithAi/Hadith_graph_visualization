<http://etratnet.ir/ontology/264-6> rdf:type owl:Ontology .

ex:suspicion6 a go:MentalState , go:Substance ;
    go:label "suspicion 6" .

ex:reasons a go:Substance ;
    go:label "reasons" .

ex:causes a go:Substance ;
    go:label "causes" .

ex:ResemblanceEvent6 a go:Event ;
    go:HasTheme ex:suspicion6 ;
    go:description "This suspicion resembles something" .

ex:ConnectionEvent6 a go:Event ;
    go:HasAgent ex:reasons ;
    go:HasTheme ex:causes ;
    go:description "Reasons must be connected to causes" .
