<http://etratnet.ir/graph/ontology/264> rdf:type go:ontology ;
        go:hascontent "4. به ابو الحسن ثالث [امام هادى] عليه‌السلام نوشتم و از آن حضرت درباره ديدن [خدا] و اختلاف نظر مردم در اين خصوص پرسيدم. حضرت عليه‌السلام مرقوم فرمود: «تا زمانى كه ميان بيننده و آنچه ديده مى‌شود هوايى نباشد كه نگاه از آن بگذرد ديدن تحقق نمى‌يابد؛ زيرا، هرگاه اين هوا بين بيننده و شى‌ء قابل رؤيت برداشته شود ديدن صورت نگيرد، و اين [وجود هوا ميان بيننده و شى‌ء قابل رويت] متضمن همانندى [آن دو] مى‌باشد؛ چون، هرگاه بيننده با شى‌ء قابل رؤيت در سببى كه موجب ديدن ميان آن دو مى‌شود يكسان [و همانند] باشد، همانندى لازم مى‌آيد، و اين همان تشبيه است؛ زيرا ميان سبب و مسبّب بايد ارتباط [و سنخيت] باشد»."@fa,
        "4.  كَتَبْتُ إِلى أَبِي الْحَسَنِ الثَّالِثِ عليه‌السلام أَسْأَلُهُ عَنِ الرُّؤيَةِ وَمَا اخْتَلَفَ فِيهِ النَّاسُ. فَكَتَبَ عليه‌السلام: «لاَ تَجُوزُ الرُّؤيَةُ مَا لَمْ يَكُنْ بَيْنَ الرَّائِي وَالْمَرْئِيِّ هَوَاءٌ يَنْفُذُهُ الْبَصَرُ، فَإِذَا انْقَطَعَ الْهَوَاءُ عَنِ الرَّائِي وَالْمَرْئِيِّ، لَمْ تَصِحَّ الرُّؤيَةُ، وَكَانَ فِي ذلِكَ الاِشْتِبَاهُ؛ لِأَنَّ الرَّائِيَ مَتى سَاوَى الْمَرْئِيَّ فِي السَّبَبِ الْمُوجِبِ بَيْنَهُمَا فِي الرُّؤيَةِ، وَجَبَ الاِشْتِبَاهُ، وَكَانَ ذلِكَ التَّشْبِيهَ؛ لِأَنَّ الْأَسْبَابَ لاَ بُدَّ مِنِ اتِّصَالِهَا بِالْمُسَبَّبَاتِ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/264-1> rdf:type owl:ontology .

exevent:writingevent1 a go:event ;
    go:hasagent ex:writer1 ;
    go:hasrecipient ex:abualhasanalthalith ;
    go:hastheme ex:questionaboutvision .

ex:writer1 a go:person ;
    rdfs:label "الكاتب" .

ex:abualhasanalthalith a go:person ;
    rdfs:label "أبي الحسن الثالث عليه‌السلام" .

ex:questionaboutvision a go:information ;
    go:abouttopic ex:vision ;
    go:abouttopic ex:differencesamongpeople .

ex:vision a go:concept ;
    rdfs:label "الرؤية" .

ex:differencesamongpeople a go:concept ;
    rdfs:label "اختلاف الناس فيها" .
go:event rdfs:label "رخداد"@fa.
go:information rdfs:label "اطلاعات"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/264-2> rdf:type owl:ontology .

ex:abu_al_hasan_al_thalith_iii_peace_be_upon_him a go:person ;
    rdfs:label "abu al-hasan al-thaaliq al-thalith (peace be upon him)" .

ex:writingevent1 a go:event ;
    go:hasagent ex:abu_al_hasan_al_thalith_iii_peace_be_upon_him ;
    go:hastheme ex:statement_on_vision .

ex:statement_on_vision a go:concept ;
    go:hascontent "vision is not valid unless there is air between the viewer and the object viewed that permits sight" .

ex:vision a go:concept .

ex:air a go:substance ;
    go:hasproperty "permits sight between viewer and viewed object" .

ex:viewer a go:person .

ex:objectviewed a go:object .

ex:visioncondition a go:condition ;
    go:hastheme ex:vision ;
    go:requires [ a go:relation ;
        go:hasagent ex:air ;
        go:hastheme [ a go:relation ;
            go:hasagent ex:viewer ;
            go:hastheme ex:objectviewed 
        ]
    ] .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/264-3> rdf:type owl:ontology .

ex:visionevent a go:perceptionevent ;
    go:hasagent ex:observer ;
    go:hastheme ex:observed ;
    go:hascondition ex:airpresence .

ex:airpresence a go:condition ;
    go:dependson ex:airbetween .

ex:airbetween a go:substance ;
    go:between ex:observer , ex:observed .

ex:aircutoffevent a go:event ;
    go:hastheme ex:airbetween .

ex:failureofvision a go:state ;
    go:hascause ex:aircutoffevent ;
    skos:definition "vision is not correct if the air is cut off between observer and observed." .

ex:observer a go:agent ;
    skos:preflabel "observer"@en .

ex:observed a go:theme ;
    skos:preflabel "observed"@en .

ex:airbetween a go:substance ;
    skos:preflabel "air between observer and observed"@en .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/264-4> rdf:type owl:ontology .

ex:interruptionofair a go:process, go:event ;
    go:hastheme ex:air ;
    go:hasagent ex:seer ;
    go:hasrecipient ex:seen ;
    go:causes ex:confusioninseeing .

ex:air a go:substance .
ex:seer a go:agent .
ex:seen a go:agent .

ex:confusioninseeing a go:event ;
    go:hastheme ex:things .

ex:things a go:object .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/264-5> rdf:type owl:ontology .

# entities
ex:perceiver a go:substance ;
  rdfs:label "perceiver"@en .

ex:perceived a go:substance ;
  rdfs:label "perceived"@en .

ex:positivecause a go:cause ;
  rdfs:label "positive cause"@en .

# events
ex:visionevent a go:event ;
  rdfs:label "vision event"@en ;
  go:hasagent ex:perceiver ;
  go:hastheme ex:perceived ;
  go:hascause ex:positivecause .

ex:equalitycondition a go:event ;
  rdfs:label "equality of positive cause between perceiver and perceived"@en ;
  go:hasagent ex:perceiver ;
  go:hastheme ex:perceived ;
  go:hascause ex:positivecause .

ex:confusionevent a go:event ;
  rdfs:label "confusion"@en ;
  go:resultsin ex:equalitycondition .

# conditional statement
ex:equalitycondition go:resultsin ex:confusionevent .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/264-6> rdf:type owl:ontology .

ex:suspicion6 a go:mentalstate , go:substance ;
    go:label "suspicion 6" .

ex:reasons a go:substance ;
    go:label "reasons" .

ex:causes a go:substance ;
    go:label "causes" .

ex:resemblanceevent6 a go:event ;
    go:hastheme ex:suspicion6 ;
    go:description "this suspicion resembles something" .

ex:connectionevent6 a go:event ;
    go:hasagent ex:reasons ;
    go:hastheme ex:causes ;
    go:description "reasons must be connected to causes" .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.