<http://etratnet.ir/ontology/264-4> rdf:type owl:Ontology .

ex:InterruptionOfAir a go:process, go:Event ;
    go:HasTheme ex:Air ;
    go:HasAgent ex:Seer ;
    go:HasRecipient ex:Seen ;
    go:causes ex:ConfusionInSeeing .

ex:Air a go:Substance .
ex:Seer a go:agent .
ex:Seen a go:agent .

ex:ConfusionInSeeing a go:Event ;
    go:HasTheme ex:Things .

ex:Things a go:object .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
