    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/389> rdf:type go:ontology ;
        go:hascontent "3. از امام صادق عليه‌السلام شنيدم كه مى‌فرمايد: «خداوند فرمان داده است و نخواسته است، و خواسته است و فرمان نداده است. به ابليس فرمان داد كه براى آدم سجده كند ولى خواست او اين بود كه سجده نكند. اگر مى‌خواست قطعا او سجده مى‌كرد؛ و آدم را از خوردن از درخت نهى كرد اما خواست او اين بود كه از آن بخورد، و اگر نخواسته بود او نمى‌خورد»."@fa,
        "3.  عَنْ أَبِي عَبْدِ اللّه‌ِ عليه‌السلام، قَالَ: سَمِعْتُهُ يَقُولُ: «أَمَرَ اللّه‌ُ وَلَمْ يَشَأْ، وَشَاءَ وَلَمْ يَأْمُرْ؛ أَمَرَ إِبْلِيسَ أَنْ يَسْجُدَ لاِدَمَ، وَشَاءَ أَنْ لاَ يَسْجُدَ، وَلَوْ شَاءَ لَسَجَدَ، وَنَهى آدَمَ عَنْ أَكْلِ الشَّجَرَةِ، وَشَاءَ أَنْ يَأْكُلَ مِنْهَا، وَلَوْ لَمْ يَشَأْ لَمْ يَأْكُلْ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/389-1> rdf:type owl:ontology .

# entities
go:god a go:agent ;
    rdfs:label "god"@en .
go:iblis a go:agent ;
    rdfs:label "iblis"@en .
go:adam a go:agent ;
    rdfs:label "adam"@en .

# events
ex:orderevent1 a go:orderevent ;
    go:hasagent go:god ;
    go:hasrecipient go:iblis ;
    go:hassubevent ex:prostrateevent1 .

ex:prostrateevent1 a go:prostrateevent ;
    go:hasagent go:iblis ;
    go:hastheme go:adam .
go:agent rdfs:label "عامل"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/389-2> rdf:type owl:ontology .

ex:event_willinggod a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:event_prostration_iblis .

ex:event_prostration_iblis a go:event ;
    go:hasagent ex:iblis ;
    go:hasrecipient ex:adam ;
    go:hasnegation true .

ex:god a go:deity .
ex:iblis a go:person .
ex:adam a go:person .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/389-3> rdf:type owl:ontology .

ex:god a go:deity ;
   go:hasagentrole ex:god .

ex:iblis a go:person ;
   go:hasagentrole ex:iblis .

ex:adam a go:person .

ex:prostrationevent a go:event ;
   go:hasagent ex:iblis ;
   go:hastheme ex:prostration ;
   go:hasrecipient ex:adam .

ex:godwillsprostration a go:event ;
   go:hasagent ex:god ;
   go:haseffect ex:prostrationevent .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/389-4> rdf:type owl:ontology .

ex:event1 a go:forbiddingevent ;
    go:hasagent ex:god ;
    go:hasrecipient ex:adam ;
    go:hastheme ex:event2 .

ex:event2 a go:eatingevent ;
    go:hasagent ex:adam ;
    go:hastheme ex:tree .

ex:god a go:deity .
ex:adam a go:human .
ex:tree a go:substance .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/389-5> rdf:type owl:ontology .

ex:event1 a go:event ;
    go:hasagent ex:adam ;
    go:hastheme ex:tree ;
    go:hascause ex:god .

ex:god a go:substance .
ex:adam a go:substance .
ex:tree a go:substance .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/389-6> rdf:type owl:ontology .

ex:event1 a go:event ;
    go:hasagent ex:adam ;
    go:hastheme ex:tree .

ex:adam a go:agent ;
    go:substancename "adam" .

ex:god a go:agent ;
    go:substancename "god" .

ex:tree a go:substance ;
    go:substancename "tree" .

ex:godwill a go:event ;
    go:hasagent ex:god ;
    go:causes ex:event1 .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.
go:agent rdfs:label "عامل"@fa.