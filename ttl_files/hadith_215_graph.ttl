@prefix bt: <http://borhan-onto.ir/ontology/time#> .
@prefix go: <http://etratnet.ir/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/215-77> rdf:type owl:Ontology .

ex:Abu_Abdullah a go:Person ;
    rdfs:label "Abu Abdullah" ;
    go:hasHonorific "peace be upon him" .

ex:Believer a go:Person ;
    go:hasRole "Believer" .

ex:BelievingEvent a go:Event ;
    go:HasAgent ex:Believer ;
    go:HasRecipient ex:Abu_Abdullah ;
    go:hasMethod "Through the hand" .

ex:PurityOfBeliever a go:State ;
    go:inheresIn ex:Believer ;
    go:hasQuality "good" .

ex:PleasureEvent a go:Event ;
    go:HasAgent ex:Abu_Abdullah ;
    go:HasTheme ex:PurityOfBeliever ;
    go:temporalRelation "until" ;
    go:TimeRelation ex:PurityOfBeliever .
go:Event rdfs:label "رخداد"@fa.
go:Time rdfs:label "زمان"@fa.