@prefix ex: <http://etratnet.ir/graph/proposition/399#> .
    @prefix bt: <http://borhan-onto.ir/ontology/time#> .
    @prefix go: <http://etratnet.ir/ontology#> .
    @prefix owl: <http://www.w3.org/2002/07/owl#> .
    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix xml: <http://www.w3.org/XML/1998/namespace> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
    @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


    @base <http://etratnet.ir/ontology/> .
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/399-1> rdf:type owl:Ontology .

ex:God a go:Substance ;
    ex:isOnlyDeity true ;
    ex:hasExclusiveDeity ex:God .
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/399-2> rdf:type owl:Ontology .

ex:God a go:Person .
ex:Good a go:Substance .
ex:CreationEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Good .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/399-3> rdf:type owl:Ontology .

ex:CreationEvent1 a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Evil .

ex:God a go:Substance ;
    go:label "God"@en .

ex:Evil a go:Substance ;
    go:label "Evil"@en .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/399-4> rdf:type owl:Ontology .

ex:BlessedPerson a go:Substance ;
    go:hasState ex:BlessedState .

ex:God a go:Substance .

ex:GoodEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:BlessedPerson ;
    go:hasEffect ex:Good .

ex:Good a go:Quality .

ex:BlessedState a go:state ;
    go:causedBy ex:GoodEvent .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.
go:Quality rdfs:label "ویژگی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/399-5> rdf:type owl:Ontology .

ex:Person1 a go:Human ;
    go:hasState ex:WoeState1 .

ex:God1 a go:Deity .

ex:Evil1 a go:NegativeEvent .

ex:CauseEvilEvent1 a go:Event ;
    go:HasAgent ex:God1 ;
    go:HasInstrument ex:Person1 ;
    go:HasTheme ex:Evil1 .

ex:WoeState1 a go:NegativeState ;
    go:appliesTo ex:Person1 .
go:Event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/399-6> rdf:type owl:Ontology .

ex:WoeEvent rdf:type go:State ;
    go:affects ex:PersonWhoSays .

ex:SayingEvent rdf:type go:CommunicationEvent ;
    go:HasAgent ex:PersonWhoSays ;
    go:HasTheme ex:QuestionContent .

ex:PersonWhoSays rdf:type go:Agent .

ex:QuestionContent rdf:type go:InformationContent ;
    go:hasText "How is this so?" .
go:Information rdfs:label "اطلاعات"@fa.
go:Agent rdfs:label "عامل"@fa.