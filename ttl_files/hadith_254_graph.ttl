    go:hasTheme ex:ThesePeople .

# Events
ex:EndSpeechEvent a go:Event ;
    go:HasAgent ex:ThesePeople ;
    go:HasTheme ex:SpeechOfThesePeople ;
    go:HasGoal ex:GodAlmighty .

ex:BeingLedAstray a go:Event ;
    go:HasAgent ex:ThesePeople .

# Causal linkage
ex:EndSpeechEvent go:causes ex:BeingLedAstray .
