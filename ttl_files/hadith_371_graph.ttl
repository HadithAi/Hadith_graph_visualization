    @base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/graph/ontology/371> rdf:type go:ontology ;
        go:hascontent "4.  «خداوند هيچ پيامبرى را برنينگيخت تا آن كه در [اعتقاد به] سه كار از او پيمان گرفت: اقرار كردن به بندگى خود نسبت به خدا، شريك نياوردن براى او، و اين كه خدا آنچه را خواهد پس و پيش كند»."@fa,
        "4.  عَنْ أَبِي عَبْدِ اللّه‌ِ عليه‌السلام، قَالَ: «مَا بَعَثَ اللّه‌ُ نَبِيّاً حَتّى يَأْخُذَ عَلَيْهِ ثَلاَثَ خِصَالٍ: الاْءِقْرَارَ لَهُ بِالْعُبُودِيَّةِ، وَخَلْعَ الْأَنْدَادِ، وَأَنَّ اللّه‌َ يُقَدِّمُ مَا يَشَاءُ، وَيُؤخِّرُ مَا يَشَاءُ»."@ar.
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/371-1> rdf:type owl:ontology .

ex:event1 a go:sending ;
    go:hasagent ex:god ;
    go:hastheme ex:prophet .

ex:event2 a go:taking ;
    go:hasagent ex:god ;
    go:hastheme ex:threecharacteristics ;
    go:relationto ex:prophet .

ex:god a go:substance .
ex:prophet a go:substance .
ex:threecharacteristics a go:quality ;
    go:hasquantity "3"^^<http://www.w3.org/2001/xmlschema#integer> ;
    go:relatedto ex:prophet .
go:substance rdfs:label "شی"@fa.
go:quality rdfs:label "ویژگی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/371-2> rdf:type owl:ontology .

ex:thesetraits a go:substance ;
    go:hascharacteristic ex:ackofhisslavery .

ex:ackofhisslavery a go:characteristic ;
    rdfs:label "acknowledgment of his slavery"@en .
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/371-3> rdf:type owl:ontology .

ex:removalofrivals a go:event ;
    go:hastheme ex:rivals ;
    go:isqualityof ex:qualities .

ex:rivals a go:substance ;
    rdfs:label "rivals"@en .

ex:qualities a go:substance ;
    rdfs:label "qualities"@en .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/371-4> rdf:type owl:ontology .

ex:givingevent a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:willedthing .

ex:god a go:deity .

ex:willedthing a go:entity ;
    go:iscausedby ex:godwill .

ex:godwill a go:concept .
go:event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/371-5> rdf:type owl:ontology .

ex:event_delay_001 a go:event ;
    go:hasagent ex:god ;
    go:hastheme ex:desired_thing .

ex:god a go:substance .
ex:desired_thing a go:substance ;
    ex:willedby ex:god .
go:event rdfs:label "رخداد"@fa.
go:substance rdfs:label "شی"@fa.