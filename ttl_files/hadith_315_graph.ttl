@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/315-1> rdf:type owl:Ontology .

ex:God_Almighty a go:Deity ;
    go:HasProperty ex:Light .

ex:Light a go:Substance .

ex:Heavens a go:Location .
ex:Earth a go:Location .

ex:God_Almighty go:IsLightOf ex:Heavens, ex:Earth .
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/315-2> rdf:type owl:Ontology .

ex:AllahAlmighty a go:Substance ;
    go:hasRole ex:Guide ;
    go:guides ex:InhabitantsOfHeavens .

ex:Guide a go:Role .
ex:InhabitantsOfHeavens a go:Group .
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/315-3> rdf:type owl:Ontology .

ex:GodAlmighty a go:Deity ;
    ex:isGuideFor ex:PeopleOfEarth .

ex:PeopleOfEarth a go:Group .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/315-4> rdf:type owl:Ontology .

ex:God_Almighty a go:Deity ;
    go:hasRole ex:Guidance .

ex:Guidance a go:ConceptualRole ;
    go:hasRecipient ex:Those_in_the_Sky .

ex:Those_in_the_Sky a go:Substance .
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/315-5> rdf:type owl:Ontology .

ex:God_Almighty a go:Agent ;
    go:provides ex:Guidance .

ex:Guidance a go:Role ;
    go:hasRecipient ex:Beings_on_Earth .

ex:Beings_on_Earth a go:Substance ;
    go:locatedIn ex:Earth .
go:Substance rdfs:label "شی"@fa.
go:Agent rdfs:label "عامل"@fa.