<http://etratnet.ir/ontology/340-39> rdf:type owl:Ontology .

ex:AngerState1 rdf:type go:MentalState ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Iblis , ex:IblisAllies , ex:IblisFollowers .

ex:God rdf:type go:Agent ;
    rdfs:label "God"@en .

ex:Iblis rdf:type go:Agent ;
    rdfs:label "Iblis"@en .

ex:IblisAllies rdf:type go:Group ;
    go:hasMemberRelationTo ex:Iblis ;
    rdfs:label "Iblis' allies"@en .

ex:IblisFollowers rdf:type go:Group ;
    go:hasMemberRelationTo ex:Iblis ;
    rdfs:label "Iblis' followers"@en .
go:Agent rdfs:label "عامل"@fa.
