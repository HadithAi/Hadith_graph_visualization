<http://etratnet.ir/ontology/340-40> rdf:type owl:Ontology .

ex:God a go:deity ;
    rdfs:label "God"@en .

ex:State1 a go:state ;
    rdfs:label "State1"@en .

ex:State2 a go:state ;
    rdfs:label "State2"@en .

ex:ChangeEvent a go:ChangeEvent ;
    go:HasAgent ex:God ;
    go:HasTheme [ a go:CompositeTheme ;
                   go:includes ex:State1 , ex:State2 ] ;
    go:negation true .
