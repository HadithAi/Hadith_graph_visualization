@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/340-38> rdf:type owl:Ontology .

# Entities
ex:God a go:Substance .
ex:Iblis a go:Substance .

# Event: God curses Iblis
ex:CursingEvent a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Iblis ;
    go:EventStart ex:PastTime .

ex:PastTime a go:TimeInterval ;
    go:TemporalPosition "curse event start time" .

# State: God is angry at Iblis from curse until now
ex:God a go:Substance ;
    go:HasEmotion ex:AngerState .

ex:AngerState a go:EmotionState ;
    go:Target ex:Iblis ;
    go:TimeInterval ex:FromCurseToNow .

ex:FromCurseToNow a go:TimeInterval ;
    go:Start ex:PastTime ;
    go:End ex:PresentTime .

ex:PresentTime a go:TimeInterval ;
    go:TemporalPosition "Now" .
go:Event rdfs:label "رخداد"@fa.
go:Time rdfs:label "زمان"@fa.
go:Substance rdfs:label "شی"@fa.
