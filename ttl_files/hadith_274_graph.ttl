@prefix ex: <http://etratnet.ir/graph/proposition/274#> .
    @prefix bt: <http://borhan-onto.ir/ontology/time#> .
    @prefix go: <http://etratnet.ir/ontology#> .
    @prefix owl: <http://www.w3.org/2002/07/owl#> .
    @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
    @prefix xml: <http://www.w3.org/XML/1998/namespace> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
    @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


    @base <http://etratnet.ir/ontology/> .
    


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/274-1> rdf:type owl:Ontology .

ex:eventInstance1 a go:Event ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Limitation ;
    go:isNegated true .

ex:God a go:Substance .
ex:Limitation a go:property .
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/274-2> rdf:type owl:Ontology .

ex:OurLord a go:God ;
    go:HasQuality ex:GreatBeyondDescription .

ex:GreatBeyondDescription a go:Quality ;
    go:describedAs "great beyond description"@en .
go:Quality rdfs:label "ویژگی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/274-3> rdf:type owl:Ontology .

ex:Bounded a go:Property .

# Entities: Any entity
ex:AnyEntity a go:Substance .

# Description Event
ex:DescriptionEvent a go:Event ;
    go:HasAgent ex:AnyEntity ;
    go:HasTheme ex:Bounded .

# Logical representation (implicit): if not ex:AnyEntity go:Bounded then not described with ex:DescriptionEvent
# Due to RDF limits, we represent the main concepts only.
go:Event rdfs:label "رخداد"@fa.
go:Substance rdfs:label "شی"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/274-4> rdf:type owl:Ontology .

ex:Eyes a go:Organ ;
    rdfs:label "الأبصار"@ar, "Eyes"@en .

ex:God a go:Deity ;
    rdfs:label "الله"@ar, "God"@en .

ex:PerceptionEvent a go:Event ;
    go:HasAgent ex:Eyes ;
    go:HasTheme ex:God ;
    ex:hasNegation true .
go:Event rdfs:label "رخداد"@fa.


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/274-5> rdf:type owl:Ontology .

ex:PerceptionEvent1 rdf:type go:perceptionEvent ;
    go:HasAgent ex:God ;
    go:HasTheme ex:Eyes .

ex:God rdf:type go:Deity .
ex:Eyes rdf:type go:BodyPart .


@base <http://etratnet.ir/ontology/> .

<http://etratnet.ir/ontology/274-6> rdf:type owl:Ontology .

:God a go:Agent ;
    ex:isGentle true ;
    ex:isAllAware true .
go:Agent rdfs:label "عامل"@fa.